    <div class="iframe-work-module">
  <div class="closed-popup-module">
    <div class="close-popup">x</div>
    <iframe src="" class="popup-module-iframe"></iframe>
  </div>
</div>

<script>
  $('.click-top-popup').click(function(){
    $('body').addClass('popup-iframe');
    $('.iframe-work-module').addClass('open-popup-module');
    var current_link = $(this).attr('data-link');
    console.log(current_link)
    $('.popup-module-iframe').attr('src', current_link);
  });

  $('.close-popup').click(function(){
    $('body').removeClass('popup-iframe');
    $('.iframe-work-module').removeClass('open-popup-module');
  });
  
  $('body').click(function(e){
    var popup = $(".click-top-popup");
    if(!popup.is(e.target) && popup.has(e.target).length === 0){
      if ($("body").hasClass("popup-iframe")){
        $("body").removeClass("popup-iframe");
        $('.iframe-work-module').removeClass('open-popup-module');
      }
    }
  });
</script>

