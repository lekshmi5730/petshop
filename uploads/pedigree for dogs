      $('.head-code-pincode-coupan,.head-code-pincode-barnds').on('click', function (e) {
        e.preventDefault();
        //sessionStorage.setItem('notification','true');
        $('.head-code-pincode-coupan').hide();
        $('.head-code-pincode-barnds').hide();
        $('.head-code-pincode-coupan').css({ "opacity": "0", "visibility": "hidden", "bottom": "-200%" })
        $('.head-code-pincode-barnds').css({ "opacity": "0", "visibility": "hidden", "bottom": "-200%" })
