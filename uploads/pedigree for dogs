      <script>
        function openLinkCustomToolBar(linkURL,sectionName) {
          if (linkURL) {
                      window.dataLayer = window.dataLayer || [];
                    dataLayer.push({
                        'event': 'eventTracking',
                        'eventCategory': 'Homepage',
                        'eventAction': sectionName + ' clicked',
                        'eventLabel': sectionName + ' clicked'
                      });
            window.location.href = linkURL;
          }
        }
      </script>
    

