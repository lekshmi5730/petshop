<script>

  async function fetchEdd(skuArr) {
    let pincode = getCpinCookie('cpin');
    let url = `https://promise-engine-371111.el.r.appspot.com/edd?cpin=${pincode}&skuid=${skuArr.join(
      ","
    )}&qty=1`;

    const options = {
      method: 'GET',
      headers: {
        "Content-Type": "application/json",
      }
    };

    try {
      const response = await fetch(url, options);
      if(response.ok) {
        const data = await response.json();
        return data;
      }

    } catch(err) {
      console.log("error", err);
      throw error;
    }

  }
  async function checkItemAlreadyInCartRecommendations(selected_id){
      updatedCartItemsArrSP = await getCartSP();
      const cartItemsSP = updatedCartItemsArrSP;
      const itemAlreadyInCartSP = cartItemsSP?.items?.find(item => item.id == selected_id);
      if(itemAlreadyInCartSP){
          return itemAlreadyInCartSP;
      }return false
  }
  function createSkuMap(data) {
    const skuMap = {};
    data.forEach(item => {
      const { skuId, ...rest } = item;
      skuMap[skuId] = rest;
    });
    return skuMap;
  }
  
  function displayEdd(eddData, skuId) {
    const data = eddData;
    const result = createSkuMap(data);

    const element = document.getElementById(`similar-products-item-edd-container-${skuId}`);
    if (result[skuId] === undefined) {
      element.innerHTML = '';
      element.style.height = "42px";
      element.classList.remove("linear-gradient-yellow");
      return;
    }
    if(result[skuId]?.is2HourDelivery) {
      element.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_36.png?v=1726039250" class="similar-products-item-edd-icon"/>
      <p class="similar-products-item-edd-text"><b style="font-weight: 900;">Superfast</b> <b>${result[skuId]?.message}</b></p>`;
      element.classList.add("linear-gradient-yellow");
    } else if(result[skuId]?.dayCount == 0 || result[skuId]?.dayCount == 1) {
      element.innerHTML = `<img src="https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_34.png?v=1726038501" class="similar-products-item-edd-icon"/>
      <p class="similar-products-item-edd-text"><b style="font-weight: 900;">Fast</b> <b>${result[skuId]?.message}</b></p>`;
      element.classList.add("linear-gradient-yellow");
    } else {
      element.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_35.png?v=1726039019" class="similar-products-item-edd-icon"/>
      <p class="similar-products-item-edd-text"><b>${result[skuId]?.message}</b></p>`;
      element.classList.remove("linear-gradient-yellow");
    }
  }
  function displayEddInDrawerMWeb(skuId){
    const eddVisibleMWeb = document.querySelector('.similar-products-item-modal-edd-mweb');
    if (skuMapGlobal[skuId] === undefined) {
      eddVisibleMWeb.style.display = "none";
      return;
    }

    if(skuMapGlobal[skuId]?.is2HourDelivery) {
      eddVisibleMWeb.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_36.png?v=1726039250" class="similar-products-item-edd-icon-quickAdd"/>
      <p class="similar-products-item-edd-text-quickAdd"><b style="font-weight: 900;">Superfast</b> <b>${skuMapGlobal[skuId]?.message}</b></p>`;
      eddVisibleMWeb.classList.add("linear-gradient-yellow");
    } else if(skuMapGlobal[skuId]?.dayCount == 0 || skuMapGlobal[skuId]?.dayCount == 1) {
      eddVisibleMWeb.innerHTML = `<img src="https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_34.png?v=1726038501" class="similar-products-item-edd-icon-quickAdd"/>
      <p class="similar-products-item-edd-text-quickAdd"><b style="font-weight: 900;">Fast</b> <b>${skuMapGlobal[skuId]?.message}</b></p>`;
      eddVisibleMWeb.classList.add("linear-gradient-yellow");
    } else {
      eddVisibleMWeb.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_35.png?v=1726039019" class="similar-products-item-edd-icon-quickAdd"/>
      <p class="similar-products-item-edd-text-quickAdd"><b>${skuMapGlobal[skuId]?.message}</b></p>`;
      eddVisibleMWeb.classList.remove("linear-gradient-yellow");
    }
  }

  function displayEddInDrawer(skuId){
    const eddVisible = document.getElementById('similar-products-item-modal-edd');
    if (skuMapGlobal[skuId] === undefined) {
      eddVisible.innerHTML = '';
      eddVisible.style.margin = "unset";
      return;
    }
    if(skuMapGlobal[skuId]?.is2HourDelivery) {
      eddVisible.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_36.png?v=1726039250" class="similar-products-item-edd-icon-quickAdd"/>
      <p class="similar-products-item-edd-text-quickAdd"><b style="font-weight: 900;">Superfast</b> <b>${skuMapGlobal[skuId]?.message}</b></p>`;
      eddVisible.classList.add("linear-gradient-yellow");
    } else if(skuMapGlobal[skuId]?.dayCount == 0 || skuMapGlobal[skuId]?.dayCount == 1) {
      eddVisible.innerHTML = `<img src="https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_34.png?v=1726038501" class="similar-products-item-edd-icon-quickAdd"/>
      <p class="similar-products-item-edd-text-quickAdd"><b style="font-weight: 900;">Fast</b> <b>${skuMapGlobal[skuId]?.message}</b></p>`;
      eddVisible.classList.add("linear-gradient-yellow");
    } else {
      eddVisible.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_35.png?v=1726039019" class="similar-products-item-edd-icon-quickAdd"/>
      <p class="similar-products-item-edd-text-quickAdd"><b>${skuMapGlobal[skuId]?.message}</b></p>`;
      eddVisible.classList.remove("linear-gradient-yellow");
    }
  }

  async function displayVariantDrawer(variantsArr, skuArray, eddData, handle, productDetails) {
    const result_sku = createSkuMap(eddData);
    const sortedVariantsArr = variantsArr.sort((a, b) => {
      return (a.isDefault === b.isDefault) ? 0 : a.isDefault ? -1 : 1;
    });

    function handleIncreaseQuantity(event) {
      changeCartQuantityDesktop(selectedVariant?.variantId, true, desktopAddToCartStepper);
    }
    function handleDecreaseQuantity(event) {
      changeCartQuantityDesktop(selectedVariant?.variantId, false, desktopAddToCartStepper, desktopAddToCart);
    }
    
    let selectedVariant = sortedVariantsArr.find(variant => variant.isDefault);

    const addOnItemModal = document.getElementById("similar-products-item-modal");
    const addOnItemModalContent = document.getElementById("similar-products-item-modal-content");
    const desktopModalBackdrop = document.getElementById("similar-products-item-modal-backdrop");


    // Create modal elements (simplified for brevity)
    const createModalElement = (tag, className, innerHTML = '', src = '', id = '') => {
      const element = document.createElement(tag);
      if (className) element.classList.add(className);
      if (innerHTML) element.innerHTML = innerHTML;
      if (src) element.setAttribute('href', src);
      if (src) element.src = src;
      if (id) element.setAttribute('id', id);
      return element;
    };

    // Modal Header
    const addOnItemModalHeader = createModalElement('div', 'similar-products-item-modal-header');
    addOnItemModalHeader.appendChild(createModalElement('div', 'similar-products-item-modal-header-text', 'Quick Add'));
    const addOnItemModalHeaderClose = createModalElement('img', 'similar-products-item-modal-header-close', '', 'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/mingcute_close-line.png?v=1725521249');
    addOnItemModalHeaderClose.alt = 'close-icon';
    addOnItemModalHeader.appendChild(addOnItemModalHeaderClose);

    // Image Container
    const addOnItemModalImageHandler = createModalElement('a', '', '', `/products/${handle}?variant=${selectedVariant?.variantId}`);
    addOnItemModalImageHandler.appendChild(createModalElement('img', 'similar-products-item-modal-image', '', selectedVariant?.imageUrl));
    const addOnItemModalImageContainer = createModalElement('div', 'similar-products-item-modal-image-container');
    addOnItemModalImageContainer.appendChild(addOnItemModalImageHandler);

    // Description
    const addOnItemModalDescriptionText = createModalElement('div', 'similar-products-item-modal-description-text', selectedVariant?.displayName);
    const addOnItemModalDescriptionArrow = createModalElement('img', 'similar-products-item-modal-description-arrow', '', 'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/ArrowRight.png?v=1725523530');
    addOnItemModalDescriptionArrow.alt = 'arrow-right-icon';
    const arrowHandler = createModalElement('a', '', '', `/products/${handle}?variant=${selectedVariant?.variantId}`);
    arrowHandler.appendChild(addOnItemModalDescriptionArrow);
    const addOnItemModalDescription = createModalElement('div', 'similar-products-item-modal-description');
    addOnItemModalDescription.appendChild(addOnItemModalDescriptionText);
    addOnItemModalDescription.appendChild(arrowHandler);

    arrowHandler.addEventListener("click", function(event) {
      event.preventDefault(); // Prevent the default anchor link behavior
      let eventPramaObj = {
        "source": "PDP recs",
        "widget": "Similar Products",
      };
      localStorage.setItem('eventPramaObj', JSON.stringify(eventPramaObj));

      // Replace the window location with the href
      window.location.replace(arrowHandler.href);
    });

    // Price Container
    const addOnItemModalPrice = createModalElement('div', 'similar-products-item-modal-price', `${selectedVariant?.price}`);
    const addOnItemModalPriceContainer = createModalElement('div', 'similar-products-item-modal-price-container');
    addOnItemModalPriceContainer.appendChild(addOnItemModalPrice);
    const addOnItemModalMrp = createModalElement(
      'div',
      'similar-products-item-modal-mrp',
      selectedVariant?.compareAtPrice === selectedVariant?.price
        ? `MRP ${selectedVariant?.compareAtPrice}`
        : `MRP <span style="text-decoration: line-through;">${selectedVariant?.compareAtPrice}</span>`
    );
    addOnItemModalPriceContainer.appendChild(addOnItemModalMrp);
    const addOnItemModalSaveText = createModalElement(
      'div',
      'similar-products-item-modal-save-text',
      `Save ${Math.round(selectedVariant?.discount)}%`
    );
    addOnItemModalPriceContainer.appendChild(addOnItemModalSaveText);
    if (selectedVariant?.compareAtPrice === selectedVariant?.price) {
      addOnItemModalSaveText.style.display = 'none';
    } else {
      addOnItemModalSaveText.style.display = 'block';
    }

    // Other Elements
    let addOnItemModalSubText;
    if(selectedVariant?.pricePerHundred){
      addOnItemModalSubText = createModalElement('div', 'similar-products-item-modal-sub-text', `(${selectedVariant?.pricePerHundred}/100g)`);
    }
    else{
      addOnItemModalSubText = createModalElement('div', 'similar-products-item-modal-sub-text', '');
    }
    const addOnItemModalEddLoader = createModalElement('div', 'loader_edd_recs');
    const addOnItemModalEddIcon = createModalElement('img', 'similar-products-item-modal-edd-icon', '', '');
    addOnItemModalEddIcon.alt = 'edd-modal-icon';
    const addOnItemModalEddText = createModalElement('div', 'similar-products-item-modal-edd-text', '');
    const addOnItemModalEdd = createModalElement('div', 'similar-products-item-modal-edd', '', 'similar-products-item-modal-edd');
    addOnItemModalEdd.setAttribute('id', 'similar-products-item-modal-edd');
    addOnItemModalEdd.appendChild(addOnItemModalEddLoader);  
    // addOnItemModalEdd.appendChild(addOnItemModalEddIcon);
    // addOnItemModalEdd.appendChild(addOnItemModalEddText);

    //Update the EDD by default
  

    // Variant Bubbles
    const addOnItemModalVariantContainer = createModalElement('div', 'similar-products-item-modal-variant-container', '','', 'similar-products-item-modal-variant-container');
    let lowStockDiv = '';
    let isLowStock = false;
    sortedVariantsArr.forEach(variant => {
      let isVisible = false;
      if (variant.inventoryQuantity < 5) {
        isVisible = true;
        isLowStock = true;
        lowStockDiv = `
          <div class="low-stock-alert">
            <p style="font-family: Lato; font-size: 12px; font-weight: 500; text-align: center; margin: 0;">
              ${variant.inventoryQuantity} units left
            </p>
          </div>
        `;
      }
      const variantBubble = createModalElement('button', 'similar-products-item-modal-variant-bubble', variant.title);
      variantBubble.setAttribute('id', `variant-bubble-${variant.id}`);
      variantBubble.dataset.id = variant.id;
      if (variant.id === selectedVariant?.id) {
        variantBubble.classList.add('active');
      }
      variantBubble.addEventListener('click', function () {
        const variantId = this.dataset.id;
        selectedVariant = variantsArr.find(v => v.id === variantId);
        updateModalContent(selectedVariant, desktopAddToCart, desktopAddToCartStepper, increaseQuantitySP, decreaseQuantity, handleIncreaseQuantity, handleDecreaseQuantity);
        displayEddInDrawer(selectedVariant?.sku);
      });
      const variantContainer = createModalElement('div', 'variant-container');
      if(isVisible) {
        variantContainer.innerHTML = lowStockDiv;
      }
      else{
        variantContainer.innerHTML = '';
      }
      const variantBox = createModalElement('div', 'variant-box');
      variantBox.setAttribute('id', `variant-box-${variant.id}`);
      variantBox.dataset.id = variant.id;

      // Append the variant bubble to the variant box
      variantBox.appendChild(variantBubble);

      // Append the variant box to the container
      variantContainer.appendChild(variantBox);

      // Append the container to the modal variant container
      addOnItemModalVariantContainer.appendChild(variantContainer);
    });
  
    //Add to Cart Button
    const desktopAddToCart = document.getElementById('similar-products-item-modal-button-desktop');
    const desktopAddToCartStepper = document.getElementById('similar-products-item-modal-button-container-desktop');
    const increaseQuantitySP = document.getElementById('similar-products-item-modal-add-button-desktop')
    const decreaseQuantity = document.getElementById('similar-products-item-modal-minus-button-desktop');

    
    addOnItemModalContent.innerHTML = '';
    addOnItemModalContent.appendChild(addOnItemModalHeader);
    addOnItemModalContent.appendChild(addOnItemModalImageContainer);
    addOnItemModalContent.appendChild(addOnItemModalDescription);
    addOnItemModalContent.appendChild(addOnItemModalPriceContainer);
    if(addOnItemModalSubText){
      addOnItemModalContent.appendChild(addOnItemModalSubText);
    }
    addOnItemModalContent.appendChild(addOnItemModalEdd);
    addOnItemModalContent.appendChild(addOnItemModalVariantContainer);
    addOnItemModalContent.appendChild(desktopAddToCart);
    addOnItemModalContent.appendChild(desktopAddToCartStepper);
    



    let defaultVariantQuantity = await checkItemAlreadyInCartRecommendations(selectedVariant?.variantId);
    if(defaultVariantQuantity?.quantity){
      desktopAddToCartStepper.style.display = 'flex';
      desktopAddToCart.style.display = 'none';
      desktopAddToCartStepper.children[0].children[1].children[0].innerHTML = defaultVariantQuantity?.quantity;
      if (!increaseQuantitySP.hasAttribute('dirty-SP')) {
        increaseQuantitySP.addEventListener('click', handleIncreaseQuantity);
        increaseQuantitySP.setAttribute('dirty-SP', 'true');
      }
      if (!decreaseQuantity.hasAttribute('dirty-SP')) {
        decreaseQuantity.addEventListener('click', handleDecreaseQuantity);
        decreaseQuantity.setAttribute('dirty-SP', 'true');
      }
      desktopAddToCartStepper.children[1].addEventListener("click", handleCartRedirect);
    }
    else{
      desktopAddToCartStepper.style.display = 'none';
      increaseQuantitySP.removeEventListener('click', changeCartQuantityDesktop);
      decreaseQuantity.removeEventListener('click', changeCartQuantityDesktop);
      desktopAddToCart.style.display = 'flex';
    }

    let variantDetailsQuickAdd = document.getElementById('similar-products-item-modal-variant-container');
    displayEddInDrawer(selectedVariant?.sku);
    if(isLowStock){
      variantDetailsQuickAdd.style.marginTop = '30px';
      variantDetailsQuickAdd.style.rowGap = '30px';
    }
    desktopAddToCart.addEventListener("click", async function() {
      window.dataLayer = window.dataLayer || [];
      const dataLayerObj = {
        "event": "add_to_cart",
        "source": 'PDP recommendation',
        "widget": 'Similar Products',
        "ecommerce": {
        "currency": "INR",
        "value": selectedVariant?.price,
        "items": [
          {
              "item_id": selectedVariant?.variantId,
              "item_sku": selectedVariant?.sku,
              "item_name": selectedVariant?.displayName,
              "coupon": "",
              "discount": "",
              "item_list_id": "",
              "item_list_name": "",
              "item_variant": selectedVariant?.displayName,
              "location_id": "",
              "price": selectedVariant?.price,
              "quantity": 1,
              "item_category": productDetails[0]?.category,
              "item_category3": productDetails[0]?.category3,
              "item_category2": productDetails[0]?.category2,
              "item_category4": "",
              "item_category5": "",
          }
        ]
        }
      }
      const clevertapObj = {
        "product_id": selectedVariant?.variantId,
        "product_name": selectedVariant?.displayName,
        "brand": productDetails[0]?.brand,
        "variant_name": selectedVariant?.displayName,
        "variant_id": selectedVariant?.variantId,
        "price": selectedVariant?.price,
        "mrp": selectedVariant?.compareAtPrice/100,
        "quantity": 1,
        "product_sku": selectedVariant?.sku,
        "product_category1": productDetails[0]?.category, 
        "product_category3": productDetails[0]?.category3,
        "product_category2": productDetails[0]?.category2,
        "product_category4": "",
        "product_category5": "",
        "source": 'PDP recommendation',
        "widget": 'Similar Products'
      };
      let url = '/cart/add.js';
      const payload = {
        id: selectedVariant?.variantId,
        quantity: 1
      };
      desktopAddToCart.classList.add('atc-loader');
      desktopAddToCart.innerHTML = '';
      try {
        const res = await fetch(url, {
          method: 'POST',
          headers: {
            Accept: "application/json",
            "Content-Type": "application/json"
          },
          body: JSON.stringify(payload)
        });
        const data = await res.json();
        if(data?.status !== 422) {
          desktopAddToCart.style.display = 'none';
          desktopAddToCartStepper.style.display = 'flex';
          desktopAddToCartStepper.children[0].children[1].children[0].innerHTML = '1';
          desktopAddToCartStepper.children[1].addEventListener("click", handleCartRedirect);
          if (!increaseQuantitySP.hasAttribute('dirty-SP')) {
            increaseQuantitySP.addEventListener('click', handleIncreaseQuantity);
            increaseQuantitySP.setAttribute('dirty-SP', 'true');
          }
          if (!decreaseQuantity.hasAttribute('dirty-SP')) {
            decreaseQuantity.addEventListener('click', handleDecreaseQuantity);
            decreaseQuantity.setAttribute('dirty-SP', 'true');
          }
          dataLayer.push(dataLayerObj);
          clevertap.event.push("Product Added", clevertapObj);
          updateCartIconNumberSP();
        }
      } catch(err) {
        console.log("error", err);
      }
      finally {
        desktopAddToCart.classList.remove('atc-loader');
        desktopAddToCart.innerHTML = 'Add to cart';
      }
    });

    

    
    // Close Modal
    addOnItemModalHeaderClose.addEventListener('click', function () {
      addOnItemModal.style.display = 'none';
      desktopModalBackdrop.style.display = 'none';
      document.body.style.overflow = 'auto';
    });
  }

  async function updateModalContent(variant, desktopAddToCart, desktopAddToCartStepper, increaseQuantitySP, decreaseQuantity, handleIncreaseQuantity, handleDecreaseQuantity) {
    let selected_variant_check = await checkItemAlreadyInCartRecommendations(variant?.variantId) 

    const addOnItemModalContent = document.getElementById("similar-products-item-modal-content");
    
    function handleIncreaseQuantityVariant(event) {
      changeCartQuantityDesktop(variant?.variantId, true, desktopAddToCartStepper);
    }
    function handleDecreaseQuantityVariant(event) {
      changeCartQuantityDesktop(variant?.variantId, false, desktopAddToCartStepper, desktopAddToCart);
    }



    document.querySelector('.similar-products-item-modal-image').src = variant.imageUrl;
    document.querySelector('.similar-products-item-modal-description-text').innerHTML = variant.displayName;
    document.querySelector('.similar-products-item-modal-price').innerHTML = `${variant.price}`;
    
    document.querySelector('.similar-products-item-modal-mrp').innerHTML = 
      variant?.compareAtPrice === variant?.price 
        ? `MRP ${variant.compareAtPrice}`
        : `MRP <span style="text-decoration: line-through;">${variant.compareAtPrice}</span>`;

    // Set the Save Text with conditional display
    document.querySelector('.similar-products-item-modal-save-text').innerHTML = `Save ${Math.round(variant.discount)}%`;
    document.querySelector('.similar-products-item-modal-save-text').style.display = 
      variant?.compareAtPrice === variant?.price ? 'none' : 'block';

    // Set the price per hundred if available
    if (variant?.pricePerHundred) {
      document.querySelector('.similar-products-item-modal-sub-text').innerHTML = `(${variant?.pricePerHundred}/100g)`;
    }
    else{
      document.querySelector('.similar-products-item-modal-sub-text').innerHTML = '';
    }
    
    const allBubbles = document.querySelectorAll('.similar-products-item-modal-variant-bubble');
    allBubbles.forEach(bubble => bubble.classList.remove('active'));

    // Remove the `#` from the ID
    const selectedBubble = document.getElementById(`variant-bubble-${variant.id}`);
    if (selectedBubble) {
      selectedBubble.classList.add('active');
    } else {
      console.error(`No element found with ID variant-bubble-${variant.id}`);
    }
    if(selected_variant_check?.quantity){
      desktopAddToCartStepper.style.display = 'flex';
      desktopAddToCart.style.display = 'none';
      desktopAddToCartStepper.children[0].children[1].children[0].innerHTML = selected_variant_check?.quantity;
      if (!increaseQuantitySP.hasAttribute('dirty-SP')) {
          increaseQuantitySP.addEventListener('click', handleIncreaseQuantityVariant);
          increaseQuantitySP.setAttribute('dirty-SP', 'true');
        }
      if (!decreaseQuantity.hasAttribute('dirty-SP')) {
        decreaseQuantity.addEventListener('click', handleDecreaseQuantityVariant);
        decreaseQuantity.setAttribute('dirty-SP', 'true');
      }
    }
    else{
      desktopAddToCartStepper.style.display = 'none';
      desktopAddToCart.style.display = 'flex';
    }
  }

  function handleCartRedirect() {
    window.location.href = '/cart';
  }


  async function displayVariantDrawerMWeb(variantsArr, handle, productDetails) {
    const sortedVariantsArr = variantsArr.sort((a, b) => {
      return (a.isDefault === b.isDefault) ? 0 : a.isDefault ? -1 : 1;
    });
    
    let selectedVariant = sortedVariantsArr.find(variant => variant.isDefault);

    const mobileDrawer = document.getElementById("mobile-drawer-container");
    function handleIncreaseQuantity(event) {
      changeCartQuantity(selectedVariant?.variantId, true, mobileAddToCartStepper);
    }
    function handleDecreaseQuantity(event) {
      changeCartQuantity(selectedVariant?.variantId, false, mobileAddToCartStepper, mobileAddToCart);
    }
    // Create modal elements (simplified for brevity)
    const createDrawerElement = (tag, className, innerHTML = '', src = '', href = '', id = '') => {
      const element = document.createElement(tag);
      if (className) element.classList.add(className);
      if (innerHTML) element.innerHTML = innerHTML;
      if (src) element.src = src;
      if (id) element.setAttribute('id', id);
      if (href && tag === 'a') element.setAttribute('href', href); 
      return element;
    };

    // Drawer Header
    const drawerHeader = createDrawerElement('div', 'mobile-drawer-header');
    const drawerImage = createDrawerElement('div', 'mobile-drawer-image');
    const drawerImageImg = createDrawerElement('img', '', '', selectedVariant?.imageUrl);
    drawerImageImg.alt = 'Product Image';
    drawerImageImg.width = 70;
    drawerImageImg.height = 70;
    drawerImage.appendChild(drawerImageImg);

    
    // Drawer Details
    const drawerDetails = createDrawerElement('div', 'mobile-drawer-details');
    const detailsHeader = createDrawerElement('div', 'mobile-drawer-details-header');
    const drawerTitle = createDrawerElement('p', 'mobile-drawer-details-title', selectedVariant?.displayName);
    const arrowButton = createDrawerElement('a', '', '','', `/products/${handle}?variant=${selectedVariant?.variantId}`);
    const arrowImg = createDrawerElement('img', '', '', 'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/ArrowRight.png?v=1725866116');
    arrowImg.width = 13;
    arrowImg.height = 16;
    arrowButton.appendChild(arrowImg);
    detailsHeader.appendChild(drawerTitle);
    detailsHeader.appendChild(arrowButton);


    arrowButton.addEventListener("click", function(event) {
      event.preventDefault();
      let eventPramaObj = {
        "source": "PDP recs",
        "widget": "Similar Products",
      };
      localStorage.setItem('eventPramaObj', JSON.stringify(eventPramaObj));
      window.location.replace(arrowButton.href);
    });
    
    // Price Details
    const priceDetails = createDrawerElement('div', 'mobile-drawer-priceDetails');
    const priceContainer = createDrawerElement('div', 'mobile-drawer-price');
    const priceValue = createDrawerElement('p', 'mobile-drawer-price-value', `${selectedVariant?.price}`);
    

    const priceMrp = createDrawerElement(
      'p',
      'mobile-drawer-price-mrp',
      selectedVariant?.compareAtPrice === selectedVariant?.price
        ? `MRP ${selectedVariant?.compareAtPrice}`
        : `MRP <span style="text-decoration: line-through;">${selectedVariant?.compareAtPrice}</span>`
    );
    const priceDiscount = createDrawerElement(
      'p',
      'mobile-drawer-price-discount',
      `SAVE ${Math.round(selectedVariant?.discount)}%`
    );

    if (selectedVariant?.compareAtPrice === selectedVariant?.price) {
      priceDiscount.style.display = 'none';
    } else {
      priceDiscount.style.display = 'block';
    }
    priceContainer.appendChild(priceValue);
    priceContainer.appendChild(priceMrp);
    priceContainer.appendChild(priceDiscount);
   
    let unitPrice;
    if(selectedVariant?.pricePerHundred){
      unitPrice = createDrawerElement('p', 'mobile-drawer-unit-price-value', `(${selectedVariant?.pricePerHundred}/100g)`);
    }
    else{
      unitPrice = createDrawerElement('p', 'mobile-drawer-unit-price-value', '');
    }

    //Other Details

    const addOnItemModalEddLoader = createDrawerElement('div', 'loader_edd_recs');

    const addOnItemModalEddIcon = createDrawerElement('img', 'similar-products-item-modal-edd-icon-mweb', '', '');
    addOnItemModalEddIcon.alt = 'edd-modal-icon';
    const addOnItemModalEddText = createDrawerElement('div', 'similar-products-item-modal-edd-text-mweb', '');
    const addOnItemModalEdd = createDrawerElement('div', 'similar-products-item-modal-edd-mweb', '', 'similar-products-item-modal-edd-mweb');
    
    addOnItemModalEdd.appendChild(addOnItemModalEddLoader);
    
    
    
    
    // Append everything
    priceDetails.appendChild(priceContainer);
    if(unitPrice){
      priceDetails.appendChild(unitPrice);
    }
    priceDetails.appendChild(addOnItemModalEdd);
    drawerDetails.appendChild(detailsHeader);
    drawerDetails.appendChild(priceDetails);

    drawerHeader.appendChild(drawerImage);
    drawerHeader.appendChild(drawerDetails);


    const crossIcon0 = createDrawerElement('img', 'mobile-drawer-cross-icon-0', '', 'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/closeIcon.png?v=1726478114', '', 'mobile-drawer-cross-icon-0');
    crossIcon0.addEventListener('click', function () {
      mobileDrawer.style.display = 'none';
      document.body.style.overflow = 'auto';
      document.getElementById('mobile-drawer-backdrop').style.display = 'none';
    })

    // Variant Bubbles
    const variantDetails = createDrawerElement('div', 'variant-details-0', '','', '','variant-details-0');
    let lowStockDiv = '';
    let isLowStockDiv = false;
    sortedVariantsArr.forEach(variant => {
      let isVisible = false;
      if (variant.inventoryQuantity < 5) {
        isLowStockDiv = true;
        isVisible = true;
        lowStockDiv = `
          <div class="low-stock-alert">
            <p style="font-family: Lato; font-size: 12px; font-weight: 500; text-align: center; margin: 0;">
              ${variant.inventoryQuantity} units left
            </p>
          </div>
        `;
      }
      else{
        lowStockDiv = '';
      }

      // Create the variant bubble
      const variantBubble = createDrawerElement('button', 'mobile-drawer-variant-bubble', variant.title);
      variantBubble.setAttribute('id', `mobile-drawer-variant-bubble-${variant.id}`);
      variantBubble.dataset.id = variant.id;
      if (variant.id === selectedVariant?.id) {
        variantBubble.classList.add('active');
      }
      variantBubble.addEventListener('click', function () {
        const variantId = this.dataset.id;
        selectedVariant = variantsArr.find(v => v.id === variantId);
        updateModalContentMWeb(selectedVariant, mobileAddToCart, mobileAddToCartStepper, increaseQuantitySP, decreaseQuantity, handleIncreaseQuantity, handleDecreaseQuantity);
        displayEddInDrawerMWeb(selectedVariant?.sku);
      });

      // Create the variant container with the optional lowStockDiv
      const variantContainer = createDrawerElement('div', 'variant-container');
      if(isVisible) {
        variantContainer.innerHTML = lowStockDiv; // Insert low stock alert if applicable
      }
      else{
        variantContainer.innerHTML = '';
      }

      // Create the variant box
      const variantBox = createDrawerElement('div', 'variant-box');
      variantBox.setAttribute('id', `variant-box-${variant.id}`);
      variantBox.dataset.id = variant.id;
      const variantTitle = createDrawerElement('p', 'variant-title', variant.title);
      variantBox.appendChild(variantTitle);

      // Append the variant box to the container
      variantContainer.appendChild(variantBubble);

      // Append the container to variant details
      variantDetails.appendChild(variantContainer);
    });

    //Add to cart button

    const mobileAddToCart = document.getElementById('similar-products-item-modal-button-mweb');
    const mobileAddToCartStepper = document.getElementById('similar-products-item-modal-button-container-mweb');
    const increaseQuantitySP = document.getElementById('similar-products-item-modal-add-button-mweb');
    const decreaseQuantity = document.getElementById('similar-products-item-modal-minus-button-mweb');

    // quantityContainer
    
    //line 
    const seperator = createDrawerElement('hr', 'mobile-separator', '', '');
    // Clear drawer content and append new elements
    mobileDrawer.innerHTML = '';
    mobileDrawer.appendChild(drawerHeader);
    mobileDrawer.appendChild(crossIcon0);
    mobileDrawer.appendChild(createDrawerElement('hr', 'mobile-divider', '', ''));
    mobileDrawer.appendChild(variantDetails);
    mobileDrawer.appendChild(mobileAddToCart);
    mobileDrawer.appendChild(mobileAddToCartStepper);
    mobileDrawer.insertBefore(seperator, mobileAddToCart);
    
    let defaultVariantQuantity = await checkItemAlreadyInCartRecommendations(selectedVariant?.variantId);
    if(defaultVariantQuantity?.quantity){
      mobileAddToCartStepper.style.display = 'flex';
      mobileAddToCart.style.display = 'none';
      mobileAddToCartStepper.children[0].children[1].children[0].innerHTML = defaultVariantQuantity?.quantity;
      if (!increaseQuantitySP.hasAttribute('dirty-SP')) {
        increaseQuantitySP.addEventListener('click', handleIncreaseQuantity);
        increaseQuantitySP.setAttribute('dirty-SP', 'true');
      }
      if (!decreaseQuantity.hasAttribute('dirty-SP')) {
        decreaseQuantity.addEventListener('click', handleDecreaseQuantity);
        decreaseQuantity.setAttribute('dirty-SP', 'true');
      }
      mobileAddToCartStepper.children[1].addEventListener("click", handleCartRedirect);
    }
    else{
      mobileAddToCartStepper.style.display = 'none';
      increaseQuantitySP.removeEventListener('click', changeCartQuantity);
      decreaseQuantity.removeEventListener('click', changeCartQuantity);
      mobileAddToCart.style.display = 'flex';
    }



    let crossIconContainer0 = document.getElementById('mobile-drawer-cross-icon-0');
    mobileAddToCart.addEventListener("click", async function() {
      const dataLayerObj = {
        "event": "add_to_cart",
        "source": 'PDP recommendation',
        "widget": 'Similar Products',
        "ecommerce": {
        "currency": "INR",
        "value": selectedVariant?.price,
        "items": [
          {
              "item_id": selectedVariant?.variantId,
              "item_sku": selectedVariant?.sku,
              "item_name": selectedVariant?.displayName,
              "coupon": "",
              "discount": "",
              "item_list_id": "",
              "item_list_name": "",
              "item_variant": selectedVariant?.displayName,
              "location_id": "",
              "price": selectedVariant?.price,
              "quantity": 1,
              "item_category": productDetails[0]?.category,
              "item_category3": productDetails[0]?.category3,
              "item_category2": productDetails[0]?.category2,
              "item_category4": "",
              "item_category5": "",
          }
        ]
        }
      }
      const clevertapObj = {
        "product_id": selectedVariant?.variantId,
        "product_name": selectedVariant?.displayName,
        "brand": productDetails[0]?.brand,
        "variant_name": selectedVariant?.displayName,
        "variant_id": selectedVariant?.variantId,
        "price": selectedVariant?.price,
        "mrp": selectedVariant?.compareAtPrice/100,
        "quantity": 1,
        "product_sku": selectedVariant?.sku,
        "product_category1": productDetails[0]?.category, 
        "product_category3": productDetails[0]?.category3,
        "product_category2": productDetails[0]?.category2,
        "product_category4": "",
        "product_category5": "",
        "source": 'PDP recommendation',
        "widget": 'Similar Products'
      };
      let url = '/cart/add.js';
      const payload = {
        id: selectedVariant?.variantId,
        quantity: 1
      };
      try {
        const res = await fetch(url, {
          method: 'POST',
          headers: {
            Accept: "application/json",
            "Content-Type": "application/json"
          },
          body: JSON.stringify(payload)
        });
        mobileAddToCart.classList.add('atc-loader-mweb')
        mobileAddToCart.innerHTML =  '';
        const data = await res.json();
        if(data?.status !== 422) {
          mobileAddToCart.style.display = 'none';
          mobileAddToCartStepper.style.display = 'flex';
          mobileAddToCartStepper.children[0].children[1].children[0].innerHTML = '1';
          mobileAddToCartStepper.children[1].addEventListener("click", handleCartRedirect);
          if (!increaseQuantitySP.hasAttribute('dirty-SP')) {
            increaseQuantitySP.addEventListener('click', handleIncreaseQuantity);
            increaseQuantitySP.setAttribute('dirty-SP', 'true');
          }
          if (!decreaseQuantity.hasAttribute('dirty-SP')) {
            decreaseQuantity.addEventListener('click', handleDecreaseQuantity);
            decreaseQuantity.setAttribute('dirty-SP', 'true');
          }
          dataLayer.push(dataLayerObj);
          clevertap.event.push("Product Added", clevertapObj);
          updateCartIconNumberSP();
        }
      } catch(err) {
        console.log("error", err);
      }
      finally{
        mobileAddToCart.innerHTML = 'Add to Cart'
        mobileAddToCart.classList.remove('atc-loader-mweb')
      }
    });
    displayEddInDrawerMWeb(selectedVariant?.sku);
    let variantDetails0 = document.getElementById('variant-details-0');
    if(isLowStockDiv){
      variantDetails0.style.marginTop = '18%';
      variantDetails0.style.rowGap = '40px';
    }
    crossIconContainer0.style.bottom = mobileDrawer.clientHeight + 7 + 'px';
  }

  async function changeCartQuantity(selectedVariantId, isAdd, mobileAddToCartStepper, mobileAddToCart) {
    let url = '/cart/change.js';
    const itemAlreadyInCartSP =  await checkItemAlreadyInCartRecommendations(selectedVariantId);
    const updateQuantitySP = itemAlreadyInCartSP ? itemAlreadyInCartSP.quantity + (isAdd ? 1 : -1) : 1;
    const payload = {
      id: selectedVariantId,
      quantity: updateQuantitySP
    };
    try {
      const res = await fetch(url, {
        method: 'POST',
        headers: {
          Accept: "application/json",
          "Content-Type": "application/json"
        },
        body: JSON.stringify(payload)
      });
      const data = await res.json();
      if(data?.status !== 422) {
        mobileAddToCartStepper.children[0].children[1].children[0].innerHTML = updateQuantitySP;
        if(updateQuantitySP ===0) {
          mobileAddToCart.style.display = 'flex';
          mobileAddToCartStepper.style.display = 'none';
        }
        updateCartIconNumberSP();
      }
    } catch(err) {
      console.log("error", err);
    }
  }

  async function changeCartQuantityDesktop(selectedVariantId, isAdd, desktopAddToCartStepper, desktopAddToCart) {
    let url = '/cart/change.js';
    const itemAlreadyInCartSP =  await checkItemAlreadyInCartRecommendations(selectedVariantId);
    const updateQuantitySP = itemAlreadyInCartSP ? itemAlreadyInCartSP.quantity + (isAdd ? 1 : -1) : 1;
    const payload = {
      id: selectedVariantId,
      quantity: updateQuantitySP
    };
    try {
      const res = await fetch(url, {
        method: 'POST',
        headers: {
          Accept: "application/json",
          "Content-Type": "application/json"
        },
        body: JSON.stringify(payload)
      });
      const data = await res.json();
      if(data?.status !== 422) {
        desktopAddToCartStepper.children[0].children[1].children[0].innerHTML = updateQuantitySP;
        if(updateQuantitySP ===0) {
          desktopAddToCart.style.display = 'flex';
          desktopAddToCartStepper.style.display = 'none';
        }
        updateCartIconNumberSP();
      }
    } catch(err) {
      console.log("error", err);
    }
  }
  
  

  async function updateModalContentMWeb(variant, mobileAddToCart, mobileAddToCartStepper, increaseQuantitySP, decreaseQuantity, handleIncreaseQuantity, handleDecreaseQuantity) {

      // Update image
      document.querySelector('.mobile-drawer-image img').src = variant.imageUrl;

      // Update product title
      document.querySelector('.mobile-drawer-details-title').innerHTML = variant.displayName;
      function handleIncreaseQuantityVariant(event) {
        changeCartQuantity(variant?.variantId, true, mobileAddToCartStepper);
      }
      function handleDecreaseQuantityVariant(event) {
        changeCartQuantity(variant?.variantId, false, mobileAddToCartStepper, mobileAddToCart);
      }
      let defaultVariantQuantity = await checkItemAlreadyInCartRecommendations(variant?.variantId);

      // Update price, MRP:, and discount
      document.querySelector('.mobile-drawer-price-value').innerHTML = `${variant.price}`;
      document.querySelector('.mobile-drawer-price-mrp').innerHTML = 
      variant?.price === variant?.compareAtPrice 
        ? `MRP ${variant.compareAtPrice}`
        : `MRP <span style="text-decoration: line-through;">${variant.compareAtPrice}</span>`;

      document.querySelector('.mobile-drawer-price-discount').innerHTML = `SAVE ${Math.round(variant.discount)}%`;
      document.querySelector('.mobile-drawer-price-discount').style.display = variant?.price === variant?.compareAtPrice ? 'none' : 'block';

      // Update price per unit
      if(variant.pricePerHundred){
        document.querySelector('.mobile-drawer-unit-price-value').innerHTML = `(${variant.pricePerHundred}/100g)`;
      }
      else{
        document.querySelector('.mobile-drawer-unit-price-value').innerHTML = '';
      }

      // Deselect all variant bubbles
      const allBubbles = document.querySelectorAll('.mobile-drawer-variant-bubble');
      allBubbles.forEach(bubble => bubble.classList.remove('active'));

      // Set the selected variant bubble to active
      const selectedBubble = document.getElementById(`mobile-drawer-variant-bubble-${variant.id}`);
      if (selectedBubble) {
          selectedBubble.classList.add('active');
      } else {
          console.error(`No element found with ID variant-bubble-${variant.id}`);
      }
      if(defaultVariantQuantity?.quantity){
        mobileAddToCartStepper.style.display = 'flex';
        mobileAddToCart.style.display = 'none';
        mobileAddToCartStepper.children[0].children[1].children[0].innerHTML = defaultVariantQuantity?.quantity;
        if (!increaseQuantitySP.hasAttribute('dirty-SP')) {
            increaseQuantitySP.addEventListener('click', handleIncreaseQuantityVariant);
            increaseQuantitySP.setAttribute('dirty-SP', 'true');
          }
        if (!decreaseQuantity.hasAttribute('dirty-SP')) {
          decreaseQuantity.addEventListener('click', handleDecreaseQuantityVariant);
          decreaseQuantity.setAttribute('dirty-SP', 'true');
        }
      }
      else{
        mobileAddToCartStepper.style.display = 'none';
        mobileAddToCart.style.display = 'flex';
      }
  }

  let addOnProductsAllVariants = [];

  let  updatedCartItemsArrSP = {"note":null,"attributes":{},"original_total_price":0,"total_price":0,"total_discount":0,"total_weight":0.0,"item_count":0,"items":[],"requires_shipping":false,"currency":"INR","items_subtotal_price":0,"cart_level_discount_applications":[],"checkout_charge_amount":0}; 

  async function getCartSP() {
    const url = '/cart.js';
    const res = await fetch(url);
    const data = await res.json();
    return data;
  }

  async function updateCartIconNumberSP() {
    updatedCartItemsArrSP = await getCartSP();
    const CartBubbles = document.querySelectorAll('cart-link__bubble');
    const cartIcons = document.querySelectorAll(".cart-link__bubble-num");
    if (CartBubbles.length) {
      CartBubbles.forEach(element => {
        showElement(element);
      })
    }
    if (cartIcons.length) {
      cartIcons.forEach(element => {
        element.textContent = updatedCartItemsArrSP?.item_count;
      })
    }
  }

  async function viewToast() {
      const successMessageDiv = `
        <div class="supertails_shipping_addmore-brij" id="supertails_shipping_addmore-brij-id-recommendations">
          <div style="display: flex; gap: 10px; justify-content:center; align-items:center;">
            <div style="display: flex; flex-direction:column; justify-content:center; align-items:flex-start;">
              <div class="sticky-atc-pdp-header-message" style="display: flex; justify-content: left; align-items: center; gap: 4px; color: black; font-size: 12px; height: 48px;">
                <img src="https://cdn.shopify.com/s/files/1/0565/8021/0861/files/sticky-atc-pdp-min.gif?v=1692697712" alt="twinkle" style="width:44px;height:44px">
                <div style="width: 144px;">
                  <div class="cart_itemadded" style="text-align:left;"> 1 Item added</div>
                  <div class="sticky-atc-pdp-footer-message" style="line-height: 15px; color: white; font-size:12px; text-align:left;"></div>
                </div>
              </div>
            </div>
            <a style="color:white" href="/cart">
              <div class="atc-btn" style="display: flex; justify-content: center; align-items: center; font-size: 12px; border: 1px solid white; padding: 10px 12px; border-radius: 6px; color: white; height: min-content; font-weight:bold; min-width:125px;">
                <svg style="margin-right:4px;" width="20" height="18" viewBox="0 0 20 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" clip-rule="evenodd" d="M0.176071 0.184241C-0.0887979 0.446838 -0.051706 0.830741 0.26626 1.11592C0.472184 1.3007 0.925431 1.37364 2.07463 1.40659L3.60813 1.45069L5.23952 7.55406L6.87105 13.6574L12.2871 13.6976C17.994 13.7398 18.3672 13.7012 18.2769 13.0787C18.1813 12.4209 17.7493 12.365 12.736 12.3628C10.1381 12.3615 7.98194 12.3089 7.94469 12.246C7.90729 12.1832 7.82849 11.9084 7.76939 11.6357L7.66212 11.1398H12.4265C16.0553 11.1398 17.2268 11.0943 17.3412 10.9491C17.56 10.6715 20.0014 4.39328 20 4.11191C19.9994 3.98115 19.9007 3.74083 19.7805 3.57787C19.5714 3.29421 19.2659 3.2785 12.5183 3.20541L5.47422 3.12912L5.06022 1.70017C4.83259 0.914205 4.55126 0.211554 4.43506 0.138466C4.10047 -0.07149 0.393999 -0.0318188 0.176071 0.184241ZM8.53307 14.7072C7.53792 15.1502 7.13115 16.1732 7.59594 17.0643C8.29344 18.4016 10.3414 18.2463 10.7713 16.8236C11.1861 15.4514 9.8017 14.1425 8.53307 14.7072ZM14.8162 14.7174C13.9019 15.1152 13.5074 16.1313 13.9271 17.008C14.6849 18.5914 16.9876 18.1895 17.1866 16.4392C17.2573 15.8173 17.2154 15.7004 16.7465 15.2114C16.1433 14.5823 15.5035 14.4185 14.8162 14.7174Z" fill="white"/>
                </svg>
                GO TO CART
              </div>
            </a>
          </div>
        </div>`;
      const toastContainer = document.createElement('div');
      toastContainer.innerHTML = successMessageDiv;
      const parentElement = document.getElementById('HeaderCart');
      while (parentElement.firstChild) {
        parentElement.removeChild(parentElement.firstChild);
      }
      parentElement.appendChild(toastContainer.firstElementChild);
      parentElement.style.display = 'flex';
      parentElement.style.justifyContent = 'center';
      parentElement.style.alignItems = 'center';
      parentElement.style.width = '350px';
      parentElement.style.height = '60px';
      $('.cart_itemadded').css('font-size', '16px');
      parentElement.style.transform = 'translateY(0px)';
      parentElement.style.opacity = '1';
      if(window.innerWidth < 768) {
        parentElement.style.marginTop = '4px';
      }
      else{
        parentElement.style.marginTop = '0px';
      }
      parentElement.style.transition = 'opacity 0.3s ease 0.15s, transform 0.25s cubic-bezier(0.165, 0.84, 0.44, 1) 0.15s';
      parentElement.classList.add('is-active')
      setTimeout(() => {
        parentElement.classList.remove('is-active');
    }, 1500);
  } 

  document.addEventListener("DOMContentLoaded", async function () {
    const widgetHeadingText = document.getElementById("similar-products-heading-text");
    const addOnItems = document.getElementById("similar-products-items");
    const addOnItemModal = document.getElementById("similar-products-item-modal");
    const addOnItemModalContent = document.getElementById("similar-products-item-modal-content");
    const addOnItemModalContentMWeb = document.getElementById("mobile-drawer-container");
    const loaderSimilar = document.getElementById("similar-products-loader");
    widgetHeadingText.style.display = "none";

    const desktopModalBackdrop = document.getElementById("similar-products-item-modal-backdrop");
    function handleBackdropClick(event) {
      if(event.target === addOnItemModal) {
        desktopModalBackdrop.style.display = "none";
        addOnItemModal.style.display = "none";
        document.body.style.overflow = 'auto';
      }
    }
    addOnItemModal.addEventListener("click", handleBackdropClick);


    const mobileDrawerBackdrop = document.getElementById("mobile-drawer-backdrop")
    const mobileDrawer = document.getElementById("mobile-drawer-container");
    
    function handleBackdropClickMWeb(event) {
      if(event.target === mobileDrawerBackdrop) {
        mobileDrawer.style.display = 'none';
        document.body.style.overflow = 'auto';
        mobileDrawerBackdrop.style.display = 'none';
      }
    }
    mobileDrawerBackdrop.addEventListener("click", handleBackdropClickMWeb);

    let addOnProducts = [];
    let addOnProductsVariants = [];
    let skuArray = [];



    let recommendationDataArrOrignal = await getPdpRecommendations();
    let featureFlagDataSimilar =  await fetchFeatureFlagsSimilar();
    let recommendationDataArr = [recommendationDataArrOrignal[0]];
    recommendationDataArr?.forEach((data) => {
      widgetHeadingText.innerHTML = data?.title;
      addOnProducts.push(...data?.recommendedProducts);
    });
    if(addOnProducts?.length === 0 || !featureFlagDataSimilar.visible){
      loaderSimilar.style.display = "none";
      document.getElementById('similar-products').style.display = 'none';
      return;
    }

    addOnProducts?.forEach((product) => {
      const defaultVariant = product?.variants?.find(variant => variant?.isDefault);
      if (defaultVariant) { 
        const addOnsSku = product?.variants?.map(variant => variant?.sku);
        skuArray.push(...addOnsSku);
        const variantWithDetails = {
          ...defaultVariant, 
          handle: product?.handle,
          totalVariants: product?.totalVariants,
          productId: product?.productId
        };
        addOnProductsVariants.push(variantWithDetails); 
      }
    });

    let eddData = await fetchEdd(skuArray);

    for (let i = 0; i < addOnProductsVariants?.length; i++) {
      const addOnItemCard = document.createElement("div");
      addOnItemCard.classList.add("similar-products-item-card");
      addOnItemCard.setAttribute("id", `similar-products-item-card-${addOnProductsVariants[i]?.sku}`);
      addOnItemCard.addEventListener("click", function(event) {
        event.preventDefault();
        let eventPramaObj = {
          "source": "PDP recs",
          "widget": "Similar Products",
        };
        localStorage.setItem('eventPramaObj', JSON.stringify(eventPramaObj));
        window.location.replace(`/products/${addOnProductsVariants[i]?.handle}?variant=${addOnProductsVariants[i]?.variantId}`);
      });

      const addOnItemImgContainer = document.createElement("div");
      addOnItemImgContainer.classList.add("similar-products-item-img-container");

      const addOnItemImg = document.createElement("img");
      addOnItemImg.classList.add("similar-products-item-img");
      addOnItemImg.setAttribute("id", `similar-products-item-img-${addOnProductsVariants[i]?.sku}`);
      addOnItemImg.src = addOnProductsVariants[i]?.imageUrl;

      const itemRating = document.createElement("div");
      itemRating.classList.add("similar-products-item-rating");
      itemRating.setAttribute("id", `similar-products-item-rating-${addOnProductsVariants[i]?.sku}`);


      const itemRatingValue = document.createElement("div");
      itemRatingValue.classList.add("similar-products-item-rating-value");
      itemRatingValue.innerHTML = `${addOnProductsVariants?.metaRating}`;

      const itemRatingStar = document.createElement("img");
      itemRatingStar.classList.add("similar-products-item-rating-star");
      itemRatingStar.src = "https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_32.png?v=1725429108";
      itemRatingStar.alt = 'rating-star';

      const itemRatingDivider = document.createElement("div");
      itemRatingDivider.classList.add("similar-products-item-rating-divider");

      const itemRatingTotal = document.createElement("div");
      itemRatingTotal.classList.add("similar-products-item-rating-total");
      itemRatingTotal.innerHTML = `${addOnProductsVariants?.metaRatingCount}`;

      const addOnItemDescriptionContainer = document.createElement("div");
      addOnItemDescriptionContainer.classList.add("similar-products-item-description-container");

      const addOnItemDescriptionText = document.createElement("div");
      addOnItemDescriptionText.classList.add("similar-products-item-description-text");
      addOnItemDescriptionText.innerHTML = addOnProductsVariants[i]?.displayName;

      const addOnItemDescriptionSubText = document.createElement("div");
      addOnItemDescriptionSubText.classList.add("similar-products-item-description-sub-text");
      addOnItemDescriptionSubText.innerHTML = addOnProductsVariants[i]?.title;

      const addOnItemDescriptionInfo = document.createElement("div");
      addOnItemDescriptionInfo.classList.add("similar-products-item-description-info");
      if(addOnProductsVariants[i]?.totalVariants > 1){
        addOnItemDescriptionInfo.innerHTML = `(${addOnProductsVariants[i]?.totalVariants} sizes available)`;
      }
      else{
        addOnItemDescriptionInfo.innerHTML = '';
        addOnItemDescriptionInfo.style.height = '11px';
      }

      const addOnItemPriceContainer = document.createElement("div");
      addOnItemPriceContainer.classList.add("similar-products-item-price-container");

      const addOnItemPrice = document.createElement("div");
      addOnItemPrice.classList.add("similar-products-item-price");
      addOnItemPrice.innerHTML = `${addOnProductsVariants[i]?.price}`;

      const addOnItemDiscount = document.createElement("div");
      addOnItemDiscount.classList.add("similar-products-item-discount");

      const addOnItemDiscountText = document.createElement("div");
      addOnItemDiscountText.classList.add("similar-products-item-discount-text");
      addOnItemDiscountText.innerHTML = `${Math.round(addOnProductsVariants[i]?.discount)}% OFF`;

      const addOnItemOldPrice = document.createElement("div");
      addOnItemOldPrice.classList.add("similar-products-item-old-price");
      if (addOnProductsVariants[i]?.compareAtPrice === addOnProductsVariants[i]?.price) {
        addOnItemOldPrice.innerHTML = `MRP ${addOnProductsVariants[i]?.compareAtPrice}`;
        addOnItemDiscount.style.display = 'none';
      } else {
        addOnItemOldPrice.innerHTML = `MRP <span style="text-decoration: line-through;">${addOnProductsVariants[i]?.compareAtPrice}</span>`;
        addOnItemDiscountText.style.display = 'block';
      }

      const addOnItemEddContainer = document.createElement("div");
      addOnItemEddContainer.classList.add("similar-products-item-edd-container");
      addOnItemEddContainer.setAttribute("id", `similar-products-item-edd-container-${addOnProductsVariants[i]?.sku}`);

      const addOnItemButton = document.createElement("button");
      addOnItemButton.classList.add("similar-products-item-button");
      addOnItemButton.innerHTML = "Add to cart";
      addOnItemButton.setAttribute("id", `similar-products-item-button-${addOnProductsVariants[i]?.productId}`);

      function handleButtonClick(event) {
        event.stopPropagation();
        const targetAddToCartButton = document.getElementById('similar-products-item-button-' + addOnProductsVariants[i]?.productId);
        recommendationDataArr?.forEach((item) => {
          const defaultVariants = item?.recommendedProducts?.find(product => product?.productId == addOnProductsVariants[i]?.productId);
          addOnProductsAllVariants = [];
          addOnProductsAllVariants.push(defaultVariants);
        });
        
        if(addOnProductsAllVariants[0]?.variants.length > 1){
          if (event.target.id === `similar-products-item-button-${addOnProductsVariants[i]?.productId}`) {
            $('body').css('overflow', 'hidden');
          }
          if(window.innerWidth <= 768){
            addOnItemModalContentMWeb.style.display = "block";
            document.getElementById('mobile-drawer-backdrop').style.display = "block";
          }
          else{
            desktopModalBackdrop.style.display = "block";
            addOnItemModal.style.display = "block";
          }
          dataLayer.push({
            "event": "variant_selection_drawer_viewed",
            "ecommerce": {
              "currency": "INR",
              "value": addOnProductsAllVariants[0]?.variants[0]?.price,
              "items": [
              {
                "item_id": addOnProductsAllVariants[0]?.variants[0]?.variantId,
                "item_name": addOnProductsAllVariants[0]?.variants[0]?.displayName,
                "coupon": "",
                "discount": "",
                "item_brand": addOnProductsAllVariants[0]?.brand,
                "item_category": addOnProductsAllVariants[0]?.category,
                "item_category2": addOnProductsAllVariants[0]?.category2,
                "item_category3": addOnProductsAllVariants[0]?.category3,
                "item_category4": "",
                "item_category5": "",
                "item_list_id": "",
                "item_list_name": "",
                "item_variant": addOnProductsAllVariants[0]?.variants[0]?.displayName,
                "location_id": "",
                "price": addOnProductsAllVariants[0]?.variants[0]?.compareAtPrice,
                "quantity": 1,
              }
              ]
            }
          }); 
          clevertap.event.push("Variant Selection Drawer Viewed", {
            "value": addOnProductsAllVariants[0]?.variants[0]?.price,
            "item_id": addOnProductsAllVariants[0]?.variants[0]?.variantId,
            "item_name": addOnProductsAllVariants[0]?.variants[0]?.displayName,
            "item_brand": addOnProductsAllVariants[0]?.brand,
            "item_variant": addOnProductsAllVariants[0]?.variants[0]?.displayName,
            "item_category": addOnProductsAllVariants[0]?.category,
            "item_category2": addOnProductsAllVariants[0]?.category2,
            "item_category3": addOnProductsAllVariants[0]?.category3,
            "item_category4": "",
            "item_category5": "",
            "item_list_id": "",
            "item_list_name": "",
            "price": addOnProductsAllVariants[0]?.variants[0]?.compareAtPrice,
            "quantity": 1,
            "currency": "INR"
          });
          displayVariantDrawer(addOnProductsAllVariants[0]?.variants, eddData,skuArray, addOnProductsAllVariants[0]?.handle, addOnProductsAllVariants);
          displayVariantDrawerMWeb(addOnProductsAllVariants[0]?.variants, addOnProductsAllVariants[0]?.handle, addOnProductsAllVariants);
        }
        else{
          async function addToCart() {
            const dataLayerObj = {
              "event": "add_to_cart",
              "source": 'PDP recommendation',
              "widget": 'Similar Products',
              "ecommerce": {
              "currency": "INR",
              "value": addOnProductsAllVariants[0]?.variants[0]?.price,
              "items": [
                {
                    "item_id": addOnProductsAllVariants[0]?.variants[0]?.variantId,
                    "item_sku": addOnProductsAllVariants[0]?.variants[0]?.sku,
                    "item_name": addOnProductsAllVariants[0]?.variants[0]?.displayName,
                    "coupon": "",
                    "discount": "",
                    "item_list_id": "",
                    "item_list_name": "",
                    "item_brand": addOnProductsAllVariants[0]?.brand,
                    "item_variant": addOnProductsAllVariants[0]?.variants[0]?.displayName,
                    "location_id": "",
                    "price": addOnProductsAllVariants[0]?.variants[0]?.price,
                    "quantity": 1,
                    "item_category": addOnProductsAllVariants[0]?.category,
                    "item_category3": addOnProductsAllVariants[0]?.category3,
                    "item_category2": addOnProductsAllVariants[0]?.category2,
                    "item_category4": "",
                    "item_category5": "",
                }
              ]
              }
            }
            const clevertapObj = {
              "product_id": addOnProductsAllVariants[0]?.variants[0]?.variantId,
              "product_name": addOnProductsAllVariants[0]?.variants[0]?.displayName,
              "brand": addOnProductsAllVariants[0]?.brand,
              "variant_name": addOnProductsAllVariants[0]?.variants[0]?.displayName,
              "variant_id": addOnProductsAllVariants[0]?.variants[0]?.variantId,
              "price": addOnProductsAllVariants[0]?.variants[0]?.price,
              "mrp": addOnProductsAllVariants[0]?.variants[0]?.compareAtPrice/100,
              "quantity": 1,
              "product_sku": addOnProductsAllVariants[0]?.variants[0]?.sku,
              "product_category1": addOnProductsAllVariants[0]?.category, 
              "product_category3": addOnProductsAllVariants[0]?.category3,
              "product_category2": addOnProductsAllVariants[0]?.category2,
              "product_category4": "",
              "product_category5": "",
              "source": "PDP recommendation",
              "widget": "Similar Products",
            };
            let url = '/cart/add.js';
            const payload = {
              id: addOnProductsAllVariants[0]?.variants[0]?.variantId,
              quantity: 1
            };
            addOnItemButton.classList.add('atc-loader-single-variant');
            addOnItemButton.innerText = '';
            try {
              const res = await fetch(url, {
                method: 'POST',
                headers: {
                  Accept: "application/json",
                  "Content-Type": "application/json"
                },
                body: JSON.stringify(payload)
              });
              const data = await res.json();
              if (data?.status !== 422) {
                dataLayer.push(dataLayerObj);
                clevertap.event.push("Product Added", clevertapObj);
                viewToast();
                updateCartIconNumberSP();
                
              }
            } catch (err) {
              console.log("error", err);
            }
            finally {
              addOnItemButton.classList.remove('atc-loader-single-variant');
              addOnItemButton.innerText = 'Add to Cart';

            }
          }
          addToCart();
        }
      }

      addOnItemButton.addEventListener("click", handleButtonClick);

      itemRating.appendChild(itemRatingValue);
      itemRating.appendChild(itemRatingStar);
      itemRating.appendChild(itemRatingDivider);
      itemRating.appendChild(itemRatingTotal);

      addOnItemImgContainer.appendChild(addOnItemImg);
      addOnItemImgContainer.appendChild(itemRating);
      if(!addOnProductsVariants[i]?.metaRating){
        itemRating.style.display = 'none';
      }

      addOnItemDescriptionContainer.appendChild(addOnItemDescriptionText);
      addOnItemDescriptionContainer.appendChild(addOnItemDescriptionSubText);
      addOnItemDescriptionContainer.appendChild(addOnItemDescriptionInfo);

      addOnItemDiscount.appendChild(addOnItemDiscountText);

      addOnItemPriceContainer.appendChild(addOnItemPrice);
      addOnItemPriceContainer.appendChild(addOnItemDiscount);

      addOnItemCard.appendChild(addOnItemImgContainer);
      addOnItemCard.appendChild(addOnItemDescriptionContainer);
      addOnItemCard.appendChild(addOnItemPriceContainer);
      addOnItemCard.appendChild(addOnItemOldPrice);
      addOnItemCard.appendChild(addOnItemEddContainer);
      addOnItemCard.appendChild(addOnItemButton);

      addOnItems.appendChild(addOnItemCard);

      displayEdd(eddData, addOnProductsVariants[i]?.sku);
      skuMapGlobal = createSkuMap(eddData, addOnProductsVariants[i]?.sku);
    }

    loaderSimilar.style.display = "none";
    widgetHeadingText.style.display = "block";

  }, {once: true});

</script>
