<script>
  window.dataLayer = window.dataLayer || [];
  const onPayOnDelieryClick = (event, title, text) =>{
    const tooltip_style =  document.getElementById('pay-on-delivery-id').style.display
    if(tooltip_style == 'flex'){
      document.getElementById('pay-on-delivery-id').style.display = 'none';
    }else{
      document.getElementById('pay-on-delivery-id').style.display = 'flex';
      if(title == 'fd'){
        title = document.getElementById('free-delivery-text-id').innerHTML;
        }
      document.getElementById("overlay-on-pdp-title-id").innerHTML = title ;
      document.getElementById("overlay-on-pdp-text-id").innerHTML =  text ;

      function getTrustMarkerString() {
        const trustmarkerTitle = document.getElementById('overlay-on-pdp-title-id').innerHTML;
        let trustmarkertxt = '';
        if(trustmarkerTitle.toLocaleLowerCase().includes('599'.toLocaleLowerCase())) {
          trustmarkertxt = 'free_delivery_above_599_clicked';
        } else if(trustmarkerTitle.toLocaleLowerCase().includes('Free delivery'.toLocaleLowerCase())) {
          trustmarkertxt = 'free_delivery_clicked';
        } else if(trustmarkerTitle.toLocaleLowerCase().includes('No returns'.toLocaleLowerCase())) {
          trustmarkertxt = 'no_rnr_clicked';
        } else if(trustmarkerTitle.toLocaleLowerCase().includes('Pay on delivery not available'.toLocaleLowerCase())) {
          trustmarkertxt = 'pay_on_delivery_not_available_clicked';
        } else if(trustmarkerTitle.toLocaleLowerCase().includes('Pay on delivery'.toLocaleLowerCase())) {
          trustmarkertxt = 'pay_on_delivery_clicked';
        } else if(trustmarkerTitle.toLocaleLowerCase().includes('Fresh stock'.toLocaleLowerCase())) {
          trustmarkertxt = 'fresh_stock_clicked';
        } else if(trustmarkerTitle.toLocaleLowerCase().includes('10 days returns'.toLocaleLowerCase())) {
          trustmarkertxt = '10day_rnr_clicked';
        } else if(trustmarkerTitle.toLocaleLowerCase().includes('Only exchange'.toLocaleLowerCase())) {
          trustmarkertxt = 'only_exchange_rnr_clicked';
        }
        return trustmarkertxt;
      }

      dataLayer.push({
      event: "trust_marker_clicked",
      trustmarker: getTrustMarkerString(),
      ecommerce: {
        currency: "INR",
        value: 353.0,
        items: [
          {
            item_id: "7503199142126",
            item_name: "Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food",
            affiliation: "supertails",
            coupon: "",
            discount: "",
            index: 0,
            item_brand: "Pedigree",
            item_category: "Dogs",
            item_category2: "Food",
            item_category3: "Dry Food",
            item_category4: "",
            item_category5: "",
            item_list_id: "",
            item_list_name: "",
            item_variant: "",
            location_id: "",
            price: "",
            quantity: 1
          }
        ]
      }
      });

    }
    event.preventDefault()
  }
  const hideTooltip = (event) =>{
    document.getElementById('pay-on-delivery-id').style.display = 'none';
    event.preventDefault()
  }
  const hideTooltipOnOverlayClick = (event) =>{
    if (event.target.id === "pay-on-delivery-id") {
      document.getElementById('pay-on-delivery-id').style.display = 'none';
    }
    event.preventDefault()
  }
  if(document.getElementById('pay-on-delivery-id-container')){
    document.getElementById('pay-on-delivery-id-container').onclick = (event) => onPayOnDelieryClick(event,'Pay on delivery','You can pay on delivery on all order values between 700 and 5000.');
  }
  if(document.getElementById('fresh-stock-id-container')){
    document.getElementById('fresh-stock-id-container').onclick = (event) => onPayOnDelieryClick(event,'Fresh stock','This product has been freshly procured, and is ready for use.');
  }
  if(document.getElementById('free-delivery-id-container')){
    document.getElementById('free-delivery-id-container').onclick = (event) => onPayOnDelieryClick(event,'fd','We provide free delivery on all orders above 599.');
  }
  if(document.getElementById('pay-on-delivery-not-id-container')){
    document.getElementById('pay-on-delivery-not-id-container').onclick = (event) => onPayOnDelieryClick(event,'Pay on delivery not available','Currently, cash on delivery is not avilable on this product.');
  }

  if(document.getElementById('no-return-id-container')){
    const header = 'AdultAFF6all_all_minus_gnvAnimal:DogsBBLDbest sellerBestCard_OfferBHW ATCBHW POPBLOCKBUSTERBLR120Brand:PedigreeBreed Size:MaxiBreed Size:MediumBreed Size:MiniBudgetCategory Type:FoodClearance SaleCred CollectionDDF POPDELDog Dry FoodDog Dry Food LMDog Food & TreatsDogsDry FoodEARLY ACCESSecoEconomicEOSSFlavor:MultipleFlavor:VegetablesFoodFood/non-food: FoodHello20LDLife stage:AdultLife stage:PuppyMargin BoostMarsMARS_GuwahatiMaxiMediumMiniMultipleMUMno-return-exchangenon_pharmacy_non_dropshipPC-VegPedigreePedigree Dry Food LMPedigree_adultPedigree_puppyPG-ADDF POPPG-PDDF POPProduct Type:Dry FoodProduct Type:Puppy FoodPuppyPuppy FoodPuppy_CornerPuppy_Corner foodRecom FeedSP ACCESSTax:18TOP 250Top 30 DogUpto 50%VegVeg/Non-Veg:VegVegetables'.includes('no-return-exchange') ? 
                   'No returns & exchange' 
                   : 'AdultAFF6all_all_minus_gnvAnimal:DogsBBLDbest sellerBestCard_OfferBHW ATCBHW POPBLOCKBUSTERBLR120Brand:PedigreeBreed Size:MaxiBreed Size:MediumBreed Size:MiniBudgetCategory Type:FoodClearance SaleCred CollectionDDF POPDELDog Dry FoodDog Dry Food LMDog Food & TreatsDogsDry FoodEARLY ACCESSecoEconomicEOSSFlavor:MultipleFlavor:VegetablesFoodFood/non-food: FoodHello20LDLife stage:AdultLife stage:PuppyMargin BoostMarsMARS_GuwahatiMaxiMediumMiniMultipleMUMno-return-exchangenon_pharmacy_non_dropshipPC-VegPedigreePedigree Dry Food LMPedigree_adultPedigree_puppyPG-ADDF POPPG-PDDF POPProduct Type:Dry FoodProduct Type:Puppy FoodPuppyPuppy FoodPuppy_CornerPuppy_Corner foodRecom FeedSP ACCESSTax:18TOP 250Top 30 DogUpto 50%VegVeg/Non-Veg:VegVegetables'.includes('exchange-only') ?
