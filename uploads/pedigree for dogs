    const sizeReconfirmationHeader = document.querySelector('.sizeChartImageDiv-header');

    sizeDrawerModel && sizeDrawerModel.addEventListener('click', function () {
        const display = sizeDrawerModel.style.display;
        if (display === 'none') {
            vid && vid.pause();
        }
    });
    chartTab1 && chartTab1.addEventListener('click', function () {
        vid && vid.pause();
    });
    chartTab2 && chartTab2.addEventListener('click', function () {
        setTimeout(() => {
            vid && vid.play();
        }, 125);
    });
    sizeReconfirmationCross && sizeReconfirmationCross.addEventListener('click', function () {
        vid && vid.pause();
    });
