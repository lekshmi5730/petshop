    <div class="transparent-backdrop"></div>
  
  <script>
    window.dataLayer = window.dataLayer || [];
    document.querySelectorAll('.avatar').forEach(item => {

      item.addEventListener('click', function(e) {
        let spanText = '';
        item?.childNodes?.forEach(node => {
          if(node.tagName === 'SPAN') {
            spanText = node.innerText;
          }
        });
        let trustmarkertxt = '';
        function getTrustMarkerString() {
          if(spanText.includes('599')) {
            trustmarkertxt = 'free_delivery_above_599_clicked';
          } else if(spanText.toLocaleLowerCase().includes('Free Delivery'.toLocaleLowerCase())) {
            trustmarkertxt = 'free_delivery_clicked';
          } else if(spanText.toLocaleLowerCase().includes('No returns'.toLocaleLowerCase())) {
            trustmarkertxt = 'no_rnr_clicked';
          } else if (spanText.toLocaleLowerCase().includes('Pay on delivery not available'.   toLocaleLowerCase())) {
            trustmarkertxt = 'pay_on_delivery_not_available_clicked';
          } else if(spanText.toLocaleLowerCase().includes('Pay on delivery'.toLocaleLowerCase())) {
            trustmarkertxt = 'pay_on_delivery_clicked';
          } else if(spanText.toLocaleLowerCase().includes('Fresh stock'.toLocaleLowerCase())) {
            trustmarkertxt = 'fresh_stock_clicked';
          } else if(spanText.toLocaleLowerCase().includes('10 days returns'.toLocaleLowerCase())) {
            trustmarkertxt = '10day_rnr_clicked';
          } else if(spanText.toLocaleLowerCase().includes('Only exchange'.toLocaleLowerCase())) {
            trustmarkertxt = 'only_exchange_rnr_clicked';
          }
          return trustmarkertxt;
        }

        dataLayer.push({
          event: "trust_marker_clicked",
          trustmarker: getTrustMarkerString(),
          ecommerce: {
            currency: "INR",
            value: 353.0,
            items: [
              {
                item_id: "7503199142126",
                item_name: "Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food",
                affiliation: "supertails",
                coupon: "",
                discount: "",
                index: 0,
                item_brand: "Pedigree",
                item_category: "Dogs",
                item_category2: "Food",
                item_category3: "Dry Food",
                item_category4: "",
                item_category5: "",
                item_list_id: "",
                item_list_name: "",
                item_variant: "",
                location_id: "",
                price: "",
                quantity: 1
              }
            ]
          }
        });
        document.querySelectorAll('.avatar').forEach(item => {
          if (item.classList.contains('clicked') && item !== this) {
            item.classList.remove('clicked');
          }
        });
        this.classList.toggle('clicked');
        if(this.classList.contains('clicked')){
          document.querySelector('.transparent-backdrop').style.display = 'block';
        }
        e.preventDefault();
      });
    });

    document.querySelector('.transparent-backdrop').addEventListener('click', function(e) {
      document.querySelectorAll('.avatar').forEach(item => {
        if (item.classList.contains('clicked')) {
          item.classList.remove('clicked');
        }
      });
      document.querySelector('.transparent-backdrop').style.display = 'none';
      e.preventDefault();
    });

  </script>
  
  
