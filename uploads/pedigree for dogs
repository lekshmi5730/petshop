  var smallestQuantity = 99999;
  jQuery.map( $('.variant-inventory'), function( item, index ) {
    if($(item).data('inventory') < smallestQuantity){
      smallestQuantity = $(item).data('inventory');
    }
    //console.log($(item).data('inventory'))
  });
  setTimeout(function(){
    //console.log(smallestQuantity);
    //console.log('************************');
    $(`.variant-inventory-${smallestQuantity}`).show();
  },2000);
  
</script>                    
