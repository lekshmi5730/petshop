    <script type="text/javascript">
        document.addEventListener('DOMContentLoaded', function() {
          const deviceType = localStorage.getItem('_deviceTypeIL');
          if(!deviceType){
            const userAgent = navigator.userAgent;
            const deviceType = getDeviceType(userAgent);
            localStorage.setItem('_deviceTypeIL', deviceType);

            // Function to determine the type of device
            function getDeviceType(userAgent) {
                // Check for common patterns in the user agent string
                if (/Android/i.test(userAgent)) {
                    return "Mobile (Android)";
                } else if (/iPhone|iPad|iPod/i.test(userAgent)) {
                    return "Mobile (iOS)";
                } else if (/Windows Phone/i.test(userAgent)) {
                    return "Mobile (Windows Phone)";
                } else if (/Windows NT/i.test(userAgent)) {
                    return "Desktop (Windows)";
                } else if (/Macintosh/i.test(userAgent)) {
                    return "Desktop (Mac)";
                } else if (/Linux/i.test(userAgent)) {
                    return "Desktop (Linux)";
                } else {
                    return "Unknown"; // Unable to determine device type
                }
            }
          }

          function pageContentLoaded() {
            const pathName = window.location.pathname;
            if(pathName == '/'){
              return "Home Page";
            }else if(pathName.includes("/collections/")){
              return "Collection Page";
            }else if(pathName.includes("/products/")){
              return "Product Page";
            }else if(pathName.includes("/search/")){
              return "Search Page";
            }else if(pathName.includes("/blogs/")){
              return "Blog Page";
            }else{
              return "Other Page";
            }
          }
          function sendFbEvents() {
            const pathName = window.location.pathname;
            let fbqData = {
                  content_name: document.title,
                  content_type: 'product_group'
                };
            if(pathName.includes("/collections")){
              fbq('track', 'ViewContent', fbqData);
            }else if(pathName.includes("/products")){
                fbqData = {
                  content_name: document.title,
                  content_type: 'product',
                  content_ids: "7503199142126",
                }
                fbq('track', 'ViewContent', fbqData);
            }else if(pathName.includes("/search")){
                fbq('track', 'ViewContent', fbqData);
            }
          }
          if(window.mixpanel){
            mixpanel.track("Page View", {
              "Page URL": window.location.pathname,
              "Title": document.title,
              "Device Type": localStorage.getItem('_deviceTypeIL'),
              "Page Content": pageContentLoaded()
            });
            if(window.fbq){
              sendFbEvents();
            }
          }
      });
    </script>
    
    

    <script>
    // let gb;

function getCpinCookie(cookieName) {
    const name = cookieName + "=";
    const decodedCookie = decodeURIComponent(document.cookie);
    const cookieArray = decodedCookie.split(';');

    for (let i = 0; i < cookieArray.length; i++) {
      let cookie = cookieArray[i].trim();
      if (cookie.indexOf(name) === 0) {
        return cookie.substring(name.length, cookie.length);
      }
    }

    return null;
  }

  async function getPinFromAPI() {
    var requestOptions = {
      method: 'GET',
      redirect: 'follow'
    };
  
    try {
      const response = await fetch("https://api.ipdata.co?api-key=6096980633af7152c073dc67a6f216e3553f759de1c8e02d8b5948b2", requestOptions);
      const result = await response.json();
      if (result.country_code == 'IN') {
        let data1 = {
          city: result.city,
          country_name: result.country_name,
          zip: result.postal,
          region: result.region,
          region_code: result.region_code
        };
        return data1;
      }
    } catch (error) {
      console.log('error', error);
    }
  }
  var cpinIncookies = getCpinCookie('cpin');
