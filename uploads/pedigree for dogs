      //   },7500)

      //   setTimeout(function(){
      //     $('.head-code-pincode-barnds').css({"opacity":"1","visibility": "visible","bottom": "-100%"})
      //   },8000)

      //   setTimeout(function(){
      //     $('.head-code-pincode-barnds').css({"opacity":"0","visibility": "hidden","bottom": "-200%"})
      //   },12000)    
      // }

      // function getCookie(cname) {
      //   var name = cname + "=";
      //   var ca = document.cookie.split(';');
      //   for (var i = 0; i < ca.length; i++) {
      //     var c = ca[i];
      //     while (c.charAt(0) == ' ') {
      //       c = c.substring(1);
      //     }
      //     if (c.indexOf(name) == 0) {
      //       return c.substring(name.length, c.length);
      //     }
      //   }
      //   return "";
      // }

      // function setCookie(cname, cvalue, exdays) {
      //   var d = new Date();
      //   d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
      //   var expires = "expires=" + d.toUTCString();
