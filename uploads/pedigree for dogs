<script>

  const mobilePincodeDiv = document.getElementById('mob_pincode');

  function handleScroll() {
    if (window?.location?.pathname !== '/cart') {
      if (window.scrollY > 0 && window.innerWidth <= 768) {
        mobilePincodeDiv.style.display = 'none'; // Hide the div on mobile scroll
      } else if (window.innerWidth <= 768) {
        mobilePincodeDiv.style.display = 'block'; // Show the div on mobile when scroll is at top
      }
    }
  }

  window.addEventListener('scroll', handleScroll);
  window.addEventListener('resize', handleScroll); // Handle resize events


  function getCpinCookie(cookieName) {
    const name = cookieName + "=";
    const decodedCookie = decodeURIComponent(document.cookie);
    const cookieArray = decodedCookie.split(';');

    for (let i = 0; i < cookieArray.length; i++) {
      let cookie = cookieArray[i].trim();
      if (cookie.indexOf(name) === 0) {
        return cookie.substring(name.length, cookie.length);
      }
    }

    return null;
  }

  var alreaysetcookie = getCpinCookie('cpin');

  if (alreaysetcookie) {
    document.addEventListener('DOMContentLoaded', function () {
      const deliveryPinLocationMobSpan1 = document.getElementsByClassName('delivery_text');

      const pincodeUrl = 'https://promise-engine-371111.el.r.appspot.com/GetPincodeDetails?cpin=' + alreaysetcookie;

      fetch(pincodeUrl)
        .then(response => {
          if (!response.ok) {
            throw new Error(`HTTP error! Status: ${response.status}`);
          }
          return response.json();
        })
        .then(data => {
          Array.from(deliveryPinLocationMobSpan1).forEach(ele => {
            ele.textContent = `${data[0].cPin}, ${data[0].city}`;
          })
          // document.cookie = `cpin=${data[0].cPin}`
          document.cookie = `cpin=${data[0].cPin}; path=/`;
          localStorage.setItem('city', data[0].city);
        })
        .catch(error => {
          console.error('Fetch error123:', error);
        });

    });
  }
</script>

<script>
  document.addEventListener("DOMContentLoaded", function (event) {
    if ($(window).width() <= 767) {
      if (sessionStorage.getItem('notification') == null) {
        sessionStorage.setItem('notification', 'true');
        setTimeout(function () {
          $('.head-code-pincode-coupan').css({ "opacity": "1", "visibility": "visible", "bottom": "-100%" })
          $('.pincode-check-btn').show();
          $('.pincode-check-btn').addClass('active');
        }, 6500)

        setTimeout(function () {
          $('.head-code-pincode-coupan').css({ "opacity": "0", "visibility": "hidden", "bottom": "-200%" })
        }, 9500)

        //setTimeout(function(){
        //$('.head-code-pincode-barnds').css({"opacity":"1","visibility": "visible","bottom": "-100%"})
        //},8000)

        //setTimeout(function(){
        //$('.head-code-pincode-barnds').css({"opacity":"0","visibility": "hidden","bottom": "-200%"})
