      <script>
                      let itemsArray = "";
                      const soldByContainer = document.getElementById('sold-by-container');
                      if(itemsArray){
                      if(Array.isArray(itemsArray)){
                        const resultHtml = itemsArray.map(item => {
                          const formattedName = `${item[0]},`;
                          const location = `${item[1]}${item[2] ? ",":"."}`;
                          const additionalInfo = item[2] ? ` ${item[2]}` : '';
                          return `${formattedName} ${location}${additionalInfo}`;
                        }).join(' OR ');
                        soldByContainer.innerHTML = `<strong><b>Sold by : </b></strong> ${resultHtml}`;  
                      } else {
                        itemsArray = itemsArray.toString().replace(/\\+/g, '');
                      itemsArray = itemsArray.slice(3, -3);
                      soldByContainer.innerHTML = `${itemsArray}`;
                      }
                    }
      </script>

