<script>
    $(document).ready(function(){
            $('#uploadery-container').remove();
        
            // Function to hide elements
            function hideElements() {
                // Hide the banner if it exists
                const banner = document.getElementById("phamacy__banner_outer_IL-id");
                if (banner) banner.style.display = 'none';
        
                // Update CSS of elements if they exist
                const buttons = document.querySelectorAll('.payment-buttons.add-to-cart-sticky1.mobilefull');
                buttons.forEach(button => {
                    button.style.padding = '10px 10px 0px 10px';
                    button.style.backgroundImage = 'none';
                    button.style.borderRadius = '0px';
                });
                const atcOuter = document.querySelectorAll('.add-border-atc');
                atcOuter.forEach(button => {
                    button.style.boxShadow = 'none';
                })
                // Hide the overlay if it exists
                const overlay = document.querySelectorAll('.pharmacy_overlay');
                overlay.forEach(element => {
                    element.style.display = 'none';
                    element.style.zIndex = '-1';
                });
            }
        
            // Add a click event listener to the close button
            $("#pharmacy_close_atc-id").on("click", function(){
                if(window.innerWidth <= 768){
                    hideElements();
                }
            });
        
            // Add scroll event listener to window
            $(window).on("scroll", function() {
                if(window.innerWidth <= 768){
                    hideElements();
                }
            });
        
           // Function to hide overlay after 5 seconds
            

        });        
</script>

