        if (result){
          var result_url = result.clickURL;
          let urlObj = new URL(result_url);
          urlObj.searchParams.set('c', 'homepage_download_banner');
          let updatedUrl = urlObj.toString();

          document.getElementById('brand-app-download-1')?.setAttribute('href', result_url);
          document.getElementById('app-download')?.setAttribute('href', result_url);
          document.getElementById('appdownload-btn-header')?.setAttribute('href', updatedUrl);
          document.getElementById('appdownload-btn-header-1')?.setAttribute('href', result_url);
          
          // if someone has landed on download-app page
          if(window.location.pathname == "/pages/download-app"){
            window.location.href = result_url;
          }
        }
      });
