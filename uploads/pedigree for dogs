<script>
  if(window.innerWidth > 768) {
    //deleting the old header search element which is overridden by the new one
    const oldHeaderSearch = document.getElementById("mainfrm");
    oldHeaderSearch?.remove();
  }
  if(window.innerWidth < 768) {
    let desktopNav = document.querySelector('nav[data-role="desktop-nav"]');
    desktopNav.style.display = "none";
  }
  function buttonClickHandler() {
    const button = document.querySelector('.menuBtn');
    const navMenu = document.querySelector("header > nav");
    button.classList.toggle("closeX");
    if (navMenu.style.display === "block") {
      navMenu.style.display = "none";
    } else {
      navMenu.style.display = "block";
    }
  }

  window.addEventListener('scroll', function () {
    var header = document.querySelector('[data-role="desktop-nav"]');
    const button = document.querySelector('.menuBtn');
    const navMenu = document.querySelector("header > nav");
    button.classList.remove("closeX");

    if (window.scrollY > 0) {
      //header.style.transform = translateY(-100%)
      
      button.style.display = "flex";
      button.style.animation = "fadeIn 200ms linear 1 forwards";
      navMenu.style.display = "none";
    } else {
      
      button.style.animation = "fadeOut 200ms linear 1 forwards"
      navMenu.style.display = "block";
    }
  });

</script>
<header class="header-desktop">
  <div class="header-top_wrapper">
    <button class="menuBtn" onclick="buttonClickHandler()">
      <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-hamburger" viewBox="0 0 64 64">
        <path class="cls-1" d="M7 15h51">.</path>
        <path class="cls-1" d="M7 32h51">.</path>
        <path class="cls-1" d="M7 49h51">.</path>
      </svg>
    </button>
