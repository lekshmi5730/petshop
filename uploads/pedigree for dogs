      $('#refcandy-poprocks').hide();
      $('._hj-widget-container').hide();

//        enableScroll()
      $('body').removeClass('overflow-class');
    }
  });

  function hamburger(x) {
    x.classList.toggle("change");
    document.querySelector('.sidebar').classList.toggle("open");
    $('.has-mobile-sub-menu-2, .has-mobile-sub-menu-2, .mobile-has-submenu').removeClass('active-menu');
    $('.mobile-sub-menu').removeClass('appear');
    $('.mobile-has-submenu, .banner').css('display', 'block');
    $('.has-mobile-sub-menu-2').find('.mobile-sub-menu-2').slideUp();
  }

  var acc = document.getElementsByClassName("data-level-2-dropdown");
  var i;

  for (i = 0; i < acc.length; i++) {
    acc[i].addEventListener("click", function() {

      if (this.classList.contains('active')) { // console.log('already active')
      } else {
        var currentElement = $(".data-level-2-dropdown.active");
        currentElement.removeClass('active');
        if (currentElement.length !== 0) { // $('.slide-nav__dropdown1')
          var heightMain = currentElement.next();

// console.log(heightMain);
          heightMain[0].style.maxHeight = null;

// var nextCurentElement = currentElement.nextElementSibling;
// if(nextCurentElement.style.maxHeight)
// nextCurentElement.style.maxHeight = null;
        }
      }

      this.classList.toggle("active");
      var panel = this.nextElementSibling;
      console.log(panel);
      if (panel.style.maxHeight) {
        panel.style.maxHeight = null;
      } else {
        panel.style.maxHeight = panel.scrollHeight + "px";
      }
    });
  }

  $('.mobile-nav-trigger').on('click', function(e) {
    e.preventDefault();
    $('body').toggleClass('overflow-class');
  });

