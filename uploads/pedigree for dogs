  var startWindows = $(window).scrollTop();
  var startShowingATC = false;  
     
  window.addEventListener('scroll',(event) => {
    if(window.innerWidth < 700 ){
      if(($(window).scrollTop() > startWindows + 25) || ($(window).scrollTop() < startWindows - 25)){
        startShowingATC = true;
      }    
      if ($(this).scrollTop() > 100 && startShowingATC ) {
        $('.add-to-cart-sticky').show();
        //$('.buy-now-sticky').show() 
      } else if($(this).scrollTop() < 100 && startShowingATC) {
        $('.add-to-cart-sticky').hide();
        //$('.buy-now-sticky').hide() 
      }
    }
  });
  
  $(function() {
 

    $('div.buynow-btn').click(function(e){
      var variant_id = $('input[type="radio"]:checked').attr('variant-id');
      var data = {items: [{id: variant_id, quantity: $('input[name="quantity"]').val()}]};
      $.ajax({
        url: "/cart/add",
        type: "post",
        data: data,
        success: function(){
          window.location.href = "/cart";
        },
        error: function(){}
      })
    })
   });

