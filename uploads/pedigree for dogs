    function sendGAEvent(delivery){
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({ ecommerce: null });

        const eventPramaObj = localStorage.getItem("eventPramaObj");

        const item = {
            "item_id": "7503199142126",
            "item_name": "Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food",
            "coupon": "",
            "discount": "",
            "index": 0,
            "item_brand": "Pedigree",
            "item_category": "Dogs",
            "item_category2": "Food",
            "item_category3": "Dry Food",
            "item_category4": "",
            "item_category5": "",
            "item_list_id": "",
            "item_list_name": "",
            "item_variant":getSelectedVariant(),
            "location_id": "",
            "price": 353.0,
            "quantity": 1,
        };

        const eventPayload = {
            "event": "view_item",
            "ecommerce": {
            "currency": "INR",
            "value": 353.0,
            "items": [item]
            }
        }
        if(eventPramaObj){
            const eventPramaObjParsed = JSON.parse(eventPramaObj);
            Object.assign(eventPayload, eventPramaObjParsed);
        }
        dataLayer.push(eventPayload);
    }

    function sendClevertapEvent(delivery){
        const eventPramaObj = localStorage.getItem("eventPramaObj");

        const clevertapPayload = {
            "product_id": "7503199142126",
            "product_name": "Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food",
            "brand": "Pedigree",
            "product_category1": "Dogs",
            "product_category2": "Food",
            "product_category3": "Dry Food",
            "product_category4": "",
            "product_category5": ""
        };

        if(eventPramaObj){
            const eventPramaObjParsed = JSON.parse(eventPramaObj);
            Object.assign(clevertapPayload, eventPramaObjParsed);
        }

        clevertap.event.push("Product Viewed", clevertapPayload);

        localStorage.removeItem("eventPramaObj");
    }

    function sendMixPanelEvent({deliveryDay} = {}){
        const eventPayload = {
                'Product Name': "Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food",
                'Product ID': 7503199142126,
                'Variant ID': 44549440340206,
                'Variant Name': "20kg (2x10kg)",
                'Brand': "Pedigree",
                'Handle': "pedigree-complete-balanced-100-vegetarian-dry-food-for-puppy-adult-dogs",
                'Category L1': "Dogs", 
                'Category L2': "Food", 
                'Category L3': "Dry Food", 
                'Category L4': null, 
                'Category L5': null, 
                'MRP': "5,600",
                'Selling Price': "4,760",
                'Rating Count': "35", 
                'Rating': "4.89", 
                'Product Sku': null,
                'City' : localStorage.getItem('city'),
                "Device Type": localStorage.getItem('_deviceTypeIL'),
                "Page Content": 'Product Page',
                "Delivery Time": 'N/A',
                "Device Type": localStorage.getItem('_deviceTypeIL'),
        }
        if(window.mixpanel && deliveryDay){
                mixpanel.track("Product Viewed",{...eventPayload,"Delivery Time": deliveryDay || ''});
        }else if(window.mixpanel){
            mixpanel.track("Product Viewed",eventPayload);
        }
    }
}
   
