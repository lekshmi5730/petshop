      // }
    }
  })
  $(window).on('load', function () {
    $('.site-header__search-input').on('input', function () {
        var txt_s = $(this).val();
        // Remove any potential HTML tags to prevent XSS attacks
        txt_s = txt_s.replace(/<[^>]*>?/gm, '');
        // Allow only alphanumeric characters, spaces, apostrophes, underscores, and commas
        txt_s = txt_s.replace(/[^a-zA-Z0-9\s'_,]/g, '');
        $(this).val(txt_s);
        if (txt_s.length > 0) {
            $(this).parents('form').find('.site-header__search-btn').addClass('active');
        } else {
            $(this).parents('form').find('.site-header__search-btn').removeClass('active');
        }
    });
  });
</script>





