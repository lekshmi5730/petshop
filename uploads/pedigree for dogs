            chartTab1.classList.remove('active');
            tab1img.style.display='none';
            chartTab2.classList.add('active');
            tab2img.style.display='flex';
            otherEvents('view_how_to_measure');
        });
    }
    if (chartTab2 && chartTab1) {
        chartTab1.addEventListener('click', function () {
            chartTab1.classList.add('active');
            tab1img.style.display='flex';
            chartTab2.classList.remove('active');
            tab2img.style.display='none';
            otherEvents('view_size_chart');
        });
    }
