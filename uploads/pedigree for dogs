<script>
  
  async function fetchEdd(skuArr) {
    let pincode = getCpinCookie('cpin');
    let url = `https://promise-engine-371111.el.r.appspot.com/edd?cpin=${pincode}&skuid=${skuArr.join(
      ","
    )}&qty=1`;

    const options = {
      method: 'GET',
      headers: {
        "Content-Type": "application/json",
      }
    };

    try {
      const response = await fetch(url, options);
      if(response.ok) {
        const data = await response.json();
        return data;
      }

    } catch(err) {
      console.log("error", err);
      throw error;
    }

  }
  async function checkItemAlreadyInCartRecommendationsPOV(selected_id){
      updatedCartItemsArr1 = await getCartPOV();
      const cartItemsPOV = updatedCartItemsArr1;
      const itemAlreadyInCartPOV = cartItemsPOV?.items?.find(item => item.id == selected_id);
      if(itemAlreadyInCartPOV){
          return itemAlreadyInCartPOV;
      }return false
  }

  function createSkuMapPOV(data) {
    const skuMapPOV = {};
    data.forEach(item => {
      const { skuId, ...rest } = item;
      skuMapPOV[skuId] = rest;
    });
    return skuMapPOV;
  }
  
  function displayEddPOV(eddData, skuId) {
    const data = eddData;
    const result = createSkuMapPOV(data);

    const elementPOV = document.getElementById(`people-also-viewed-item-edd-container-${skuId}`);
    if (result[skuId] === undefined) {
      elementPOV.innerHTML = '';
      elementPOV.style.height = "42px";
      elementPOV.classList.remove("linear-gradient-yellow");
      return;
    }

    if(result[skuId]?.is2HourDelivery) {
      elementPOV.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_36.png?v=1726039250" class="people-also-viewed-item-edd-icon"/>
      <p class="people-also-viewed-item-edd-text"><b style="font-weight: 900;">Superfast</b> <b>${result[skuId]?.message}</b></p>`;
      elementPOV.classList.add("linear-gradient-yellow");
    } else if(result[skuId]?.dayCount == 0 || result[skuId]?.dayCount == 1) {
      elementPOV.innerHTML = `<img src="https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_34.png?v=1726038501" class="people-also-viewed-item-edd-icon"/>
      <p class="people-also-viewed-item-edd-text"><b style="font-weight: 900;">Fast</b> <b>${result[skuId]?.message}</b></p>`;
      elementPOV.classList.add("linear-gradient-yellow");
    } else {
      elementPOV.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_35.png?v=1726039019" class="people-also-viewed-item-edd-icon"/>
      <p class="people-also-viewed-item-edd-text"><b>${result[skuId]?.message}</b></p>`;
      elementPOV.classList.remove("linear-gradient-yellow");
    }
  }
  function displayEddInDrawerMWebPOV(skuId){
    const eddVisibleMWebPOV = document.querySelector('.people-also-viewed-item-modal-edd-mweb');
    if (skuMapGlobalPOV[skuId] === undefined) {
      eddVisibleMWebPOV.style.display = "none";
      return;
    }
    if(skuMapGlobalPOV[skuId]?.is2HourDelivery) {
      eddVisibleMWebPOV.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_36.png?v=1726039250" class="people-also-viewed-item-edd-icon-quickAdd"/>
      <p class="people-also-viewed-item-edd-text-quickAdd"><b style="font-weight: 900;">Superfast</b> <b>${skuMapGlobalPOV[skuId]?.message}</b></p>`;
      eddVisibleMWebPOV.classList.add("linear-gradient-yellow");
    } else if(skuMapGlobalPOV[skuId]?.dayCount == 0 || skuMapGlobalPOV[skuId]?.dayCount == 1) {
      eddVisibleMWebPOV.innerHTML = `<img src="https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_34.png?v=1726038501" class="people-also-viewed-item-edd-icon-quickAdd"/>
      <p class="people-also-viewed-item-edd-text-quickAdd"><b style="font-weight: 900;">Fast</b> <b>${skuMapGlobalPOV[skuId]?.message}</b></p>`;
      eddVisibleMWebPOV.classList.add("linear-gradient-yellow");
    } else {
      eddVisibleMWebPOV.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_35.png?v=1726039019" class="people-also-viewed-item-edd-icon-quickAdd"/>
      <p class="people-also-viewed-item-edd-text-quickAdd"><b>${skuMapGlobalPOV[skuId]?.message}</b></p>`;
      eddVisibleMWebPOV.classList.remove("linear-gradient-yellow");
    }
  }

  function displayEddInDrawerPOV(skuId){
    const eddVisible = document.getElementById('people-also-viewed-item-modal-edd');
    if (skuMapGlobalPOV[skuId] === undefined) {
      eddVisible.innerHTML = '';
      eddVisible.style.margin = "unset";
      return;
    }
    if(skuMapGlobalPOV[skuId]?.is2HourDelivery) {
      eddVisible.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_36.png?v=1726039250" class="people-also-viewed-item-edd-icon-quickAdd"/>
      <p class="people-also-viewed-item-edd-text-quickAdd"><b style="font-weight: 900;">Superfast</b> <b>${skuMapGlobalPOV[skuId]?.message}</b></p>`;
      eddVisible.classList.add("linear-gradient-yellow");
    } else if(skuMapGlobalPOV[skuId]?.dayCount == 0 || skuMapGlobalPOV[skuId]?.dayCount == 1) {
      eddVisible.innerHTML = `<img src="https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_34.png?v=1726038501" class="people-also-viewed-item-edd-icon-quickAdd"/>
      <p class="people-also-viewed-item-edd-text-quickAdd"><b style="font-weight: 900;">Fast</b> <b>${skuMapGlobalPOV[skuId]?.message}</b></p>`;
      eddVisible.classList.add("linear-gradient-yellow");
    } else {
      eddVisible.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_35.png?v=1726039019" class="people-also-viewed-item-edd-icon-quickAdd"/>
      <p class="people-also-viewed-item-edd-text-quickAdd"><b>${skuMapGlobalPOV[skuId]?.message}</b></p>`;
      eddVisible.classList.remove("linear-gradient-yellow");
    }
  }

  async function displayVariantDrawerPOV(variantsArr, skuArray, eddData, handle, productDetails) {
    const result_sku = createSkuMapPOV(eddData);
    const sortedVariantsArr = variantsArr.sort((a, b) => {
      return (a.isDefault === b.isDefault) ? 0 : a.isDefault ? -1 : 1;
    });
    function handleIncreaseQuantityPOV(event) {
      changeCartQuantityDesktop(selectedVariant?.variantId, true, desktopAddToCartStepper);
    }
    function handleDecreaseQuantityPOV(event) {
      changeCartQuantityDesktop(selectedVariant?.variantId, false, desktopAddToCartStepper, desktopAddToCart);
    }
    
    let selectedVariant = sortedVariantsArr.find(variant => variant.isDefault);

    const addOnItemModal = document.getElementById("people-also-viewed-item-modal");
    const addOnItemModalContent = document.getElementById("people-also-viewed-item-modal-content");
    const desktopModalBackdrop = document.getElementById("people-also-viewed-item-modal-backdrop");


    // Create modal elements (simplified for brevity)
    const createModalElementPOV = (tag, className, innerHTML = '', src = '', id = '') => {
      const elementPOV = document.createElement(tag);
      if (className) elementPOV.classList.add(className);
      if (innerHTML) elementPOV.innerHTML = innerHTML;
      if (src) elementPOV.setAttribute('href', src);
      if (src) elementPOV.src = src;
      if (id) elementPOV.setAttribute('id', id);
      return elementPOV;
    };

    // Modal Header
    const addOnItemModalHeader = createModalElementPOV('div', 'people-also-viewed-item-modal-header');
    addOnItemModalHeader.appendChild(createModalElementPOV('div', 'people-also-viewed-item-modal-header-text', 'Quick Add'));
    const addOnItemModalHeaderClose = createModalElementPOV('img', 'people-also-viewed-item-modal-header-close', '', 'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/mingcute_close-line.png?v=1725521249');
    addOnItemModalHeaderClose.alt = 'close-icon';
    addOnItemModalHeader.appendChild(addOnItemModalHeaderClose);

    // Image Container
    const addOnItemModalImageHandler = createModalElementPOV('a', '', '', `/products/${handle}?variant=${selectedVariant?.variantId}`);
    addOnItemModalImageHandler.appendChild(createModalElementPOV('img', 'people-also-viewed-item-modal-image', '', selectedVariant?.imageUrl));
    const addOnItemModalImageContainer = createModalElementPOV('div', 'people-also-viewed-item-modal-image-container');
    addOnItemModalImageContainer.appendChild(addOnItemModalImageHandler);

    // Description
    const addOnItemModalDescriptionText = createModalElementPOV('div', 'people-also-viewed-item-modal-description-text', selectedVariant?.displayName);
    const addOnItemModalDescriptionArrow = createModalElementPOV('img', 'people-also-viewed-item-modal-description-arrow', '', 'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/ArrowRight.png?v=1725523530');
    addOnItemModalDescriptionArrow.alt = 'arrow-right-icon';
    const arrowHandler = createModalElementPOV('a', '', '', `/products/${handle}?variant=${selectedVariant?.variantId}`);
    arrowHandler.appendChild(addOnItemModalDescriptionArrow);
    const addOnItemModalDescription = createModalElementPOV('div', 'people-also-viewed-item-modal-description');
    addOnItemModalDescription.appendChild(addOnItemModalDescriptionText);
    addOnItemModalDescription.appendChild(arrowHandler);

    arrowHandler.addEventListener("click", function(event) {
      event.preventDefault();
      let eventPramaObj = {
        "source": "PDP recs",
        "widget": "People Also Viewed",
      };
      localStorage.setItem('eventPramaObj', JSON.stringify(eventPramaObj));
      window.location.replace(arrowHandler.href);
    });

    // Price Container
    const addOnItemModalPrice = createModalElementPOV('div', 'people-also-viewed-item-modal-price', `${selectedVariant?.price}`);
    const addOnItemModalPriceContainer = createModalElementPOV('div', 'people-also-viewed-item-modal-price-container');
    addOnItemModalPriceContainer.appendChild(addOnItemModalPrice);

    const addOnItemModalMrp = createModalElementPOV(
      'div',
      'people-also-viewed-item-modal-mrp',
      selectedVariant?.compareAtPrice === selectedVariant?.price
      ? `MRP ${selectedVariant?.compareAtPrice}`
      : `MRP <span style="text-decoration: line-through;">${selectedVariant?.compareAtPrice}</span>`
    );
    addOnItemModalPriceContainer.appendChild(addOnItemModalMrp);

    const addOnItemModalSaveText = createModalElementPOV(
      'div',
      'people-also-viewed-item-modal-save-text',
      `Save ${Math.round(selectedVariant?.discount)}%`
    );
    addOnItemModalPriceContainer.appendChild(addOnItemModalSaveText);
    if (selectedVariant?.compareAtPrice === selectedVariant?.price) {
      addOnItemModalSaveText.style.display = 'none';
    } else {
      addOnItemModalSaveText.style.display = 'block';
    }
    
    // Other Elements
    let addOnItemModalSubText;
    if(selectedVariant?.pricePerHundred){
      addOnItemModalSubText = createModalElementPOV('div', 'people-also-viewed-item-modal-sub-text', `(${selectedVariant?.pricePerHundred}/100g)`);
    }
    else{
      addOnItemModalSubText = createModalElementPOV('div', 'people-also-viewed-item-modal-sub-text', '');
    }
    const addOnItemModalEddLoader = createModalElementPOV('div', 'loader_edd_recs');
    const addOnItemModalEddIcon = createModalElementPOV('img', 'people-also-viewed-item-modal-edd-icon', '', '');
    addOnItemModalEddIcon.alt = 'edd-modal-icon';
    const addOnItemModalEddText = createModalElementPOV('div', 'people-also-viewed-item-modal-edd-text', '');
    const addOnItemModalEdd = createModalElementPOV('div', 'people-also-viewed-item-modal-edd', '', 'people-also-viewed-item-modal-edd');
    addOnItemModalEdd.setAttribute('id', 'people-also-viewed-item-modal-edd');
    addOnItemModalEdd.appendChild(addOnItemModalEddLoader);
    // addOnItemModalEdd.appendChild(addOnItemModalEddIcon);
    // addOnItemModalEdd.appendChild(addOnItemModalEddText);

    //Update the EDD by default
  

    // Variant Bubbles
    const addOnItemModalVariantContainer = createModalElementPOV('div', 'people-also-viewed-item-modal-variant-container', '', '', 'people-also-viewed-item-modal-variant-container');
    addOnItemModalVariantContainer.setAttribute('id', 'people-also-viewed-item-modal-variant-container');
    let lowStockDiv = '';
    let isLowStock = false;
    sortedVariantsArr.forEach(variant => {
      let isVisible = false;
      if (variant.inventoryQuantity < 5) {
        isLowStock = true;
        isVisible = true;
        lowStockDiv = `
          <div class="low-stock-alert-pov">
            <p style="font-family: Lato; font-size: 12px; font-weight: 500; text-align: center; margin: 0;">
              ${variant.inventoryQuantity} units left
            </p>
          </div>
        `;
      }
      const variantBubble = createModalElementPOV('button', 'people-also-viewed-item-modal-variant-bubble', variant.title);
      variantBubble.setAttribute('id', `variant-bubble-${variant.id}`);
      variantBubble.dataset.id = variant.id;
      if (variant.id === selectedVariant?.id) {
        variantBubble.classList.add('active');
      }
      variantBubble.addEventListener('click', function () {
        const variantId = this.dataset.id;
        selectedVariant = variantsArr.find(v => v.id === variantId);
        updateModalContentPOV(selectedVariant, desktopAddToCart, desktopAddToCartStepper, increaseQuantityPOV, decreaseQuantity, handleIncreaseQuantityPOV, handleIncreaseQuantityPOV);
        displayEddInDrawerPOV(selectedVariant?.sku);
      });
      const variantContainer = createModalElementPOV('div', 'variant-container');
      if(isVisible) {
        variantContainer.innerHTML = lowStockDiv; // Insert low stock alert if applicable
      }
      else{
        variantContainer.innerHTML = '';
      }
      const variantBox = createModalElementPOV('div', 'variant-box');
      variantBox.setAttribute('id', `variant-box-${variant.id}`);
      variantBox.dataset.id = variant.id;

      // Append the variant bubble to the variant box
      variantBox.appendChild(variantBubble);

      // Append the variant box to the container
      variantContainer.appendChild(variantBox);

      // Append the container to the modal variant container
      addOnItemModalVariantContainer.appendChild(variantContainer);
    });
  
    //Add to Cart Button
    const desktopAddToCart = document.getElementById('people-also-viewed-item-modal-button-desktop');
    const desktopAddToCartStepper = document.getElementById('people-also-viewed-item-modal-button-container-desktop');
    const increaseQuantityPOV = document.getElementById('people-also-viewed-item-modal-add-button-desktop')
    const decreaseQuantity = document.getElementById('people-also-viewed-item-modal-minus-button-desktop');

    
    

    addOnItemModalContent.innerHTML = '';
    addOnItemModalContent.appendChild(addOnItemModalHeader);
    addOnItemModalContent.appendChild(addOnItemModalImageContainer);
    addOnItemModalContent.appendChild(addOnItemModalDescription);
    addOnItemModalContent.appendChild(addOnItemModalPriceContainer);
    if(addOnItemModalSubText){
      addOnItemModalContent.appendChild(addOnItemModalSubText);
    }
    addOnItemModalContent.appendChild(addOnItemModalEdd);
    addOnItemModalContent.appendChild(addOnItemModalVariantContainer);
    addOnItemModalContent.appendChild(desktopAddToCart);
    addOnItemModalContent.appendChild(desktopAddToCartStepper);

    let defaultVariantQuantity = await checkItemAlreadyInCartRecommendationsPOV(selectedVariant?.variantId);
    if(defaultVariantQuantity?.quantity){
      desktopAddToCartStepper.style.display = 'flex';
      desktopAddToCart.style.display = 'none';
      desktopAddToCartStepper.children[0].children[1].children[0].innerHTML = defaultVariantQuantity?.quantity;
      if (!increaseQuantityPOV.hasAttribute('dirty-POV')) {
        increaseQuantityPOV.addEventListener('click', handleIncreaseQuantityPOV);
        increaseQuantityPOV.setAttribute('dirty-POV', 'true');
      }
      if (!decreaseQuantity.hasAttribute('dirty-POV')) {
        decreaseQuantity.addEventListener('click', handleDecreaseQuantityPOV);
        decreaseQuantity.setAttribute('dirty-POV', 'true');
      }
      desktopAddToCartStepper.children[1].addEventListener("click", handleCartRedirect);
    }
    else{
      desktopAddToCartStepper.style.display = 'none';
      increaseQuantityPOV.removeEventListener('click', changeCartQuantityDesktopPOV);
      decreaseQuantity.removeEventListener('click', changeCartQuantityDesktopPOV);
      desktopAddToCart.style.display = 'flex';
    }
    
    displayEddInDrawerPOV(selectedVariant?.sku);
    let variantDetailsQuickAddPOV = document.getElementById('people-also-viewed-item-modal-variant-container');
    if(isLowStock){
      variantDetailsQuickAddPOV.style.marginTop = '30px';
      variantDetailsQuickAddPOV.style.rowGap = '30px';
    }

    desktopAddToCart.addEventListener("click", async function() {
      const dataLayerObj = {
        "event": "add_to_cart",
        "source": 'PDP recommendation',
        "widget": 'People Also Viewed',
        "ecommerce": {
        "currency": "INR",
        "value": selectedVariant?.price,
        "items": [
          {
              "item_id": selectedVariant?.variantId,
              "item_sku": selectedVariant?.sku,
              "item_name": selectedVariant?.displayName,
              "coupon": "",
              "discount": "",
              "item_list_id": "",
              "item_list_name": "",
              "item_variant": selectedVariant?.displayName,
              "location_id": "",
              "price": selectedVariant?.price,
              "quantity": 1,
              "item_category": productDetails[0]?.category,
              "item_category3": productDetails[0]?.category3,
              "item_category2": productDetails[0]?.category2,
              "item_category4": "",
              "item_category5": "",
          }
        ]
        }
      }
      const clevertapObj = {
        "product_id": selectedVariant?.variantId,
        "product_name": selectedVariant?.displayName,
        "brand": "",
        "variant_name": selectedVariant?.displayName,
        "variant_id": selectedVariant?.variantId,
        "price": selectedVariant?.price,
        "mrp": selectedVariant?.compareAtPrice/100,
        "quantity": 1,
        "product_sku": selectedVariant?.sku,
        "product_category1": "", 
        "product_category3": "",
        "product_category2": "",
        "product_category4": "",
        "product_category5": "",
        "source": "PDP recommendation",
        "widget": "People Also Viewed"
      };
      
      let url = '/cart/add.js';
      const payload = {
        id: selectedVariant?.variantId,
        quantity: 1
      };
      try {
        const res = await fetch(url, {
          method: 'POST',
          headers: {
            Accept: "application/json",
            "Content-Type": "application/json"
          },
          body: JSON.stringify(payload)
        });
        desktopAddToCart.classList.add('atc-loader');
        desktopAddToCart.innerHTML = '';
        const data = await res.json();
        if(data?.status !== 422) {
          desktopAddToCart.style.display = 'none';
          desktopAddToCartStepper.style.display = 'flex';
          desktopAddToCartStepper.children[0].children[1].children[0].innerHTML = '1';
          desktopAddToCartStepper.children[1].addEventListener("click", handleCartRedirect);
          if (!increaseQuantityPOV.hasAttribute('dirty-POV')) {
            increaseQuantityPOV.addEventListener('click', handleIncreaseQuantityPOV);
            increaseQuantityPOV.setAttribute('dirty-POV', 'true');
          }
          if (!decreaseQuantity.hasAttribute('dirty-POV')) {
            decreaseQuantity.addEventListener('click', handleDecreaseQuantityPOV);
            decreaseQuantity.setAttribute('dirty-POV', 'true');
          }
          dataLayer.push(dataLayerObj);
          clevertap.event.push("Product Added", clevertapObj);
          updateCartIconNumberPOV();
        }
      } catch(err) {
        console.log("error", err);
      }
      finally {
        desktopAddToCart.classList.remove('atc-loader');
        desktopAddToCart.innerHTML = 'Add to cart';
      }
    });


    // Close Modal
    addOnItemModalHeaderClose.addEventListener('click', function () {
      addOnItemModal.style.display = 'none';
      desktopModalBackdrop.style.display = 'none';
      document.body.style.overflow = 'auto';
    });
  }

  async function updateModalContentPOV(variant, desktopAddToCart, desktopAddToCartStepper, increaseQuantityPOV, decreaseQuantity, handleIncreaseQuantityPOV, handleDecreaseQuantityPOV) {

    let selected_variant_check = await checkItemAlreadyInCartRecommendationsPOV(variant?.variantId)
    const addToCart = document.getElementById('people-also-viewed-item-modal-button');
    const addOnItemModalContent = document.getElementById("people-also-viewed-item-modal-content");

    function handleIncreaseQuantityVariantPOV(event) {
      changeCartQuantityDesktopPOV(variant?.variantId, true, desktopAddToCartStepper);
    }
    function handleDecreaseQuantityVariantPOV(event) {
      changeCartQuantityDesktopPOV(variant?.variantId, false, desktopAddToCartStepper, desktopAddToCart);
    }
    

    document.querySelector('.people-also-viewed-item-modal-image').src = variant.imageUrl;
    document.querySelector('.people-also-viewed-item-modal-description-text').innerHTML = variant.displayName;
    document.querySelector('.people-also-viewed-item-modal-price').innerHTML = `${variant.price}`;
    if(variant?.pricePerHundred) {
      document.querySelector('.people-also-viewed-item-modal-sub-text').innerHTML = `(${variant.pricePerHundred}/100g)`;  
    }
    else{
      document.querySelector('.people-also-viewed-item-modal-sub-text').innerHTML = ``;
    }

    document.querySelector('.people-also-viewed-item-modal-mrp').innerHTML = 
      variant?.compareAtPrice === variant?.price 
        ? `MRP ${variant.compareAtPrice}`
        : `MRP <span style="text-decoration: line-through;">${variant.compareAtPrice}</span>`;

    // Set the Save Text with conditional display
    document.querySelector('.people-also-viewed-item-modal-save-text').innerHTML = `Save ${Math.round(variant.discount)}%`;
    document.querySelector('.people-also-viewed-item-modal-save-text').style.display = 
      variant?.compareAtPrice === variant?.price ? 'none' : 'block';
    
    const allBubbles = document.querySelectorAll('.people-also-viewed-item-modal-variant-bubble');
    allBubbles.forEach(bubble => bubble.classList.remove('active'));

    // Remove the `#` from the ID
    const selectedBubble = document.getElementById(`variant-bubble-${variant.id}`);
    if (selectedBubble) {
      selectedBubble.classList.add('active');
    } else {
      console.error(`No elementPOV found with ID variant-bubble-${variant.id}`);
    }
    if(selected_variant_check?.quantity){
      desktopAddToCartStepper.style.display = 'flex';
      desktopAddToCart.style.display = 'none';
      desktopAddToCartStepper.children[0].children[1].children[0].innerHTML = selected_variant_check?.quantity;
      if (!increaseQuantityPOV.hasAttribute('dirty-POV')) {
          increaseQuantityPOV.addEventListener('click', handleIncreaseQuantityVariantPOV);
          increaseQuantityPOV.setAttribute('dirty-POV', 'true');
        }
      if (!decreaseQuantity.hasAttribute('dirty-POV')) {
        decreaseQuantity.addEventListener('click', handleDecreaseQuantityVariantPOV);
        decreaseQuantity.setAttribute('dirty-POV', 'true');
      }
    }
    else{
      desktopAddToCartStepper.style.display = 'none';
      desktopAddToCart.style.display = 'flex';
    }
  }
  function handleCartRedirect() {
    window.location.href = '/cart';
  }

  async function displayVariantDrawerMWebPOV(variantsArr, handle, productDetails) {
    const sortedVariantsArr = variantsArr.sort((a, b) => {
      return (a.isDefault === b.isDefault) ? 0 : a.isDefault ? -1 : 1;
    });
    
    function handleIncreaseQuantityPOV(event) {
      changeCartQuantityPOV(selectedVariant?.variantId, true, mobileAddToCartStepper);
    }
    function handleDecreaseQuantityPOV(event) {
      changeCartQuantityPOV(selectedVariant?.variantId, false, mobileAddToCartStepper, mobileAddToCart);
    }
    let selectedVariant = sortedVariantsArr.find(variant => variant.isDefault);

    const mobileDrawer = document.getElementById("mobile-drawer-container-POV");
    const mobileDrawerBackdrop = document.getElementById("mobile-drawer-backdrop-POV");
    
    function handleBackdropClick(event) {
      if(event.target === addOnItemModal) {
        mobileDrawer.style.display = 'none';
        document.body.style.overflow = 'auto';
        mobileDrawerBackdrop.style.display = 'none';
      }
    }
    mobileDrawerBackdrop.addEventListener("click", handleBackdropClick);

    // Create modal elements (simplified for brevity)
    const createDrawerElementPOV = (tag, className, innerHTML = '', src = '', href = '', id = '') => {
      const elementPOV = document.createElement(tag);
      if (className) elementPOV.classList.add(className);
      if (innerHTML) elementPOV.innerHTML = innerHTML;
      if (src) elementPOV.src = src;
      if (id) elementPOV.setAttribute('id', id);
      if (href && tag === 'a') elementPOV.setAttribute('href', href); 
      return elementPOV;
    };

    // Drawer Header
    const drawerHeader = createDrawerElementPOV('div', 'mobile-drawer-header');
    const drawerImage = createDrawerElementPOV('div', 'mobile-drawer-image');
    const drawerImageImg = createDrawerElementPOV('img', '', '', selectedVariant?.imageUrl);
    drawerImageImg.alt = 'Product Image';
    drawerImageImg.width = 70;
    drawerImageImg.height = 70;
    drawerImage.appendChild(drawerImageImg);
    
    // Drawer Details
    const drawerDetails = createDrawerElementPOV('div', 'mobile-drawer-details');
    const detailsHeader = createDrawerElementPOV('div', 'mobile-drawer-details-header');
    const drawerTitle = createDrawerElementPOV('p', 'mobile-drawer-details-title', selectedVariant?.displayName);
    const arrowButton = createDrawerElementPOV('a', '', '','', `/products/${handle}?variant=${selectedVariant?.variantId}`);
    const arrowImg = createDrawerElementPOV('img', '', '', 'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/ArrowRight.png?v=1725866116');
    arrowImg.width = 13;
    arrowImg.height = 16;
    arrowButton.appendChild(arrowImg);
    detailsHeader.appendChild(drawerTitle);
    detailsHeader.appendChild(arrowButton);


    arrowButton.addEventListener("click", function(event) {
      event.preventDefault();
      let eventPramaObj = {
        "source": "PDP recs",
        "widget": "People Also Viewed",
      };
      localStorage.setItem('eventPramaObj', JSON.stringify(eventPramaObj));
      window.location.replace(arrowButton.href);
    });

    
    // Price Details
    const priceDetails = createDrawerElementPOV('div', 'mobile-drawer-priceDetails');
    const priceContainer = createDrawerElementPOV('div', 'mobile-drawer-price');
    const priceValue = createDrawerElementPOV('p', 'mobile-drawer-price-value', `${selectedVariant?.price}`);

    const priceMrp = createDrawerElementPOV(
      'p',
      'mobile-drawer-price-mrp',
      selectedVariant?.compareAtPrice === selectedVariant?.price
        ? `MRP ${selectedVariant?.compareAtPrice}`
        : `MRP <span style="text-decoration: line-through;">${selectedVariant?.compareAtPrice}</span>`
    );
    const priceDiscount = createDrawerElementPOV(
      'p',
      'mobile-drawer-price-discount',
      `SAVE ${Math.round(selectedVariant?.discount)}%`
    );

    if (selectedVariant?.compareAtPrice === selectedVariant?.price) {
      priceDiscount.style.display = 'none';
    } else {
      priceDiscount.style.display = 'block';
    }

    priceContainer.appendChild(priceValue);
    priceContainer.appendChild(priceMrp);
    priceContainer.appendChild(priceDiscount);
    

    let unitPrice;
    if(selectedVariant?.pricePerHundred){
      unitPrice = createDrawerElementPOV('p', 'mobile-drawer-unit-price-value', `(${selectedVariant?.pricePerHundred}/100g)`);
    }
    else{
      unitPrice = createDrawerElementPOV('p', 'mobile-drawer-unit-price-value', ``);
    }

    //Other Details 

    const addOnItemModalEddLoader1 = createDrawerElementPOV('div', 'loader_edd_rec1' );

    const addOnItemModalEddIcon = createDrawerElementPOV('img', 'people-also-viewed-item-modal-edd-icon-mweb', '', '');
    addOnItemModalEddIcon.alt = 'edd-modal-icon';
    const addOnItemModalEddText = createDrawerElementPOV('div', 'people-also-viewed-item-modal-edd-text-mweb', '');
    const addOnItemModalEdd = createDrawerElementPOV('div', 'people-also-viewed-item-modal-edd-mweb', '', 'people-also-viewed-item-modal-edd-mweb');
    
    addOnItemModalEdd.appendChild(addOnItemModalEddLoader1);
    
    
    
    // Append everything
    priceDetails.appendChild(priceContainer);
    if(unitPrice){
      priceDetails.appendChild(unitPrice);
    }
    priceDetails.appendChild(addOnItemModalEdd);
    drawerDetails.appendChild(detailsHeader);
    drawerDetails.appendChild(priceDetails);

    drawerHeader.appendChild(drawerImage);
    drawerHeader.appendChild(drawerDetails);

    const crossIcon1 = createDrawerElementPOV('img', 'mobile-drawer-cross-icon-1', '', 'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/closeIcon.png?v=1726478114', '', 'mobile-drawer-cross-icon-1');
    crossIcon1.addEventListener('click', function () {
      mobileDrawer.style.display = 'none';
      document.body.style.overflow = 'auto';
      document.getElementById('mobile-drawer-backdrop-POV').style.display = 'none';
    })

    // Variant Bubbles
    const variantDetails = createDrawerElementPOV('div', 'variant-details-1', '', '','','variant-details-1');
    let lowStockDiv = '';
    let isLowStock = false;
    sortedVariantsArr.forEach(variant => {
      let isVisible = false;
      if (variant.inventoryQuantity < 5) {
        isLowStock = true;
        isVisible = true;
        lowStockDiv = `
          <div class="low-stock-alert-pov">
            <p style="font-family: Lato; font-size: 12px; font-weight: 500; text-align: center; margin: 0;">
              ${variant.inventoryQuantity} units left
            </p>
          </div>
        `;
      }

      // Create the variant bubble
      const variantBubble = createDrawerElementPOV('button', 'mobile-drawer-variant-bubble', variant.title);
      variantBubble.setAttribute('id', `mobile-drawer-variant-bubble-${variant.id}`);
      variantBubble.dataset.id = variant.id;
      if (variant.id === selectedVariant?.id) {
        variantBubble.classList.add('active');
      }
      variantBubble.addEventListener('click', function () {
        const variantId = this.dataset.id;
        selectedVariant = variantsArr.find(v => v.id === variantId);
        updateModalContentMWebPOV(selectedVariant, mobileAddToCart, mobileAddToCartStepper, handleIncreaseQuantityPOV, handleDecreaseQuantityPOV, increaseQuantityPOV, decreaseQuantity);
        displayEddInDrawerMWebPOV(selectedVariant?.sku);
      });

      // Create the variant container with the optional lowStockDiv
      const variantContainer = createDrawerElementPOV('div', 'variant-container');
      if(isVisible) {
        variantContainer.innerHTML = lowStockDiv; // Insert low stock alert if applicable
      }
      else{
        variantContainer.innerHTML = '';
      }

      // Create the variant box
      const variantBox = createDrawerElementPOV('div', 'variant-box');
      variantBox.setAttribute('id', `variant-box-${variant.id}`);
      variantBox.dataset.id = variant.id;
      const variantTitle = createDrawerElementPOV('p', 'variant-title', variant.title);
      variantBox.appendChild(variantTitle);

      // Append the variant box to the container
      variantContainer.appendChild(variantBubble);

      // Append the container to variant details
      variantDetails.appendChild(variantContainer);
    });

    //Add to cart button

    const mobileAddToCart = document.getElementById('people-also-viewed-item-modal-button-mweb');
    const mobileAddToCartStepper = document.getElementById('people-also-viewed-item-modal-button-container-mweb');
    const increaseQuantityPOV = document.getElementById('people-also-viewed-item-modal-add-button-mweb');
    const decreaseQuantity = document.getElementById('people-also-viewed-item-modal-minus-button-mweb');



    // Clear drawer content and append new elements
    const seperator = createDrawerElementPOV('hr', 'mobile-separator', '', '');
    mobileDrawer.innerHTML = '';
    mobileDrawer.appendChild(drawerHeader);
    mobileDrawer.appendChild(crossIcon1);
    mobileDrawer.appendChild(createDrawerElementPOV('hr', 'mobile-divider', '', ''));
    mobileDrawer.appendChild(variantDetails);
    mobileDrawer.appendChild(mobileAddToCart);
    mobileDrawer.appendChild(mobileAddToCartStepper);
    mobileDrawer.insertBefore(seperator, mobileAddToCart);
    
    let defaultVariantQuantity = await checkItemAlreadyInCartRecommendationsPOV(selectedVariant?.variantId);
    if(defaultVariantQuantity?.quantity){
      mobileAddToCartStepper.style.display = 'flex';
      mobileAddToCart.style.display = 'none';
      mobileAddToCartStepper.children[0].children[1].children[0].innerHTML = defaultVariantQuantity?.quantity;
      if (!increaseQuantityPOV.hasAttribute('dirty-POV')) {
        increaseQuantityPOV.addEventListener('click', handleIncreaseQuantityPOV);
        increaseQuantityPOV.setAttribute('dirty-POV', 'true');
      }
      if (!decreaseQuantity.hasAttribute('dirty-POV')) {
        decreaseQuantity.addEventListener('click', handleDecreaseQuantityPOV);
        decreaseQuantity.setAttribute('dirty-POV', 'true');
      }
      mobileAddToCartStepper.children[1].addEventListener("click", handleCartRedirect);
    }
    else{
      mobileAddToCartStepper.style.display = 'none';
      increaseQuantityPOV.removeEventListener('click', changeCartQuantityPOV);
      decreaseQuantity.removeEventListener('click', changeCartQuantityPOV);
      mobileAddToCart.style.display = 'flex';
    }


    let crossIconContainer1 = document.getElementById('mobile-drawer-cross-icon-1');
    
    mobileAddToCart.addEventListener("click", async function() {
      const dataLayerObj = {
        "event": "add_to_cart",
        "source": 'PDP recommendation',
        "widget": 'People Also Viewed',
        "ecommerce": {
        "currency": "INR",
        "value": selectedVariant?.price,
        "items": [
          {
              "item_id": selectedVariant?.variantId,
              "item_sku": selectedVariant?.sku,
              "item_name": selectedVariant?.displayName,
              "coupon": "",
              "discount": "",
              "item_list_id": "",
              "item_list_name": "",
              "item_variant": selectedVariant?.displayName,
              "location_id": "",
              "price": selectedVariant?.price,
              "quantity": 1,
              "item_category": productDetails[0]?.category,
              "item_category3": productDetails[0]?.category3,
              "item_category2": productDetails[0]?.category2,
              "item_category4": "",
              "item_category5": "",
          }
        ]
        }
      }
      const clevertapObj = {
        "product_id": selectedVariant?.variantId,
        "product_name": selectedVariant?.displayName,
        "brand": "",
        "variant_name": selectedVariant?.displayName,
        "variant_id": selectedVariant?.variantId,
        "price": selectedVariant?.price,
        "mrp": selectedVariant?.compareAtPrice/100,
        "quantity": 1,
        "product_sku": selectedVariant?.sku,
        "product_category1": "", 
        "product_category3": "",
        "product_category2": "",
        "product_category4": "",
        "product_category5": "",
        "source": 'PDP recommendation',
        "widget": 'People Also Viewed'
      };
      
      let url = '/cart/add.js';
      const payload = {
        id: selectedVariant?.variantId,
        quantity: 1
      };
      try {
        const res = await fetch(url, {
          method: 'POST',
          headers: {
            Accept: "application/json",
            "Content-Type": "application/json"
          },
          body: JSON.stringify(payload)
        });
        mobileAddToCart.classList.add('atc-loader-mweb')
        mobileAddToCart.innerHTML =  '';
        const data = await res.json();
        if(data?.status !== 422) {
          mobileAddToCart.style.display = 'none';
          mobileAddToCartStepper.style.display = 'flex';
          mobileAddToCartStepper.children[0].children[1].children[0].innerHTML = '1';
          mobileAddToCartStepper.children[1].addEventListener("click", handleCartRedirect);
          if (!increaseQuantityPOV.hasAttribute('dirty-POV')) {
            increaseQuantityPOV.addEventListener('click', handleIncreaseQuantityPOV);
            increaseQuantityPOV.setAttribute('dirty-POV', 'true');
          }
          if (!decreaseQuantity.hasAttribute('dirty-POV')) {
            decreaseQuantity.addEventListener('click', handleDecreaseQuantityPOV);
            decreaseQuantity.setAttribute('dirty-POV', 'true');
          }
          dataLayer.push(dataLayerObj);
          clevertap.event.push("Product Added", clevertapObj);
          updateCartIconNumberPOV();
        }
      } catch(err) {
        console.log("error", err);
      }
      finally{
        mobileAddToCart.innerHTML = 'Add to cart'
        mobileAddToCart.classList.remove('atc-loader-mweb')
      }
    });
    displayEddInDrawerMWebPOV(selectedVariant?.sku);
    let variantDetails1 = document.getElementById('variant-details-1');
    if(isLowStock){
      variantDetails1.style.marginTop = '18%';
      variantDetails1.style.rowGap = '40px';
    }
    crossIconContainer1.style.bottom = mobileDrawer.clientHeight + 7 + 'px';

  }
  async function changeCartQuantityDesktopPOV(selectedVariantId, isAdd, desktopAddToCartStepper, desktopAddToCart) {
    let url = '/cart/change.js';
    const itemAlreadyInCartPOV =  await checkItemAlreadyInCartRecommendationsPOV(selectedVariantId);
    const updateQuantityPOV = itemAlreadyInCartPOV ? itemAlreadyInCartPOV.quantity + (isAdd ? 1 : -1) : 1;
    const payload = {
      id: selectedVariantId,
      quantity: updateQuantityPOV
    };
    try {
      const res = await fetch(url, {
        method: 'POST',
        headers: {
          Accept: "application/json",
          "Content-Type": "application/json"
        },
        body: JSON.stringify(payload)
      });
      const data = await res.json();
      if(data?.status !== 422) {
        desktopAddToCartStepper.children[0].children[1].children[0].innerHTML = updateQuantityPOV;
        if(updateQuantityPOV ===0) {
          desktopAddToCart.style.display = 'flex';
          desktopAddToCartStepper.style.display = 'none';
        }
        updateCartIconNumberPOV();
      }
    } catch(err) {
      console.log("error", err);
    }
  }

  async function changeCartQuantityPOV(selectedVariantId, isAdd, mobileAddToCartStepper, mobileAddToCart) {
    let url = '/cart/change.js';
    const itemAlreadyInCartPOV =  await checkItemAlreadyInCartRecommendationsPOV(selectedVariantId);
    const updateQuantityPOV = itemAlreadyInCartPOV ? itemAlreadyInCartPOV.quantity + (isAdd ? 1 : -1) : 1;
    const payload = {
      id: selectedVariantId,
      quantity: updateQuantityPOV
    };
    try {
      const res = await fetch(url, {
        method: 'POST',
        headers: {
          Accept: "application/json",
          "Content-Type": "application/json"
        },
        body: JSON.stringify(payload)
      });
      const data = await res.json();
      if(data?.status !== 422) {
        mobileAddToCartStepper.children[0].children[1].children[0].innerHTML = updateQuantityPOV;
        if(updateQuantityPOV ===0) {
          mobileAddToCart.style.display = 'flex';
          mobileAddToCartStepper.style.display = 'none';
        }
        updateCartIconNumberPOV();
      }
    } catch(err) {
      console.log("error", err);
    }
  }

  async function updateModalContentMWebPOV(variant, mobileAddToCart, mobileAddToCartStepper, handleIncreaseQuantityPOV, handleDecreaseQuantityPOV, increaseQuantityPOV, decreaseQuantity) {
      // Update image
      document.querySelector('.mobile-drawer-image img').src = variant.imageUrl;

      // Update product title
      document.querySelector('.mobile-drawer-details-title').innerHTML = variant.displayName;
      function handleIncreaseQuantityVariantPOV(event) {
        changeCartQuantityPOV(variant?.variantId, true, mobileAddToCartStepper);
      }
      function handleDecreaseQuantityVariantPOV(event) {
        changeCartQuantityPOV(variant?.variantId, false, mobileAddToCartStepper, mobileAddToCart);
      }

      // Update price, MRP, and discount
      document.querySelector('.mobile-drawer-price-value').innerHTML = `${variant.price}`;
      let defaultVariantQuantity = await checkItemAlreadyInCartRecommendationsPOV(variant?.variantId);
      
      document.querySelector('.mobile-drawer-price-mrp').innerHTML = 
      variant?.price === variant?.compareAtPrice 
        ? `MRP ${variant.compareAtPrice}`
        : `MRP <span style="text-decoration: line-through;">${variant.compareAtPrice}</span>`;

      document.querySelector('.mobile-drawer-price-discount').innerHTML = `SAVE ${Math.round(variant.discount)}%`;
      document.querySelector('.mobile-drawer-price-discount').style.display = variant?.price === variant?.compareAtPrice ? 'none' : 'block';

      // Update price per unit
      if(variant?.pricePerHundred){
        document.querySelector('.mobile-drawer-unit-price-value').innerHTML = `(${variant.pricePerHundred}/100g)`;
      }
      else{
        document.querySelector('.mobile-drawer-unit-price-value').innerHTML = '';
      }

      // Deselect all variant bubbles
      const allBubbles = document.querySelectorAll('.mobile-drawer-variant-bubble');
      allBubbles.forEach(bubble => bubble.classList.remove('active'));

      // Set the selected variant bubble to active
      const selectedBubble = document.getElementById(`mobile-drawer-variant-bubble-${variant.id}`);
      if (selectedBubble) {
          selectedBubble.classList.add('active');
      } else {
          console.error(`No elementPOV found with ID variant-bubble-${variant.id}`);
      }

      if(defaultVariantQuantity?.quantity){
        mobileAddToCartStepper.style.display = 'flex';
        mobileAddToCart.style.display = 'none';
        mobileAddToCartStepper.children[0].children[1].children[0].innerHTML = defaultVariantQuantity?.quantity;
        if (!increaseQuantityPOV.hasAttribute('dirty-POV')) {
            increaseQuantityPOV.addEventListener('click', handleIncreaseQuantityVariantPOV);
            increaseQuantityPOV.setAttribute('dirty-POV', 'true');
          }
        if (!decreaseQuantity.hasAttribute('dirty-POV')) {
          decreaseQuantity.addEventListener('click', handleDecreaseQuantityVariantPOV);
          decreaseQuantity.setAttribute('dirty-POV', 'true');
        }
      }
      else{
        mobileAddToCartStepper.style.display = 'none';
        mobileAddToCart.style.display = 'flex';
      }

  }

  let peopleAlsoViewedAllVariants = [];

  let  updatedCartItemsArr1 = {"note":null,"attributes":{},"original_total_price":0,"total_price":0,"total_discount":0,"total_weight":0.0,"item_count":0,"items":[],"requires_shipping":false,"currency":"INR","items_subtotal_price":0,"cart_level_discount_applications":[],"checkout_charge_amount":0}; 

  async function getCartPOV() {
    const url = '/cart.js';
    const res = await fetch(url);
    const data = await res.json();
    return data;
  }

  async function updateCartIconNumberPOV() {
    updatedCartItemsArr1 = await getCartPOV();
    const CartBubbles = document.querySelectorAll('cart-link__bubble');
    const cartIcons = document.querySelectorAll(".cart-link__bubble-num");
    if (CartBubbles.length) {
      CartBubbles.forEach(elementPOV => {
        showElement(elementPOV);
      })
    }
    if (cartIcons.length) {
      cartIcons.forEach(elementPOV => {
        elementPOV.textContent = updatedCartItemsArr1?.item_count;
      })
    }
  }
  async function viewToastPOV() {
      const successMessageDiv = `
        <div class="supertails_shipping_addmore-brij" id="supertails_shipping_addmore-brij-recommendations-POV">
          <div style="display: flex; gap: 10px; justify-content:center; align-items:center;">
            <div style="display: flex; flex-direction:column; justify-content:center; align-items:flex-start;">
              <div class="sticky-atc-pdp-header-message" style="display: flex; justify-content: left; align-items: center; gap: 4px; color: black; font-size: 12px; height: 48px;">
                <img src="https://cdn.shopify.com/s/files/1/0565/8021/0861/files/sticky-atc-pdp-min.gif?v=1692697712" alt="twinkle" style="width:44px;height:44px">
                <div style="width: 144px;">
                  <div class="cart_itemadded" style="text-align:left;"> 1 Item added</div>
                  <div class="sticky-atc-pdp-footer-message" style="line-height: 15px; color: white; font-size:12px; text-align:left;"></div>
                </div>
              </div>
            </div>
            <a style="color:white" href="/cart">
              <div class="atc-btn" style="display: flex; justify-content: center; align-items: center; font-size: 12px; border: 1px solid white; padding: 10px 12px; border-radius: 6px; color: white; height: min-content; font-weight:bold; min-width:125px;">
                <svg style="margin-right:4px;" width="20" height="18" viewBox="0 0 20 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" clip-rule="evenodd" d="M0.176071 0.184241C-0.0887979 0.446838 -0.051706 0.830741 0.26626 1.11592C0.472184 1.3007 0.925431 1.37364 2.07463 1.40659L3.60813 1.45069L5.23952 7.55406L6.87105 13.6574L12.2871 13.6976C17.994 13.7398 18.3672 13.7012 18.2769 13.0787C18.1813 12.4209 17.7493 12.365 12.736 12.3628C10.1381 12.3615 7.98194 12.3089 7.94469 12.246C7.90729 12.1832 7.82849 11.9084 7.76939 11.6357L7.66212 11.1398H12.4265C16.0553 11.1398 17.2268 11.0943 17.3412 10.9491C17.56 10.6715 20.0014 4.39328 20 4.11191C19.9994 3.98115 19.9007 3.74083 19.7805 3.57787C19.5714 3.29421 19.2659 3.2785 12.5183 3.20541L5.47422 3.12912L5.06022 1.70017C4.83259 0.914205 4.55126 0.211554 4.43506 0.138466C4.10047 -0.07149 0.393999 -0.0318188 0.176071 0.184241ZM8.53307 14.7072C7.53792 15.1502 7.13115 16.1732 7.59594 17.0643C8.29344 18.4016 10.3414 18.2463 10.7713 16.8236C11.1861 15.4514 9.8017 14.1425 8.53307 14.7072ZM14.8162 14.7174C13.9019 15.1152 13.5074 16.1313 13.9271 17.008C14.6849 18.5914 16.9876 18.1895 17.1866 16.4392C17.2573 15.8173 17.2154 15.7004 16.7465 15.2114C16.1433 14.5823 15.5035 14.4185 14.8162 14.7174Z" fill="white"/>
                </svg>
                GO TO CART
              </div>
            </a>
          </div>
        </div>`;
      const toastContainer = document.createElement('div');
      toastContainer.innerHTML = successMessageDiv;
      const parentElement = document.getElementById('HeaderCart');
      while (parentElement.firstChild) {
        parentElement.removeChild(parentElement.firstChild);
      }
      parentElement.appendChild(toastContainer.firstElementChild);
      parentElement.style.display = 'flex';
      parentElement.style.justifyContent = 'center';
      parentElement.style.alignItems = 'center';
      parentElement.style.width = '350px';
      parentElement.style.height = '60px';
      $('.cart_itemadded').css('font-size', '16px');
      parentElement.style.transform = 'translateY(0px)';
      parentElement.style.opacity = '1';
      if(window.innerWidth < 768) {
        parentElement.style.marginTop = '4px';
      }
      else{
        parentElement.style.marginTop = '0px';
      }
      parentElement.style.transition = 'opacity 0.3s ease 0.15s, transform 0.25s cubic-bezier(0.165, 0.84, 0.44, 1) 0.15s';
      parentElement.classList.add('is-active')
      setTimeout(() => {
        parentElement.classList.remove('is-active');
    }, 1500);
  } 

  document.addEventListener("DOMContentLoaded", async function () {
    const widgetHeadingText = document.getElementById("people-also-viewed-heading-text");
    const addOnItems = document.getElementById("people-also-viewed-items");
    const addOnItemModal = document.getElementById("people-also-viewed-item-modal");
    const addOnItemModalContent = document.getElementById("people-also-viewed-item-modal-content");
    const addOnItemModalContentMWeb = document.getElementById("mobile-drawer-container-POV");
    const desktopModalBackdrop = document.getElementById("people-also-viewed-item-modal-backdrop");
    const loaderPeopleViewed = document.getElementById("people-also-viewed-loader");
    widgetHeadingText.style.display = "none";

    function handleBackdropClick(event) {
      if(event.target === addOnItemModal) {
        desktopModalBackdrop.style.display = "none";
        addOnItemModal.style.display = "none";
        document.body.style.overflow = 'auto';
      }
    }
    addOnItemModal.addEventListener("click", handleBackdropClick);


    const mobileDrawerBackdrop = document.getElementById("mobile-drawer-backdrop-POV")
    const mobileDrawer = document.getElementById("mobile-drawer-container-POV");
    
    function handleBackdropClickMWeb(event) {
      if(event.target === mobileDrawerBackdrop) {
        mobileDrawer.style.display = 'none';
        document.body.style.overflow = 'auto';
        mobileDrawerBackdrop.style.display = 'none';
      }
    }
    mobileDrawerBackdrop.addEventListener("click", handleBackdropClickMWeb);

    let addOnProducts = [];
    let addOnProductsVariants = [];
    let skuArray = [];
    let featureFlagDataPOV =  await fetchFeatureFlagsPOV();

    let recommendationDataArr = await getPdpRecommendations();
    recommendationDataArr = [recommendationDataArr[2]];
    recommendationDataArr?.forEach((data) => {
      widgetHeadingText.innerHTML = data?.title;
      addOnProducts.push(...data?.recommendedProducts);
    });
    
    if(addOnProducts?.length === 0 || !featureFlagDataPOV.visible) {
      loaderPeopleViewed.style.display = "none";
      document.getElementById('people-also-viewed').style.display = 'none';
      return;
    }

    addOnProducts?.forEach((product) => {
      const defaultVariant = product?.variants?.find(variant => variant?.isDefault);
      const addOnsSku = product?.variants?.map(variant => variant?.sku);
      skuArray.push(...addOnsSku);
      const variantWithDetailsPOV = {
        ...defaultVariant, 
        handle: product?.handle,
        totalVariants: product?.totalVariants,
        productId: product?.productId
      };
      addOnProductsVariants.push(variantWithDetailsPOV);
    });

    let eddData = await fetchEdd(skuArray);

    for (let i = 0; i < addOnProductsVariants?.length; i++) {
      const addOnItemCard = document.createElement("div");
      addOnItemCard.classList.add("people-also-viewed-item-card");
      addOnItemCard.setAttribute("id", `people-also-viewed-item-card-${addOnProductsVariants[i]?.sku}`);
      addOnItemCard.addEventListener("click", function(event) {
        event.preventDefault();
        let eventPramaObj = {
          "source": "PDP recs",
          "widget": "Similar Products",
        };
        localStorage.setItem('eventPramaObj', JSON.stringify(eventPramaObj));
        window.location.replace(`/products/${addOnProductsVariants[i]?.handle}?variant=${addOnProductsVariants[i]?.variantId}`);
      });
      const addOnItemImgContainer = document.createElement("div");
      addOnItemImgContainer.classList.add("people-also-viewed-item-img-container");

      const addOnItemImg = document.createElement("img");
      addOnItemImg.classList.add("people-also-viewed-item-img");
      addOnItemImg.setAttribute("id", `people-also-viewed-item-img-${addOnProductsVariants[i]?.sku}`);
      addOnItemImg.src = addOnProductsVariants[i]?.imageUrl;

      const itemRating = document.createElement("div");
      itemRating.classList.add("people-also-viewed-item-rating");

      const itemRatingValue = document.createElement("div");
      itemRatingValue.classList.add("people-also-viewed-item-rating-value");
      itemRatingValue.innerHTML = `${addOnProductsVariants[i]?.metaRating}`;

      const itemRatingStar = document.createElement("img");
      itemRatingStar.classList.add("people-also-viewed-item-rating-star");
      itemRatingStar.src = "https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_32.png?v=1725429108";
      itemRatingStar.alt = 'rating-star';

      const itemRatingDivider = document.createElement("div");
      itemRatingDivider.classList.add("people-also-viewed-item-rating-divider");

      const itemRatingTotal = document.createElement("div");
      itemRatingTotal.classList.add("people-also-viewed-item-rating-total");
      itemRatingTotal.innerHTML = `${addOnProductsVariants[i]?.metaRatingCount}`;

      const addOnItemDescriptionContainer = document.createElement("div");
      addOnItemDescriptionContainer.classList.add("people-also-viewed-item-description-container");

      const addOnItemDescriptionText = document.createElement("div");
      addOnItemDescriptionText.classList.add("people-also-viewed-item-description-text");
      addOnItemDescriptionText.innerHTML = addOnProductsVariants[i]?.displayName;

      const addOnItemDescriptionSubText = document.createElement("div");
      addOnItemDescriptionSubText.classList.add("people-also-viewed-item-description-sub-text");
      addOnItemDescriptionSubText.innerHTML = addOnProductsVariants[i]?.title;

      const addOnItemDescriptionInfo = document.createElement("div");
      addOnItemDescriptionInfo.classList.add("people-also-viewed-item-description-info");
      if(addOnProductsVariants[i]?.totalVariants > 1){
        addOnItemDescriptionInfo.innerHTML = `(${addOnProductsVariants[i]?.totalVariants} sizes available)`;
      }
      else{
        addOnItemDescriptionInfo.innerHTML = '';
        addOnItemDescriptionInfo.style.height = '11px';
      }

      const addOnItemPriceContainer = document.createElement("div");
      addOnItemPriceContainer.classList.add("people-also-viewed-item-price-container");

      const addOnItemPrice = document.createElement("div");
      addOnItemPrice.classList.add("people-also-viewed-item-price");
      addOnItemPrice.innerHTML = `${addOnProductsVariants[i]?.price}`;

      const addOnItemDiscount = document.createElement("div");
      addOnItemDiscount.classList.add("people-also-viewed-item-discount");

      const addOnItemDiscountText = document.createElement("div");
      addOnItemDiscountText.classList.add("people-also-viewed-item-discount-text");
      addOnItemDiscountText.innerHTML = `${Math.round(addOnProductsVariants[i]?.discount)}% OFF`;

      const addOnItemOldPrice = document.createElement("div");
      addOnItemOldPrice.classList.add("people-also-viewed-item-old-price");
      if (addOnProductsVariants[i]?.compareAtPrice === addOnProductsVariants[i]?.price) {
        addOnItemOldPrice.innerHTML = `MRP ${addOnProductsVariants[i]?.compareAtPrice}`;
        addOnItemDiscount.style.display = 'none';
      } else {
        addOnItemOldPrice.innerHTML = `MRP <span style="text-decoration: line-through;">${addOnProductsVariants[i]?.compareAtPrice}</span>`;
        addOnItemDiscountText.style.display = 'block';
      }

      const addOnItemEddContainer = document.createElement("div");
      addOnItemEddContainer.classList.add("people-also-viewed-item-edd-container");
      addOnItemEddContainer.setAttribute("id", `people-also-viewed-item-edd-container-${addOnProductsVariants[i]?.sku}`);

      const addOnItemButton = document.createElement("button");
      addOnItemButton.classList.add("people-also-viewed-item-button");
      addOnItemButton.innerHTML = "Add to cart";
      addOnItemButton.setAttribute("id", `people-also-viewed-item-button-${addOnProductsVariants[i]?.productId}`);

      function handleButtonClick(event) {
        event.stopPropagation();
        recommendationDataArr?.forEach((item) => {
          const defaultVariants = item?.recommendedProducts?.find(product => product?.productId == addOnProductsVariants[i]?.productId);
          peopleAlsoViewedAllVariants = [];
          peopleAlsoViewedAllVariants.push(defaultVariants);
        });
        if(peopleAlsoViewedAllVariants[0]?.variants.length > 1){
          if (event.target.id === `people-also-viewed-item-button-${addOnProductsVariants[i]?.productId}`) {
            $('body').css('overflow', 'hidden');
          }
          if(window.innerWidth <= 768){
            addOnItemModalContentMWeb.style.display = "block";
            document.getElementById('mobile-drawer-backdrop-POV').style.display = "block";
          }
          else{
            desktopModalBackdrop.style.display = "block";
            addOnItemModal.style.display = "block";
          }
          dataLayer.push({
            "event": "variant_selection_drawer_viewed",
            "ecommerce": {
              "currency": "INR",
              "value": peopleAlsoViewedAllVariants[0]?.variants[0]?.price,
              "items": [
              {
                "item_id": peopleAlsoViewedAllVariants[0]?.variants[0]?.variantId,
                "item_name": peopleAlsoViewedAllVariants[0]?.variants[0]?.displayName,
                "coupon": "",
                "discount": "",
                "item_brand": peopleAlsoViewedAllVariants[0]?.brand,
                "item_category": peopleAlsoViewedAllVariants[0]?.category,
                "item_category2": peopleAlsoViewedAllVariants[0]?.category2,
                "item_category3": peopleAlsoViewedAllVariants[0]?.category3,
                "item_category4": "",
                "item_category5": "",
                "item_list_id": "",
                "item_list_name": "",
                "item_variant": peopleAlsoViewedAllVariants[0]?.variants[0]?.displayName,
                "location_id": "",
                "price": peopleAlsoViewedAllVariants[0]?.variants[0]?.compareAtPrice,
                "quantity": 1,
              }
              ]
            }
          }); 
          clevertap.event.push("Variant Selection Drawer Viewed", {
            "value": peopleAlsoViewedAllVariants[0]?.variants[0]?.price,
            "item_id": peopleAlsoViewedAllVariants[0]?.variants[0]?.variantId,
            "item_name": peopleAlsoViewedAllVariants[0]?.variants[0]?.displayName,
            "item_brand": peopleAlsoViewedAllVariants[0]?.brand,
            "item_variant": peopleAlsoViewedAllVariants[0]?.variants[0]?.displayName,
            "item_category": peopleAlsoViewedAllVariants[0]?.category,
            "item_category2": peopleAlsoViewedAllVariants[0]?.category2,
            "item_category3": peopleAlsoViewedAllVariants[0]?.category3,
            "item_category4": "",
            "item_category5": "",
            "item_list_id": "",
            "item_list_name": "",
            "price": peopleAlsoViewedAllVariants[0]?.variants[0]?.compareAtPrice,
            "quantity": 1,
            "currency": "INR"
          });
          displayVariantDrawerPOV(peopleAlsoViewedAllVariants[0]?.variants, eddData,skuArray, peopleAlsoViewedAllVariants[0]?.handle, peopleAlsoViewedAllVariants);
          displayVariantDrawerMWebPOV(peopleAlsoViewedAllVariants[0]?.variants, peopleAlsoViewedAllVariants[0]?.handle, peopleAlsoViewedAllVariants);
        }
        else{
          async function addToCart() {
            const dataLayerObj = {
              "event": "add_to_cart",
              "source": 'PDP recommendation',
              "widget": 'People Also Viewed',
              "ecommerce": {
              "currency": "INR",
              "value": peopleAlsoViewedAllVariants[0]?.variants[0]?.price,
              "items": [
                {
                    "item_id": peopleAlsoViewedAllVariants[0]?.variants[0]?.variantId,
                    "item_sku": peopleAlsoViewedAllVariants[0]?.variants[0]?.sku,
                    "item_name": peopleAlsoViewedAllVariants[0]?.variants[0]?.displayName,
                    "coupon": "",
                    "discount": "",
                    "item_list_id": "",
                    "item_list_name": "",
                    "item_variant": peopleAlsoViewedAllVariants[0]?.variants[0]?.displayName,
                    "location_id": "",
                    "price": peopleAlsoViewedAllVariants[0]?.variants[0]?.price,
                    "quantity": 1,
                    "item_category": peopleAlsoViewedAllVariants[0]?.category,
                    "item_category3": peopleAlsoViewedAllVariants[0]?.category3,
                    "item_category2": peopleAlsoViewedAllVariants[0]?.category2,
                    "item_category4": "",
                    "item_category5": "",
                }
              ]
              }
            }
            const clevertapObj = {
              "product_id": peopleAlsoViewedAllVariants[0]?.variants[0]?.variantId,
              "product_name": peopleAlsoViewedAllVariants[0]?.variants[0]?.displayName,
              "brand": "",
              "variant_name": peopleAlsoViewedAllVariants[0]?.variants[0]?.displayName,
              "variant_id": peopleAlsoViewedAllVariants[0]?.variants[0]?.variantId,
              "price": peopleAlsoViewedAllVariants[0]?.variants[0]?.price,
              "mrp": peopleAlsoViewedAllVariants[0]?.variants[0]?.compareAtPrice/100,
              "quantity": 1,
              "product_sku": peopleAlsoViewedAllVariants[0]?.variants[0]?.sku,
              "product_category1": "", 
              "product_category3": "",
              "product_category2": "",
              "product_category4": "",
              "product_category5": "",
              "source": "PDP recommendation",
              "widget": "People Also Viewed",
            };
            
            let url = '/cart/add.js';
            const payload = {
              id: peopleAlsoViewedAllVariants[0]?.variants[0]?.variantId,
              quantity: 1
            };
            addOnItemButton.classList.add('atc-loader-single-variant');
            addOnItemButton.innerText = '';
            try {
              const res = await fetch(url, {
                method: 'POST',
                headers: {
                  Accept: "application/json",
                  "Content-Type": "application/json"
                },
                body: JSON.stringify(payload)
              });
              const data = await res.json();
              if (data?.status !== 422) {
                dataLayer.push(dataLayerObj);
                clevertap.event.push("Product Added", clevertapObj);
                viewToastPOV();
                updateCartIconNumberPOV();
              }
            } catch (err) {
              console.log("error", err);
            }
            finally {
              addOnItemButton.classList.remove('atc-loader-single-variant');
              addOnItemButton.innerText = 'Add to cart';
            }
          }
          addToCart();
        } 
      }

      addOnItemButton.addEventListener("click", handleButtonClick);

      itemRating.appendChild(itemRatingValue);
      itemRating.appendChild(itemRatingStar);
      itemRating.appendChild(itemRatingDivider);
      itemRating.appendChild(itemRatingTotal);

      addOnItemImgContainer.appendChild(addOnItemImg);
      addOnItemImgContainer.appendChild(itemRating);

      if(!addOnProductsVariants[i]?.metaRating){
        itemRating.style.display = 'none';
      }


      addOnItemDescriptionContainer.appendChild(addOnItemDescriptionText);
      addOnItemDescriptionContainer.appendChild(addOnItemDescriptionSubText);
      addOnItemDescriptionContainer.appendChild(addOnItemDescriptionInfo);

      addOnItemDiscount.appendChild(addOnItemDiscountText);

      addOnItemPriceContainer.appendChild(addOnItemPrice);
      addOnItemPriceContainer.appendChild(addOnItemDiscount);

      addOnItemCard.appendChild(addOnItemImgContainer);
      addOnItemCard.appendChild(addOnItemDescriptionContainer);
      addOnItemCard.appendChild(addOnItemPriceContainer);
      addOnItemCard.appendChild(addOnItemOldPrice);
      addOnItemCard.appendChild(addOnItemEddContainer);
      addOnItemCard.appendChild(addOnItemButton);

      addOnItems.appendChild(addOnItemCard);

      displayEddPOV(eddData, addOnProductsVariants[i]?.sku);
      skuMapGlobalPOV = createSkuMapPOV(eddData, addOnProductsVariants[i]?.sku);
    }

    loaderPeopleViewed.style.display = "none";
    widgetHeadingText.style.display = "block";

  }, {once: true});

</script>
