    var onClickPincodeSubmitButton = (event) =>{
        const pincodeInput = document.getElementById('pincode-numberInput');
        const pincodeValue = pincodeInput.value;
        const errorSpan = document.getElementById('errorSpanID');
        if(!pincodePattern.test(pincodeValue)){
            errorSpan.style.display = 'block';
        }
        else{
            var deliveryPinLocationSpan = document.querySelectorAll('[data-role="delivery_pin_location"]');
            const deliveryPinLocationMobSpan1 = document.getElementsByClassName('delivery_text');
            const pincodeUrl = 'https://promise-engine-371111.el.r.appspot.com/GetPincodeDetails?cpin=' + pincodeValue;

            fetch(pincodeUrl)
            .then(response => {
                if (!response.ok) {
                    errorSpan.style.display = 'block';
                    throw new Error(`HTTP error! Status: ${response.status}`);
                }
                return response.json();
            })
            .then(data => {
                deliveryPinLocationSpan.forEach((el, ind) => {
                    el.textContent = `${data[0].cPin}, ${data[0].city}`;
                  });
                Array.from(deliveryPinLocationMobSpan1).forEach(ele => {
                    ele.textContent = `${data[0].cPin}, ${data[0].city}`;
                })
                document.cookie = `cpin=${data[0].cPin}; path=/`;
                localStorage.setItem('city', data[0].city);
                window.sessionStorage.setItem('enteredPinFlag',true);
                hidePinCodeModal();
                getDetailsOfDeliveryType();
            })
            .catch(error => {
                errorSpan.style.display = 'block';
                console.error('Fetch error:', error);
            });
            // Clear error message if there was any
            errorSpan.style.display = 'none';
        }
        event.preventDefault();
        event.stopImmediatePropagation();
    }

    
    document.getElementById("enterPinCodeSection_1").onclick = onClickEnterPinCodeSection;
    document.getElementById("enterPinCodeSection_2").onclick = onClickEnterPinCodeSection;
    
    document.getElementById("closeModal").onclick = hidePinCodeModal; 
    window.onclick = onClickOverlay;
    document.getElementById("pincode-submit-button").onclick = onClickPincodeSubmitButton;
    
    function getCookie(cname) {
      let name = cname + "=";
      let decodedCookie = decodeURIComponent(document.cookie);
      let ca = decodedCookie.split(';');
      for(let i = 0; i < ca.length; i++) {
        let c = ca[i];
        while (c.charAt(0) == ' ') {
          c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
          return c.substring(name.length, c.length);
        }
      }
      return "";
    }

    let isOnLoadCalled = false;
    window.onload = function() {
        if (!isOnLoadCalled) {
            isOnLoadCalled = true;
            
            if (getCookie("cpin")) {
                getDetailsOfDeliveryType('', true);  
            } else {
                sendGAEvent('');
                sendClevertapEvent('');
                sendMixPanelEvent();
            }
        }
    };
    $(document).on('change','.product-section .variant-input',function(){
        if (getCookie("cpin")) {
            getDetailsOfDeliveryType("", true);
        }
    });

