<script>
  function disableScroll() {

// Get the current page scroll position
    scrollTop = window.pageYOffset || document.documentElement.scrollTop;
    scrollLeft = window.pageXOffset || document.documentElement.scrollLeft,

// if any scroll is attempted,
// set this to the previous value
    window.onscroll = function() {
      window.scrollTo(scrollLeft, scrollTop);
    };
  }

  function enableScroll() {
    window.onscroll = function() {};
  }

  $('.slide-nav__link.close-header').click(function() {
    $('body').removeClass('overflow-class');
  });
  const div = document.querySelector('.mobile-nav-trigger');

  $(".mobile-nav-trigger").click(function() {
    var isActive = div.classList.contains('is-active'); // true
    var mob_doc = document.getElementById("mob_pincode")
    var location = window.location.pathname
    if(location != '/cart'){
      if(mob_doc.style.display === "block"){
      mob_doc.style.display = "none"
    }else{
      mob_doc.style.display = "block"
    }
    }
    
    
    console.log('active'.isActive);

// var openClass = document.getElementsByClassName("slide-nav__dropdown is-active").getElementsByClassName("slide-nav__item");
