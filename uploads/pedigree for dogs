        border: 1px solid #EEEEEE;
        padding: 2px 16px;
        display: flex;
        width: 50%;
        height:100%;
        border-radius: 10px;
        margin: unset;
        box-shadow: 0px 4px 8px 0px #0000000F;
      }
      .people-also-viewed-item-modal-button-container-mweb{
        display: none;
        flex-direction: row;
        align-items: center;
        justify-content: unset;
        height: 44px;
        gap: 10px;
      }
      .people-also-viewed-item-modal-view-cart{
        display: flex;
        flex-direction: row;
        justify-content: center;
        align-items: center;
        padding: 9px 21px;
        width: 50%;
        height: 100%;
        background: #EF6C00;
        border-radius: 10px;
        cursor: pointer;
        font-family: 'Gotham Rounded';
        font-size: 14px;
        font-weight: 350;
        line-height: 18px;
        text-align: center;
        color: #FFFFFF;
        bottom: unset;
        position: unset;
      }
      .people-also-viewed-loader-images-container {
        
      }
    
      .people-also-viewed-loader-img {
        display: none; 
      }
    
      .people-also-viewed-loader-img {
          display: none; 
          height: 340px !important;
          width: 161px !important;
        }
      
        .people-also-viewed-loader-img:nth-child(1), .people-also-viewed-loader-img:nth-child(2), .people-also-viewed-loader-img:nth-child(3) {
          display: block;
        }
        #supertails_shipping_addmore-brij-recommendations-POV {
          margin-top: 8px;
        }
    }
  .people-also-viewed-loader-images-container{
    display: flex;
    gap: 20px;
    margin-left: 30px;
    overflow: hidden;
  }
