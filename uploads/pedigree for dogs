  <div class="site-header__drawer-animate"><script>
  function runMyFunction(e) {
    gtag('event', e, {
      'event_category': 'Hamburger menu internal navigation (from code)',
      'event_label': ''
    });

    return true;
  }
  function hdrlnk(title, link, trackingtitle) {
    var mainTitle = trackingtitle;
    if (trackingtitle.includes('_Icon_')) {
      mainTitle = trackingtitle.split('_Icon_')[1];
    }
    if (mainTitle.includes('_Image_')) {
      mainTitle = mainTitle.split('_Image_')[0];
    }
    if (mainTitle.includes('_Label_')) {
      mainTitle = mainTitle.split('_Label_')[0];
    }

    window.dataLayer = window.dataLayer || [];
    dataLayer.push({'event': 'eventTracking', 'eventCategory': 'Hamburger menu internal navigation (from code)', 'eventAction': mainTitle, 'eventLabel': ''});
    dataLayer.push({event: 'hbm_navigation', hbm_elements: mainTitle});

  }
</script>

