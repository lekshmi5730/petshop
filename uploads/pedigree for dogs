    margin: 8px 0px -2px 0px;
    aspect-ratio: 2;
    --_g: no-repeat radial-gradient(circle closest-side, #000 90%, #0000);
    background: var(--_g) 0% 50%, var(--_g) 50% 50%, var(--_g) 100% 50%;
    background-size: calc(100% / 3) 50%;
    animation: l33 1s infinite linear;
  }
  
  @keyframes l33 {
  20% {
    background-position: 0% 0%, 50% 50%, 100% 50%;
  }
  40% {
    background-position: 0% 100%, 50% 0%, 100% 50%;
  }
  60% {
    background-position: 0% 50%, 50% 100%, 100% 0%;
  }
  80% {
    background-position: 0% 50%, 50% 50%, 100% 100%;
  }
}
  
</style>

<script>
  async function fetchFeatureFlagsPOB() {
    const url = 'https://supertails-backend.el.r.appspot.com/features/people_also_bought_pdp/is-visible?platform=web';
    
    const options = {
      method: 'GET',
    };

    try {
      const response = await fetch(url, options);
      if (!response.ok) {
        throw new Error(`HTTP error! status: ${response.status}`);
      }
      const data = await response.json();
      return data;

    } catch (error) {
      // Handle any errors
      console.error('Error fetching data:', error);
    }
  }
  async function fetchFeatureFlagsPOV() {
    const url = 'https://supertails-backend.el.r.appspot.com/features/people_also_viewed_pdp/is-visible?platform=web';
    
    const options = {
      method: 'GET',
    };

    try {
      const response = await fetch(url, options);
      if (!response.ok) {
        throw new Error(`HTTP error! status: ${response.status}`);
      }
      const data = await response.json();
      return data;

    } catch (error) {
      // Handle any errors
      console.error('Error fetching data:', error);
    }
  }
  async function fetchFeatureFlagsSimilar() {
    const url = 'https://supertails-backend.el.r.appspot.com/features/similar_products_pdp/is-visible?platform=web';
    
    const options = {
      method: 'GET',
    };

    try {
      const response = await fetch(url, options);
      if (!response.ok) {
        throw new Error(`HTTP error! status: ${response.status}`);
      }
      const data = await response.json();
      return data;

    } catch (error) {
      // Handle any errors
      console.error('Error fetching data:', error);
    }
  }
  async function checkItemAlreadyInCartRecommendationsPOB(selected_id){
      updatedCartItemsArr2 = await getCartPOB();
      const cartItemsPOB = updatedCartItemsArr2;
      const itemAlreadyInCartPOB = cartItemsPOB?.items?.find(item => item.id == selected_id);
      if(itemAlreadyInCartPOB){
          return itemAlreadyInCartPOB;
      }return false
  }

// Call the function to execute the requests


  async function getPdpRecommendations() {
    const productId = 7503199142126
    let userId = null;
    let uid = 'uid-1694509332797-65994';
    if(userId) {
      uid = `uid-${userId}`;
    }
    const url = `https://supertails-backend.el.r.appspot.com/recommendation/product/add-ons/web?pageType=PRODUCT&id=${productId}&uid=${uid}`;

    const options = {
      method: 'GET',
      headers: {
        "Content-Type": "application/json",
      }
    };

    try {
      const response = await fetch(url, options);
      if (response.ok) {
        const data = await response.json();
        return data?.widgets;
      }
    } catch (err) {
      console.log("error", err);
      throw error;
    }
  }

  async function fetchEdd(skuArr) {
    let pincode = getCpinCookie('cpin');
    let url = `https://promise-engine-371111.el.r.appspot.com/edd?cpin=${pincode}&skuid=${skuArr.join(
      ","
    )}&qty=1`;

    const options = {
      method: 'GET',
      headers: {
        "Content-Type": "application/json",
      }
    };

    try {
      const response = await fetch(url, options);
      if(response.ok) {
        const data = await response.json();
        return data;
      }

    } catch(err) {
      console.log("error", err);
      throw error;
    }

  }

  function createSkuMapPOB(data) {
    const skuMapPOV = {};
    data.forEach(item => {
      const { skuId, ...rest } = item;
      skuMapPOV[skuId] = rest;
    });
    return skuMapPOV;
  }
  
  function displayEddPOB(eddData, skuId) {
    const data = eddData;
    const result = createSkuMapPOB(data);

    const elementEddPOB = document.getElementById(`people-also-bought-item-edd-container-${skuId}`);
    if (result[skuId] === undefined) {
      elementEddPOB.innerHTML = '';
      elementEddPOB.style.height = "42px";
      elementEddPOB.classList.remove("linear-gradient-yellow");
      return;
    }

    if(result[skuId]?.is2HourDelivery) {
      elementEddPOB.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_36.png?v=1726039250" class="people-also-bought-item-edd-icon"/>
      <p class="people-also-bought-item-edd-text"><b style="font-weight: 900;">Superfast</b> <b>${result[skuId]?.message}</b></p>`;
      elementEddPOB.classList.add("linear-gradient-yellow");
    } else if(result[skuId]?.dayCount == 0 || result[skuId]?.dayCount == 1) {
      elementEddPOB.innerHTML = `<img src="https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_34.png?v=1726038501" class="people-also-bought-item-edd-icon"/>
      <p class="people-also-bought-item-edd-text"><b style="font-weight: 900;">Fast</b> <b>${result[skuId]?.message}</b></p>`;
      elementEddPOB.classList.add("linear-gradient-yellow");
    } else {
      elementEddPOB.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_35.png?v=1726039019" class="people-also-bought-item-edd-icon"/>
      <p class="people-also-bought-item-edd-text"><b>${result[skuId]?.message}</b></p>`;
      elementEddPOB.classList.remove("linear-gradient-yellow");
    }
  }
  function displayEddInDrawerMWebPOB(skuId){
    const eddVisibleMWebPOB = document.querySelector('.people-also-bought-item-modal-edd-mweb');
    if (skuMapGlobalPOB[skuId] === undefined) {
      eddVisibleMWebPOB.style.display = "none";
      return;
    }
    if(skuMapGlobalPOB[skuId]?.is2HourDelivery) {
      eddVisibleMWebPOB.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_36.png?v=1726039250" class="people-also-bought-item-edd-icon-quickAdd"/>
      <p class="people-also-bought-item-edd-text-quickAdd"><b style="font-weight: 900;">Superfast</b> <b>${skuMapGlobalPOB[skuId]?.message}</b></p>`;
      eddVisibleMWebPOB.classList.add("linear-gradient-yellow");
    } else if(skuMapGlobalPOB[skuId]?.dayCount == 0 || skuMapGlobalPOB[skuId]?.dayCount == 1) {
      eddVisibleMWebPOB.innerHTML = `<img src="https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_34.png?v=1726038501" class="people-also-bought-item-edd-icon-quickAdd"/>
      <p class="people-also-bought-item-edd-text-quickAdd"><b style="font-weight: 900;">Fast</b> <b>${skuMapGlobalPOB[skuId]?.message}</b></p>`;
      eddVisibleMWebPOB.classList.add("linear-gradient-yellow");
    } else {
      eddVisibleMWebPOB.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_35.png?v=1726039019" class="people-also-bought-item-edd-icon-quickAdd"/>
      <p class="people-also-bought-item-edd-text-quickAdd"><b>${skuMapGlobalPOB[skuId]?.message}</b></p>`;
      eddVisibleMWebPOB.classList.remove("linear-gradient-yellow");
    }
  }

  function displayEddInDrawerPOB(skuId){
    const eddVisible = document.getElementById('people-also-bought-item-modal-edd');
    if (skuMapGlobalPOB[skuId] === undefined) {
      eddVisible.innerHTML = '';
      eddVisible.style.margin = "unset";
      return;
    }
    if(skuMapGlobalPOB[skuId]?.is2HourDelivery) {
      eddVisible.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_36.png?v=1726039250" class="people-also-bought-item-edd-icon-quickAdd"/>
      <p class="people-also-bought-item-edd-text-quickAdd"><b style="font-weight: 900;">Superfast</b> <b>${skuMapGlobalPOB[skuId]?.message}</b></p>`;
      eddVisible.classList.add("linear-gradient-yellow");
    } else if(skuMapGlobalPOB[skuId]?.dayCount == 0 || skuMapGlobalPOB[skuId]?.dayCount == 1) {
      eddVisible.innerHTML = `<img src="https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_34.png?v=1726038501" class="people-also-bought-item-edd-icon-quickAdd"/>
      <p class="people-also-bought-item-edd-text-quickAdd"><b style="font-weight: 900;">Fast</b> <b>${skuMapGlobalPOB[skuId]?.message}</b></p>`;
      eddVisible.classList.add("linear-gradient-yellow");
    } else {
      eddVisible.innerHTML = `<img src=https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_35.png?v=1726039019" class="people-also-bought-item-edd-icon-quickAdd"/>
      <p class="people-also-bought-item-edd-text-quickAdd"><b>${skuMapGlobalPOB[skuId]?.message}</b></p>`;
      eddVisible.classList.remove("linear-gradient-yellow");
    }
  }

  async function displayVariantDrawerPOB(variantsArr, skuArray, eddData, handle, productDetails) {
    const result_sku = createSkuMapPOB(eddData);
    const sortedVariantsArr = variantsArr.sort((a, b) => {
      return (a.isDefault === b.isDefault) ? 0 : a.isDefault ? -1 : 1;
    });
    function handleIncreaseQuantityPOB(event) {
      changeCartQuantityDesktopPOB(selectedVariant?.variantId, true, desktopAddToCartStepper);
    }
    function handleDecreaseQuantityPOB(event) {
      changeCartQuantityDesktopPOB(selectedVariant?.variantId, false, desktopAddToCartStepper, desktopAddToCart);
    }
    
    let selectedVariant = sortedVariantsArr.find(variant => variant.isDefault);

    const addOnItemModal = document.getElementById("people-also-bought-item-modal");
    const addOnItemModalContent = document.getElementById("people-also-bought-item-modal-content");
    const desktopModalBackdrop = document.getElementById("people-also-bought-item-modal-backdrop");

    // Create modal elements (simplified for brevity)
    const createModalElementPOB = (tag, className, innerHTML = '', src = '', id = '') => {
      const elementPOB = document.createElement(tag);
      if (className) elementPOB.classList.add(className);
      if (innerHTML) elementPOB.innerHTML = innerHTML;
      if (src) elementPOB.setAttribute('href', src);
      if (src) elementPOB.src = src;
      if (id) elementPOB.setAttribute('id', id);
      return elementPOB;
    };

    // Modal Header
    const addOnItemModalHeader = createModalElementPOB('div', 'people-also-bought-item-modal-header');
    addOnItemModalHeader.appendChild(createModalElementPOB('div', 'people-also-bought-item-modal-header-text', 'Quick Add'));
    const addOnItemModalHeaderClose = createModalElementPOB('img', 'people-also-bought-item-modal-header-close', '', 'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/mingcute_close-line.png?v=1725521249');
    addOnItemModalHeaderClose.alt = 'close-icon';
    addOnItemModalHeader.appendChild(addOnItemModalHeaderClose);

    // Image Container
    const addOnItemModalImageHandler = createModalElementPOB('a', '', '', `/products/${handle}?variant=${selectedVariant?.variantId}`);
    addOnItemModalImageHandler.appendChild(createModalElementPOB('img', 'people-also-bought-item-modal-image', '', selectedVariant?.imageUrl));
    const addOnItemModalImageContainer = createModalElementPOB('div', 'people-also-bought-item-modal-image-container');
    addOnItemModalImageContainer.appendChild(addOnItemModalImageHandler);

    // Description
    const addOnItemModalDescriptionText = createModalElementPOB('div', 'people-also-bought-item-modal-description-text', selectedVariant?.displayName);
    const addOnItemModalDescriptionArrow = createModalElementPOB('img', 'people-also-bought-item-modal-description-arrow', '', 'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/ArrowRight.png?v=1725523530');
    addOnItemModalDescriptionArrow.alt = 'arrow-right-icon';
    const arrowHandler = createModalElementPOB('a', '', '', `/products/${handle}?variant=${selectedVariant?.variantId}`);
    arrowHandler.appendChild(addOnItemModalDescriptionArrow);
    const addOnItemModalDescription = createModalElementPOB('div', 'people-also-bought-item-modal-description');
    addOnItemModalDescription.appendChild(addOnItemModalDescriptionText);
    addOnItemModalDescription.appendChild(arrowHandler);

    arrowHandler.addEventListener("click", function(event) {
      event.preventDefault();
      let eventPramaObj = {
        "source": "PDP recs",
        "widget": "People Also Bought",
      };
      localStorage.setItem('eventPramaObj', JSON.stringify(eventPramaObj));
      window.location.replace(arrowHandler.href);
    });

    // Price Container
    const addOnItemModalPrice = createModalElementPOB('div', 'people-also-bought-item-modal-price', `${selectedVariant?.price}`);
    
    const addOnItemModalPriceContainer = createModalElementPOB('div', 'people-also-bought-item-modal-price-container');
    addOnItemModalPriceContainer.appendChild(addOnItemModalPrice);

    const addOnItemModalMrp = createModalElementPOB(
      'div',
      'people-also-bought-item-modal-mrp',
      selectedVariant?.compareAtPrice === selectedVariant?.price
        ? `MRP ${selectedVariant?.compareAtPrice}`
        : `MRP <span style="text-decoration: line-through;">${selectedVariant?.compareAtPrice}</span>`
    );
    addOnItemModalPriceContainer.appendChild(addOnItemModalMrp);
    const addOnItemModalSaveText = createModalElementPOB(
      'div',
      'people-also-bought-item-modal-save-text',
      `Save ${Math.round(selectedVariant?.discount)}%`
    );
    addOnItemModalPriceContainer.appendChild(addOnItemModalSaveText);
    if (selectedVariant?.compareAtPrice === selectedVariant?.price) {
      addOnItemModalSaveText.style.display = 'none';
    } else {
      addOnItemModalSaveText.style.display = 'block';
    }
    

    // Other Elements
    let addOnItemModalSubText;
    if(selectedVariant?.pricePerHundred){
      addOnItemModalSubText = createModalElementPOB('div', 'people-also-bought-item-modal-sub-text', `(${selectedVariant?.pricePerHundred}/100g)`);
    }
    else{
      addOnItemModalSubText = createModalElementPOB('div', 'people-also-bought-item-modal-sub-text', '');
    }
    const addOnItemModalEddLoader = createModalElementPOB('div', 'loader_edd_recs');
    const addOnItemModalEddIcon = createModalElementPOB('img', 'people-also-bought-item-modal-edd-icon', '', '');
    addOnItemModalEddIcon.alt = 'edd-modal-icon';
    const addOnItemModalEddText = createModalElementPOB('div', 'people-also-bought-item-modal-edd-text', '');
    const addOnItemModalEdd = createModalElementPOB('div', 'people-also-bought-item-modal-edd', '', 'people-also-bought-item-modal-edd');
    addOnItemModalEdd.setAttribute('id', 'people-also-bought-item-modal-edd');
    addOnItemModalEdd.appendChild(addOnItemModalEddLoader);
    // addOnItemModalEdd.appendChild(addOnItemModalEddIcon);
    // addOnItemModalEdd.appendChild(addOnItemModalEddText);

    //Update the EDD by default
  

    // Variant Bubbles
    const addOnItemModalVariantContainer = createModalElementPOB('div', 'people-also-bought-item-modal-variant-container', '','','people-also-bought-item-modal-variant-container');
    let lowStockDiv = '';
    let isLowStock = false;
    sortedVariantsArr.forEach(variant => {
      isLowStock = true;
      let isVisible = false;
      if (variant.inventoryQuantity < 5) {
        isVisible = true;
        lowStockDiv = `
          <div class="low-stock-alert-pob">
            <p style="font-family: Lato; font-size: 12px; font-weight: 500; text-align: center; margin: 0;">
              ${variant.inventoryQuantity} units left
            </p>
          </div>
        `;
      }
      const variantBubble = createModalElementPOB('button', 'people-also-bought-item-modal-variant-bubble', variant.title);
      variantBubble.setAttribute('id', `variant-bubble-${variant.id}`);
      variantBubble.dataset.id = variant.id;
      if (variant.id === selectedVariant?.id) {
        variantBubble.classList.add('active');
      }
      variantBubble.addEventListener('click', function () {
        const variantId = this.dataset.id;
        selectedVariant = variantsArr.find(v => v.id === variantId);
        updateModalContentPOB(selectedVariant, desktopAddToCart, desktopAddToCartStepper, increaseQuantityPOB, decreaseQuantity, handleIncreaseQuantityPOB, handleIncreaseQuantityPOB);
        displayEddInDrawerPOB(selectedVariant?.sku);
      });
      const variantContainer = createModalElementPOB('div', 'variant-container');
      if(isVisible) {
        variantContainer.innerHTML = lowStockDiv; // Insert low stock alert if applicable
      }
      else{
        variantContainer.innerHTML = '';
      }
      const variantBox = createModalElementPOB('div', 'variant-box');
      variantBox.setAttribute('id', `variant-box-${variant.id}`);
      variantBox.dataset.id = variant.id;

      // Append the variant bubble to the variant box
      variantBox.appendChild(variantBubble);

      // Append the variant box to the container
      variantContainer.appendChild(variantBox);

      // Append the container to the modal variant container
      addOnItemModalVariantContainer.appendChild(variantContainer);
    });
  
    //Add to Cart Button

    const desktopAddToCart = document.getElementById('people-also-bought-item-modal-button-desktop');
    const desktopAddToCartStepper = document.getElementById('people-also-bought-item-modal-button-container-desktop');
    const increaseQuantityPOB = document.getElementById('people-also-bought-item-modal-add-button-desktop')
    const decreaseQuantity = document.getElementById('people-also-bought-item-modal-minus-button-desktop');

    


    addOnItemModalContent.innerHTML = '';
    addOnItemModalContent.appendChild(addOnItemModalHeader);
    addOnItemModalContent.appendChild(addOnItemModalImageContainer);
    addOnItemModalContent.appendChild(addOnItemModalDescription);
    addOnItemModalContent.appendChild(addOnItemModalPriceContainer);
    if(addOnItemModalSubText){
      addOnItemModalContent.appendChild(addOnItemModalSubText);
    }
    addOnItemModalContent.appendChild(addOnItemModalEdd);
    addOnItemModalContent.appendChild(addOnItemModalVariantContainer);
    addOnItemModalContent.appendChild(desktopAddToCart);
    addOnItemModalContent.appendChild(desktopAddToCartStepper);

    let defaultVariantQuantity = await checkItemAlreadyInCartRecommendationsPOB(selectedVariant?.variantId);
    if(defaultVariantQuantity?.quantity){
      desktopAddToCartStepper.style.display = 'flex';
      desktopAddToCart.style.display = 'none';
      desktopAddToCartStepper.children[0].children[1].children[0].innerHTML = defaultVariantQuantity?.quantity;
      if (!increaseQuantityPOB.hasAttribute('dirty-POB')) {
        increaseQuantityPOB.addEventListener('click', handleIncreaseQuantityPOB);
        increaseQuantityPOB.setAttribute('dirty-POB', 'true');
      }
      if (!decreaseQuantity.hasAttribute('dirty-POB')) {
        decreaseQuantity.addEventListener('click', handleDecreaseQuantityPOB);
        decreaseQuantity.setAttribute('dirty-POB', 'true');
      }
      desktopAddToCartStepper.children[1].addEventListener("click", handleCartRedirect);
    }
    else{
      desktopAddToCartStepper.style.display = 'none';
      increaseQuantityPOB.removeEventListener('click', changeCartQuantityDesktopPOB);
      decreaseQuantity.removeEventListener('click', changeCartQuantityDesktopPOB);
      desktopAddToCart.style.display = 'flex';
    }
    


    let variantDetailsQuickAdd = document.getElementById('people-also-bought-item-modal-variant-container');
    displayEddInDrawerPOB(selectedVariant?.sku);
    if(isLowStock){
      variantDetailsQuickAdd.style.marginTop = '30px';
      variantDetailsQuickAdd.style.rowGap = '30px';
    }

    desktopAddToCart.addEventListener("click", async function() {
      const dataLayerObj = {
        "event": "add_to_cart",
        "source": 'PDP recommendation',
        "widget": 'People Also Bought',
        "ecommerce": {
        "currency": "INR",
        "value": selectedVariant?.price,
        "items": [
          {
              "item_id": selectedVariant?.variantId,
              "item_sku": selectedVariant?.sku,
              "item_name": selectedVariant?.displayName,
              "coupon": "",
              "discount": "",
              "item_list_id": "",
              "item_list_name": "",
              "item_variant": selectedVariant?.displayName,
              "location_id": "",
              "price": selectedVariant?.price,
              "quantity": 1,
              "item_category": productDetails[0]?.category,
              "item_category3": productDetails[0]?.category3,
              "item_category2": productDetails[0]?.category2,
              "item_category4": "",
              "item_category5": "",
          }
        ]
        }
      }
      const clevertapObj = {
        "product_id": selectedVariant?.variantId,
        "product_name": selectedVariant?.displayName,
        "brand": "",
        "variant_name": selectedVariant?.displayName,
        "variant_id": selectedVariant?.variantId,
        "price": selectedVariant?.price,
        "mrp": selectedVariant?.compareAtPrice/100,
        "quantity": 1,
        "product_sku": selectedVariant?.sku,
        "product_category1": "", 
        "product_category3": "",
        "product_category2": "",
        "product_category4": "",
        "product_category5": "",
        "source": 'PDP recommendation',
        "widget": 'People Also Bought'
      };
      let url = '/cart/add.js';
      const payload = {
        id: selectedVariant?.variantId,
        quantity: 1
      };
      desktopAddToCart.classList.add('atc-loader');
      desktopAddToCart.innerHTML = '';
      try {
        const res = await fetch(url, {
          method: 'POST',
          headers: {
            Accept: "application/json",
            "Content-Type": "application/json"
          },
          body: JSON.stringify(payload)
        });
        const data = await res.json();
        if(data?.status !== 422) {
          desktopAddToCart.style.display = 'none';
          desktopAddToCartStepper.style.display = 'flex';
          desktopAddToCartStepper.children[0].children[1].children[0].innerHTML = '1';
          desktopAddToCartStepper.children[1].addEventListener("click", handleCartRedirect);
          if (!increaseQuantityPOB.hasAttribute('dirty-POB')) {
            increaseQuantityPOB.addEventListener('click', handleIncreaseQuantityPOB);
            increaseQuantityPOB.setAttribute('dirty-POB', 'true');
          }
          if (!decreaseQuantity.hasAttribute('dirty-POB')) {
            decreaseQuantity.addEventListener('click', handleDecreaseQuantityPOB);
            decreaseQuantity.setAttribute('dirty-POB', 'true');
          }
          dataLayer.push(dataLayerObj);
          clevertap.event.push("Product Added", clevertapObj);
          updateCartIconNumberPOB();
        }
      } catch(err) {
        console.log("error", err);
      }
      finally {
        desktopAddToCart.classList.remove('atc-loader');
        desktopAddToCart.innerHTML = 'Add to cart';
      }
    });


    // Close Modal
    addOnItemModalHeaderClose.addEventListener('click', function () {
      addOnItemModal.style.display = 'none';
      desktopModalBackdrop.style.display = 'none';
      document.body.style.overflow = 'auto';
    });
  }

  async function updateModalContentPOB(variant, desktopAddToCart, desktopAddToCartStepper, handleIncreaseQuantityPOB, handleDecreaseQuantityPOB, increaseQuantityPOB, decreaseQuantity) {
    let selected_variant_check = await checkItemAlreadyInCartRecommendationsPOB(variant?.variantId)
    function handleIncreaseQuantityVariantPOB(event) {
      changeCartQuantityDesktopPOB(variant?.variantId, true, desktopAddToCartStepper);
    }
    function handleDecreaseQuantityVariantPOB(event) {
      changeCartQuantityDesktopPOB(variant?.variantId, false, desktopAddToCartStepper, desktopAddToCart);
    }
    const quantityContainer =  document.getElementById('people-also-bought-item-modal-button-container');
    const addToCart = document.getElementById('people-also-bought-item-modal-button');
    const addOnItemModalContent = document.getElementById("people-also-bought-item-modal-content");
    
    document.querySelector('.people-also-bought-item-modal-image').src = variant.imageUrl;
    document.querySelector('.people-also-bought-item-modal-description-text').innerHTML = variant.displayName;
    document.querySelector('.people-also-bought-item-modal-price').innerHTML = `${variant.price}`;
    
    if(variant?.pricePerHundred){
      document.querySelector('.people-also-bought-item-modal-sub-text').innerHTML = `(${variant.pricePerHundred}/100g)`;
    }
    else{
      document.querySelector('.people-also-bought-item-modal-sub-text').innerHTML = ``;
    }
    
    document.querySelector('.people-also-bought-item-modal-mrp').innerHTML = 
      variant?.compareAtPrice === variant?.price 
        ? `MRP ${variant.compareAtPrice}`
        : `MRP <span style="text-decoration: line-through;">${variant.compareAtPrice}</span>`;

    // Set the Save Text with conditional display
    document.querySelector('.people-also-bought-item-modal-save-text').innerHTML = `Save ${Math.round(variant.discount)}%`;
    document.querySelector('.people-also-bought-item-modal-save-text').style.display = 
      variant?.compareAtPrice === variant?.price ? 'none' : 'block';

    const allBubbles = document.querySelectorAll('.people-also-bought-item-modal-variant-bubble');
    allBubbles.forEach(bubble => bubble.classList.remove('active'));

    // Remove the `#` from the ID
    const selectedBubble = document.getElementById(`variant-bubble-${variant.id}`);
    if (selectedBubble) {
      selectedBubble.classList.add('active');
    } else {
      console.error(`No elementPOB found with ID variant-bubble-${variant.id}`);
    }
    if(selected_variant_check?.quantity){
      desktopAddToCartStepper.style.display = 'flex';
      desktopAddToCart.style.display = 'none';
      desktopAddToCartStepper.children[0].children[1].children[0].innerHTML = selected_variant_check?.quantity;
      if (!increaseQuantityPOB.hasAttribute('dirty-POB')) {
          increaseQuantityPOB.addEventListener('click', handleIncreaseQuantityVariantPOB);
          increaseQuantityPOB.setAttribute('dirty-POB', 'true');
        }
      if (!decreaseQuantity.hasAttribute('dirty-POB')) {
        decreaseQuantity.addEventListener('click', handleDecreaseQuantityVariantPOB);
        decreaseQuantity.setAttribute('dirty-POB', 'true');
      }
    }
    else{
      desktopAddToCartStepper.style.display = 'none';
      desktopAddToCart.style.display = 'flex';
    }
  }
  function handleCartRedirect() {
    window.location.href = '/cart';
  }

  async function displayVariantDrawerMWebPOB(variantsArr, handle, productDetails) {
    const sortedVariantsArr = variantsArr.sort((a, b) => {
      return (a.isDefault === b.isDefault) ? 0 : a.isDefault ? -1 : 1;
    });

    function handleIncreaseQuantityPOB(event) {
      changeCartQuantityPOB(selectedVariant?.variantId, true, mobileAddToCartStepper);
    }
    function handleDecreaseQuantityPOB(event) {
      changeCartQuantityPOB(selectedVariant?.variantId, false, mobileAddToCartStepper, mobileAddToCart);
    }
    
    let selectedVariant = sortedVariantsArr.find(variant => variant.isDefault);

    const mobileDrawer = document.getElementById("mobile-drawer-container-POB");

    // Create modal elements (simplified for brevity)
    const createDrawerElementPOB = (tag, className, innerHTML = '', src = '', href = '', id = '') => {
      const elementPOB = document.createElement(tag);
      if (className) elementPOB.classList.add(className);
      if (innerHTML) elementPOB.innerHTML = innerHTML;
      if (src) elementPOB.src = src;
      if (id) elementPOB.setAttribute('id', id);
      if (href && tag === 'a') elementPOB.setAttribute('href', href); 
      return elementPOB;
    };

    // Drawer Header
    const drawerHeader = createDrawerElementPOB('div', 'mobile-drawer-header');
    const drawerImage = createDrawerElementPOB('div', 'mobile-drawer-image');
    const drawerImageImg = createDrawerElementPOB('img', '', '', selectedVariant?.imageUrl);
    drawerImageImg.alt = 'Product Image';
    drawerImageImg.width = 70;
    drawerImageImg.height = 70;
    drawerImage.appendChild(drawerImageImg);
    
    // Drawer Details
    const drawerDetails = createDrawerElementPOB('div', 'mobile-drawer-details');
    const detailsHeader = createDrawerElementPOB('div', 'mobile-drawer-details-header');
    const drawerTitle = createDrawerElementPOB('p', 'mobile-drawer-details-title', selectedVariant?.displayName);
    const arrowButton = createDrawerElementPOB('a', '', '','', `/products/${handle}?variant=${selectedVariant?.variantId}`);
    const arrowImg = createDrawerElementPOB('img', '', '', 'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/ArrowRight.png?v=1725866116');
    arrowImg.width = 13;
    arrowImg.height = 16;
    arrowButton.appendChild(arrowImg);
    detailsHeader.appendChild(drawerTitle);
    detailsHeader.appendChild(arrowButton);

    arrowButton.addEventListener("click", function(event) {
      event.preventDefault();
      let eventPramaObj = {
        "source": "PDP recs",
        "widget": "People Also Bought",
      };
      localStorage.setItem('eventPramaObj', JSON.stringify(eventPramaObj));
      window.location.replace(arrowButton.href);
    });
    
    // Price Details
    const priceDetails = createDrawerElementPOB('div', 'mobile-drawer-priceDetails');
    const priceContainer = createDrawerElementPOB('div', 'mobile-drawer-price');
    const priceValue = createDrawerElementPOB('p', 'mobile-drawer-price-value', `${selectedVariant?.price}`);
    priceContainer.appendChild(priceValue);

    const priceMrp = createDrawerElementPOB(
      'p',
      'mobile-drawer-price-mrp',
      selectedVariant?.compareAtPrice === selectedVariant?.price
        ? `MRP ${selectedVariant?.compareAtPrice}`
        : `MRP <span style="text-decoration: line-through;">${selectedVariant?.compareAtPrice}</span>`
    );
    const priceDiscount = createDrawerElementPOB(
      'p',
      'mobile-drawer-price-discount',
      `SAVE ${Math.round(selectedVariant?.discount)}%`
    );

    if (selectedVariant?.compareAtPrice === selectedVariant?.price) {
      priceDiscount.style.display = 'none';
    } else {
      priceDiscount.style.display = 'block';
    }
    priceContainer.appendChild(priceMrp);
    priceContainer.appendChild(priceDiscount);

    let unitPrice;
    if(selectedVariant?.pricePerHundred){
      unitPrice = createDrawerElementPOB('p', 'mobile-drawer-unit-price-value', `(${selectedVariant?.pricePerHundred}/100g)` );
    }
    else{
      unitPrice = createDrawerElementPOB('p', 'mobile-drawer-unit-price-value', '');
    }
    

    //Other Details

    const addOnItemModalEddLoader2 = createDrawerElementPOB('div','loader_edd_rec2')

    const addOnItemModalEddIcon = createDrawerElementPOB('img', 'people-also-bought-item-modal-edd-icon-mweb', '', '');
    addOnItemModalEddIcon.alt = 'edd-modal-icon';
    const addOnItemModalEddText = createDrawerElementPOB('div', 'people-also-bought-item-modal-edd-text-mweb', '');
    const addOnItemModalEdd = createDrawerElementPOB('div', 'people-also-bought-item-modal-edd-mweb', '', 'people-also-bought-item-modal-edd-mweb');
    
    addOnItemModalEdd.appendChild(addOnItemModalEddLoader2);
    
    
    
    // Append everything
    priceDetails.appendChild(priceContainer);
    if(unitPrice){
      priceDetails.appendChild(unitPrice);
    }
    priceDetails.appendChild(addOnItemModalEdd);
    drawerDetails.appendChild(detailsHeader);
    drawerDetails.appendChild(priceDetails);

    drawerHeader.appendChild(drawerImage);
    drawerHeader.appendChild(drawerDetails);
    const crossIcon2 = createDrawerElementPOB('img', 'mobile-drawer-cross-icon-2', '', 'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/closeIcon.png?v=1726478114', '', 'mobile-drawer-cross-icon-2');
    crossIcon2.addEventListener('click', function () {
      mobileDrawer.style.display = 'none';
      document.body.style.overflow = 'auto';
      document.getElementById('mobile-drawer-backdrop-POB').style.display = 'none';
    })

    // Variant Bubbles
    const variantDetails = createDrawerElementPOB('div', 'variant-details-2', '', '','','variant-details-2');
    let lowStockDiv = '';
    let isLowStock = false;
    sortedVariantsArr.forEach(variant => {
      let isVisible = false;
      if (variant.inventoryQuantity < 5) {
        isLowStock = true;
        isVisible = true;
        lowStockDiv = `
          <div class="low-stock-alert-pob">
            <p style="font-family: Lato; font-size: 12px; font-weight: 500; text-align: center; margin: 0;">
              ${variant.inventoryQuantity} units left
            </p>
          </div>
        `;
      }

      // Create the variant bubble
      const variantBubble = createDrawerElementPOB('button', 'mobile-drawer-variant-bubble', variant.title);
      variantBubble.setAttribute('id', `mobile-drawer-variant-bubble-${variant.id}`);
      variantBubble.dataset.id = variant.id;
      if (variant.id === selectedVariant?.id) {
        variantBubble.classList.add('active');
      }
      variantBubble.addEventListener('click', function () {
        const variantId = this.dataset.id;
        selectedVariant = variantsArr.find(v => v.id === variantId);
        updateModalContentMWebPOB(selectedVariant, mobileAddToCart, mobileAddToCartStepper, handleIncreaseQuantityPOB, handleDecreaseQuantityPOB, increaseQuantityPOB, decreaseQuantity);
        displayEddInDrawerMWebPOB(selectedVariant?.sku);
      });

      // Create the variant container with the optional lowStockDiv
      const variantContainer = createDrawerElementPOB('div', 'variant-container');
      if(isVisible) variantContainer.innerHTML = lowStockDiv; 
      else{
        variantContainer.innerHTML = '';
      }

      // Create the variant box
      const variantBox = createDrawerElementPOB('div', 'variant-box');
      variantBox.setAttribute('id', `variant-box-${variant.id}`);
      variantBox.dataset.id = variant.id;
      const variantTitle = createDrawerElementPOB('p', 'variant-title', variant.title);
      variantBox.appendChild(variantTitle);

      // Append the variant box to the container
      variantContainer.appendChild(variantBubble);

      // Append the container to variant details
      variantDetails.appendChild(variantContainer);
    });

    //Add to cart button

    const mobileAddToCart = document.getElementById('people-also-bought-item-modal-button-mweb');
    const mobileAddToCartStepper = document.getElementById('people-also-bought-item-modal-button-container-mweb');
    const increaseQuantityPOB = document.getElementById('people-also-bought-item-modal-add-button-mweb');
    const decreaseQuantity = document.getElementById('people-also-bought-item-modal-minus-button-mweb');


    // Clear drawer content and append new elements
    const seperator = createDrawerElementPOB('hr', 'mobile-separator', '', '');
    mobileDrawer.innerHTML = '';
    mobileDrawer.appendChild(drawerHeader);
    mobileDrawer.appendChild(createDrawerElementPOB('hr', 'mobile-divider', '', ''));
    mobileDrawer.appendChild(variantDetails);
    mobileDrawer.appendChild(mobileAddToCart);
    mobileDrawer.appendChild(mobileAddToCartStepper);
    mobileDrawer.insertBefore(seperator, mobileAddToCart);
    
    let defaultVariantQuantity = await checkItemAlreadyInCartRecommendationsPOB(selectedVariant?.variantId);
    if(defaultVariantQuantity?.quantity){
      mobileAddToCartStepper.style.display = 'flex';
      mobileAddToCart.style.display = 'none';
      mobileAddToCartStepper.children[0].children[1].children[0].innerHTML = defaultVariantQuantity?.quantity;
      if (!increaseQuantityPOB.hasAttribute('dirty-POB')) {
        increaseQuantityPOB.addEventListener('click', handleIncreaseQuantityPOB);
        increaseQuantityPOB.setAttribute('dirty-POB', 'true');
      }
      if (!decreaseQuantity.hasAttribute('dirty-POB')) {
        decreaseQuantity.addEventListener('click', handleDecreaseQuantityPOB);
        decreaseQuantity.setAttribute('dirty-POB', 'true');
      }
      mobileAddToCartStepper.children[1].addEventListener("click", handleCartRedirect);
    }
    else{
      mobileAddToCartStepper.style.display = 'none';
      increaseQuantityPOB.removeEventListener('click', changeCartQuantityPOB);
      decreaseQuantity.removeEventListener('click', changeCartQuantityPOB);
      mobileAddToCart.style.display = 'flex';
    }




    mobileDrawer.appendChild(crossIcon2);
    let crossIconContainer2 = document.getElementById('mobile-drawer-cross-icon-2');
    let variantDetails2 = document.getElementById('variant-details-2');
    if(isLowStock){
      variantDetails2.style.marginTop = '18%';
      variantDetails2.style.rowGap = '40px';
    }
    crossIconContainer2.style.bottom = mobileDrawer.clientHeight + 7 + 'px';
    
    mobileAddToCart.addEventListener("click", async function() {
      const dataLayerObj = {
        "event": "add_to_cart",
        "source": 'PDP recommendation',
        "widget": 'People Also Bought',
        "ecommerce": {
        "currency": "INR",
        "value": selectedVariant?.price,
        "items": [
          {
              "item_id": selectedVariant?.variantId,
              "item_sku": selectedVariant?.sku,
              "item_name": selectedVariant?.displayName,
              "coupon": "",
              "discount": "",
              "item_list_id": "",
              "item_list_name": "",
              "item_variant": selectedVariant?.displayName,
              "location_id": "",
              "price": selectedVariant?.price,
              "quantity": 1,
              "item_category": productDetails[0]?.category,
              "item_category3": productDetails[0]?.category3,
              "item_category2": productDetails[0]?.category2,
              "item_category4": "",
              "item_category5": "",
          }
        ]
        }
      }
      const clevertapObj = {
        "product_id": selectedVariant?.variantId,
        "product_name": selectedVariant?.displayName,
        "brand": "",
        "variant_name": selectedVariant?.displayName,
        "variant_id": selectedVariant?.variantId,
        "price": selectedVariant?.price,
        "mrp": selectedVariant?.compareAtPrice/100,
        "quantity": 1,
        "product_sku": selectedVariant?.sku,
        "product_category1": "", 
        "product_category3": "",
        "product_category2": "",
        "product_category4": "",
        "product_category5": "",
        "source": "PDP recommendation",
        "widget": "People Also Bought",
      };
      let url = '/cart/add.js';
      const payload = {
        id: selectedVariant?.variantId,
        quantity: 1
      };
      mobileAddToCart.classList.add('atc-loader-mweb')
      mobileAddToCart.innerHTML =  '';
      try {
        const res = await fetch(url, {
          method: 'POST',
          headers: {
            Accept: "application/json",
            "Content-Type": "application/json"
          },
          body: JSON.stringify(payload)
        });
        const data = await res.json();
        if(data?.status !== 422) {
          mobileAddToCart.style.display = 'none';
          mobileAddToCartStepper.style.display = 'flex';
          mobileAddToCartStepper.children[0].children[1].children[0].innerHTML = '1';
          mobileAddToCartStepper.children[1].addEventListener("click", handleCartRedirect);
          if (!increaseQuantityPOB.hasAttribute('dirty-POB')) {
            increaseQuantityPOB.addEventListener('click', handleIncreaseQuantityPOB);
            increaseQuantityPOB.setAttribute('dirty-POB', 'true');
          }
          if (!decreaseQuantity.hasAttribute('dirty-POB')) {
            decreaseQuantity.addEventListener('click', handleDecreaseQuantityPOB);
            decreaseQuantity.setAttribute('dirty-POB', 'true');
          }
          dataLayer.push(dataLayerObj);
          clevertap.event.push("Product Added", clevertapObj);
          updateCartIconNumberPOB();
        }
      } catch(err) {
        console.log("error", err);
      }
      finally{
        mobileAddToCart.innerHTML = 'Add to cart'
        mobileAddToCart.classList.remove('atc-loader-mweb')
      }
    });
    displayEddInDrawerMWebPOB(selectedVariant?.sku);
  }

  async function changeCartQuantityPOB(selectedVariantId, isAdd, mobileAddToCartStepper, mobileAddToCart) {
    let url = '/cart/change.js';
    const itemAlreadyInCartPOB =  await checkItemAlreadyInCartRecommendationsPOB(selectedVariantId);
    const updateQuantityPOB = itemAlreadyInCartPOB ? itemAlreadyInCartPOB.quantity + (isAdd ? 1 : -1) : 1;
    const payload = {
      id: selectedVariantId,
      quantity: updateQuantityPOB
    };
    try {
      const res = await fetch(url, {
        method: 'POST',
        headers: {
          Accept: "application/json",
          "Content-Type": "application/json"
        },
        body: JSON.stringify(payload)
      });
      const data = await res.json();
      if(data?.status !== 422) {
        mobileAddToCartStepper.children[0].children[1].children[0].innerHTML = updateQuantityPOB;
        if(updateQuantityPOB ===0) {
          mobileAddToCart.style.display = 'flex';
          mobileAddToCartStepper.style.display = 'none';
        }
        updateCartIconNumberPOB();
      }
    } catch(err) {
      console.log("error", err);
    }
  }
  async function changeCartQuantityDesktopPOB(selectedVariantId, isAdd, desktopAddToCartStepper, desktopAddToCart) {
    let url = '/cart/change.js';
    const itemAlreadyInCartPOB =  await checkItemAlreadyInCartRecommendationsPOB(selectedVariantId);
    const updateQuantityPOB = itemAlreadyInCartPOB ? itemAlreadyInCartPOB.quantity + (isAdd ? 1 : -1) : 1;
    const payload = {
      id: selectedVariantId,
      quantity: updateQuantityPOB
    };
    try {
      const res = await fetch(url, {
        method: 'POST',
        headers: {
          Accept: "application/json",
          "Content-Type": "application/json"
        },
        body: JSON.stringify(payload)
      });
      const data = await res.json();
      if(data?.status !== 422) {
        desktopAddToCartStepper.children[0].children[1].children[0].innerHTML = updateQuantityPOB;
        if(updateQuantityPOB ===0) {
          desktopAddToCart.style.display = 'flex';
          desktopAddToCartStepper.style.display = 'none';
        }
        updateCartIconNumberPOB();
      }
    } catch(err) {
      console.log("error", err);
    }
  }
  async function viewToastPOB() {
      const successMessageDiv = `
        <div class="supertails_shipping_addmore-brij" id="supertails_shipping_addmore-brij-recommendations-POB">
          <div style="display: flex; gap: 10px; justify-content:center; align-items:center;">
            <div style="display: flex; flex-direction:column; justify-content:center; align-items:flex-start;">
              <div class="sticky-atc-pdp-header-message" style="display: flex; justify-content: left; align-items: center; gap: 4px; color: black; font-size: 12px; height: 48px;">
                <img src="https://cdn.shopify.com/s/files/1/0565/8021/0861/files/sticky-atc-pdp-min.gif?v=1692697712" alt="twinkle" style="width:44px;height:44px">
                <div style="width: 144px;">
                  <div class="cart_itemadded" style="text-align:left;"> 1 Item added</div>
                  <div class="sticky-atc-pdp-footer-message" style="line-height: 15px; color: white; font-size:12px; text-align:left;"></div>
                </div>
              </div>
            </div>
            <a style="color:white" href="/cart">
              <div class="atc-btn" style="display: flex; justify-content: center; align-items: center; font-size: 12px; border: 1px solid white; padding: 10px 12px; border-radius: 6px; color: white; height: min-content; font-weight:bold; min-width:125px;">
                <svg style="margin-right:4px;" width="20" height="18" viewBox="0 0 20 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" clip-rule="evenodd" d="M0.176071 0.184241C-0.0887979 0.446838 -0.051706 0.830741 0.26626 1.11592C0.472184 1.3007 0.925431 1.37364 2.07463 1.40659L3.60813 1.45069L5.23952 7.55406L6.87105 13.6574L12.2871 13.6976C17.994 13.7398 18.3672 13.7012 18.2769 13.0787C18.1813 12.4209 17.7493 12.365 12.736 12.3628C10.1381 12.3615 7.98194 12.3089 7.94469 12.246C7.90729 12.1832 7.82849 11.9084 7.76939 11.6357L7.66212 11.1398H12.4265C16.0553 11.1398 17.2268 11.0943 17.3412 10.9491C17.56 10.6715 20.0014 4.39328 20 4.11191C19.9994 3.98115 19.9007 3.74083 19.7805 3.57787C19.5714 3.29421 19.2659 3.2785 12.5183 3.20541L5.47422 3.12912L5.06022 1.70017C4.83259 0.914205 4.55126 0.211554 4.43506 0.138466C4.10047 -0.07149 0.393999 -0.0318188 0.176071 0.184241ZM8.53307 14.7072C7.53792 15.1502 7.13115 16.1732 7.59594 17.0643C8.29344 18.4016 10.3414 18.2463 10.7713 16.8236C11.1861 15.4514 9.8017 14.1425 8.53307 14.7072ZM14.8162 14.7174C13.9019 15.1152 13.5074 16.1313 13.9271 17.008C14.6849 18.5914 16.9876 18.1895 17.1866 16.4392C17.2573 15.8173 17.2154 15.7004 16.7465 15.2114C16.1433 14.5823 15.5035 14.4185 14.8162 14.7174Z" fill="white"/>
                </svg>
                GO TO CART
              </div>
            </a>
          </div>
        </div>`;
      const toastContainer = document.createElement('div');
      toastContainer.innerHTML = successMessageDiv;
      const parentElement = document.getElementById('HeaderCart');
      while (parentElement.firstChild) {
        parentElement.removeChild(parentElement.firstChild);
      }
      parentElement.appendChild(toastContainer.firstElementChild);
      parentElement.style.display = 'flex';
      parentElement.style.justifyContent = 'center';
      parentElement.style.alignItems = 'center';
      parentElement.style.width = '350px';
      parentElement.style.height = '60px';
      $('.cart_itemadded').css('font-size', '16px');
      parentElement.style.transform = 'translateY(0px)';
      parentElement.style.opacity = '1';
      if(window.innerWidth < 768){
        parentElement.style.marginTop = '4px';
      }
      else{
        parentElement.style.marginTop = '0px';
      }
      parentElement.style.transition = 'opacity 0.3s ease 0.15s, transform 0.25s cubic-bezier(0.165, 0.84, 0.44, 1) 0.15s';
      parentElement.classList.add('is-active')
      setTimeout(() => {
        parentElement.classList.remove('is-active');
    }, 1500);
  } 

  async function updateModalContentMWebPOB(variant, mobileAddToCart, mobileAddToCartStepper, handleIncreaseQuantityPOB, handleDecreaseQuantityPOB, increaseQuantityPOB, decreaseQuantity) {
      // Update image
      document.querySelector('.mobile-drawer-image img').src = variant.imageUrl;

      // Update product title
      function handleIncreaseQuantityVariantPOB(event) {
        changeCartQuantityPOB(variant?.variantId, true, mobileAddToCartStepper);
      }
      function handleDecreaseQuantityVariantPOB(event) {
        changeCartQuantityPOB(variant?.variantId, false, mobileAddToCartStepper, mobileAddToCart);
      }
      document.querySelector('.mobile-drawer-details-title').innerHTML = variant.displayName;

      // Update price, MRP, and discount
      document.querySelector('.mobile-drawer-price-value').innerHTML = `${variant.price}`;
      let defaultVariantQuantity = await checkItemAlreadyInCartRecommendationsPOB(variant?.variantId);

      document.querySelector('.mobile-drawer-price-mrp').innerHTML = 
      variant?.price === variant?.compareAtPrice 
        ? `MRP ${variant.compareAtPrice}`
        : `MRP <span style="text-decoration: line-through;">${variant.compareAtPrice}</span>`;

      document.querySelector('.mobile-drawer-price-discount').innerHTML = `SAVE ${Math.round(variant.discount)}%`;
      document.querySelector('.mobile-drawer-price-discount').style.display = variant?.price === variant?.compareAtPrice ? 'none' : 'block';


      // Update price per unit
      if(variant?.pricePerHundred){
        document.querySelector('.mobile-drawer-unit-price-value').innerHTML = `(${variant.pricePerHundred}/100g)`;
      }
      else{
        document.querySelector('.mobile-drawer-unit-price-value').innerHTML = '';
      }

      // Deselect all variant bubbles
      const allBubbles = document.querySelectorAll('.mobile-drawer-variant-bubble');
      allBubbles.forEach(bubble => bubble.classList.remove('active'));

      // Set the selected variant bubble to active
      const selectedBubble = document.getElementById(`mobile-drawer-variant-bubble-${variant.id}`);
      if (selectedBubble) {
          selectedBubble.classList.add('active');
      } else {
          console.error(`No elementPOB found with ID variant-bubble-${variant.id}`);
      }

      if(defaultVariantQuantity?.quantity){
        mobileAddToCartStepper.style.display = 'flex';
        mobileAddToCart.style.display = 'none';
        mobileAddToCartStepper.children[0].children[1].children[0].innerHTML = defaultVariantQuantity?.quantity;
        if (!increaseQuantityPOB.hasAttribute('dirty-POB')) {
            increaseQuantityPOB.addEventListener('click', handleIncreaseQuantityVariantPOB);
            increaseQuantityPOB.setAttribute('dirty-POB', 'true');
          }
        if (!decreaseQuantity.hasAttribute('dirty-POB')) {
          decreaseQuantity.addEventListener('click', handleDecreaseQuantityVariantPOB);
          decreaseQuantity.setAttribute('dirty-POB', 'true');
        }
      }
      else{
        mobileAddToCartStepper.style.display = 'none';
        mobileAddToCart.style.display = 'flex';
      }
  }

  let peopleAlsoBoughtAllVariants = [];

  let  updatedCartItemsArr2 = {"note":null,"attributes":{},"original_total_price":0,"total_price":0,"total_discount":0,"total_weight":0.0,"item_count":0,"items":[],"requires_shipping":false,"currency":"INR","items_subtotal_price":0,"cart_level_discount_applications":[],"checkout_charge_amount":0}; 

  async function getCartPOB() {
    const url = '/cart.js';
    const res = await fetch(url);
    const data = await res.json();
    return data;
  }

  async function updateCartIconNumberPOB() {
    updatedCartItemsArr2 = await getCartPOB();
    const CartBubbles = document.querySelectorAll('cart-link__bubble');
    const cartIcons = document.querySelectorAll(".cart-link__bubble-num");
    if (CartBubbles.length) {
      CartBubbles.forEach(elementPOB => {
        showElement(elementPOB);
      })
    }
    if (cartIcons.length) {
      cartIcons.forEach(elementPOB => {
        elementPOB.textContent = updatedCartItemsArr2?.item_count;
      })
    }
  }

  document.addEventListener("DOMContentLoaded", async function () {
    const widgetHeadingText = document.getElementById("people-also-bought-heading-text");
    const addOnItems = document.getElementById("people-also-bought-items");
    const addOnItemModal = document.getElementById("people-also-bought-item-modal");
    const addOnItemModalContent = document.getElementById("people-also-bought-item-modal-content");
    const desktopModalBackdrop = document.getElementById("people-also-bought-item-modal-backdrop");
    const loaderPeopleBought = document.getElementById("people-also-bought-loader");
    widgetHeadingText.style.display = "none";

    function handleBackdropClick(event) {
      if(event.target === addOnItemModal) {
        desktopModalBackdrop.style.display = "none";
        addOnItemModal.style.display = "none";
        document.body.style.overflow = 'auto';
      }
    }
    addOnItemModal.addEventListener("click", handleBackdropClick);
    
    const mobileDrawerBackdrop = document.getElementById("mobile-drawer-backdrop-POB")
    const mobileDrawer = document.getElementById("mobile-drawer-container-POB");
    
    function handleBackdropClickMWeb(event) {
      if(event.target === mobileDrawerBackdrop) {
        mobileDrawer.style.display = 'none';
        document.body.style.overflow = 'auto';
        mobileDrawerBackdrop.style.display = 'none';
      }
    }
    mobileDrawerBackdrop.addEventListener("click", handleBackdropClickMWeb);

    const addOnItemModalContentMWeb = document.getElementById("mobile-drawer-container-POB");

    let addOnProducts = [];
    let addOnProductsVariants = [];
    let skuArray = [];
    let featureFlagDataPOB =  await fetchFeatureFlagsPOB();
    let recommendationDataArr = await getPdpRecommendations();
    recommendationDataArr = [recommendationDataArr[1]];
    recommendationDataArr?.forEach((data) => {
      widgetHeadingText.innerHTML = data?.title;
      addOnProducts.push(...data?.recommendedProducts);
    });
    if(addOnProducts?.length === 0 || !featureFlagDataPOB.visible) {
      loaderPeopleBought.style.display = 'none';
      document.getElementById('people-also-bought').style.display = 'none';
      return;
    }

    addOnProducts?.forEach((product) => {
      const defaultVariant = product?.variants?.find(variant => variant?.isDefault);
      const addOnsSku = product?.variants?.map(variant => variant?.sku);
      skuArray.push(...addOnsSku);
      defaultVariant.totalVariants = product?.totalVariants;
      defaultVariant.handle = product?.handle;
      defaultVariant.productId = product?.productId;
      addOnProductsVariants.push(defaultVariant);
    });

    let eddData = await fetchEdd(skuArray);

    for (let i = 0; i < addOnProductsVariants?.length; i++) {
      const addOnItemCard = document.createElement("div");
      addOnItemCard.classList.add("people-also-bought-item-card");
      addOnItemCard.setAttribute("id", `people-also-bought-item-card-${addOnProductsVariants[i]?.sku}`);

      addOnItemCard.addEventListener("click", function(event) {
        event.preventDefault();
        let eventPramaObj = {
          "source": "PDP recs",
          "widget": "People Also Bought",
        };
        localStorage.setItem('eventPramaObj', JSON.stringify(eventPramaObj));
        window.location.replace(`/products/${addOnProductsVariants[i]?.handle}?variant=${addOnProductsVariants[i]?.variantId}`);
      });

      const addOnItemImgContainer = document.createElement("div");
      addOnItemImgContainer.classList.add("people-also-bought-item-img-container");

      const addOnItemImg = document.createElement("img");
      addOnItemImg.classList.add("people-also-bought-item-img");
      addOnItemImg.setAttribute("id", `people-also-bought-item-img-${addOnProductsVariants[i]?.sku}`);
      addOnItemImg.src = addOnProductsVariants[i]?.imageUrl;

      const itemRating = document.createElement("div");
      itemRating.classList.add("people-also-bought-item-rating");

      const itemRatingValue = document.createElement("div");
      itemRatingValue.classList.add("people-also-bought-item-rating-value");
      itemRatingValue.innerHTML = `${addOnProductsVariants[i]?.metaRating}`;

      const itemRatingStar = document.createElement("img");
      itemRatingStar.classList.add("people-also-bought-item-rating-star");
      itemRatingStar.src = "https://cdn.shopify.com/s/files/1/0565/8021/0861/files/svgviewer-png-output_32.png?v=1725429108";
      itemRatingStar.alt = 'rating-star';

      const itemRatingDivider = document.createElement("div");
      itemRatingDivider.classList.add("people-also-bought-item-rating-divider");

      const itemRatingTotal = document.createElement("div");
      itemRatingTotal.classList.add("people-also-bought-item-rating-total");
      itemRatingTotal.innerHTML = `${addOnProductsVariants[i]?.metaRatingCount}`;

      const addOnItemDescriptionContainer = document.createElement("div");
      addOnItemDescriptionContainer.classList.add("people-also-bought-item-description-container");

      const addOnItemDescriptionText = document.createElement("div");
      addOnItemDescriptionText.classList.add("people-also-bought-item-description-text");
      addOnItemDescriptionText.innerHTML = addOnProductsVariants[i]?.displayName;

      const addOnItemDescriptionSubText = document.createElement("div");
      addOnItemDescriptionSubText.classList.add("people-also-bought-item-description-sub-text");
      addOnItemDescriptionSubText.innerHTML = addOnProductsVariants[i]?.title;

      const addOnItemDescriptionInfo = document.createElement("div");
      addOnItemDescriptionInfo.classList.add("people-also-bought-item-description-info");
      if(addOnProductsVariants[i]?.totalVariants > 1){
        addOnItemDescriptionInfo.innerHTML = `(${addOnProductsVariants[i]?.totalVariants} sizes available)`;
      }
      else{
        addOnItemDescriptionInfo.innerHTML = '';
        addOnItemDescriptionInfo.style.height = '11px';
      }
      

      const addOnItemPriceContainer = document.createElement("div");
      addOnItemPriceContainer.classList.add("people-also-bought-item-price-container");

      const addOnItemPrice = document.createElement("div");
      addOnItemPrice.classList.add("people-also-bought-item-price");
      addOnItemPrice.innerHTML = `${addOnProductsVariants[i]?.price}`;

      const addOnItemDiscount = document.createElement("div");
      addOnItemDiscount.classList.add("people-also-bought-item-discount");

      const addOnItemDiscountText = document.createElement("div");
      addOnItemDiscountText.classList.add("people-also-bought-item-discount-text");
      addOnItemDiscountText.innerHTML = `${Math.round(addOnProductsVariants[i]?.discount)}% OFF`;

      const addOnItemOldPrice = document.createElement("div");
      addOnItemOldPrice.classList.add("people-also-bought-item-old-price");
      if (addOnProductsVariants[i]?.compareAtPrice === addOnProductsVariants[i]?.price) {
        addOnItemOldPrice.innerHTML = `MRP ${addOnProductsVariants[i]?.compareAtPrice}`;
        addOnItemDiscount.style.display = 'none';
      } else {
        addOnItemOldPrice.innerHTML = `MRP <span style="text-decoration: line-through;">${addOnProductsVariants[i]?.compareAtPrice}</span>`;
        addOnItemDiscountText.style.display = 'block';
      }

      const addOnItemEddContainer = document.createElement("div");
      addOnItemEddContainer.classList.add("people-also-bought-item-edd-container");
      addOnItemEddContainer.setAttribute("id", `people-also-bought-item-edd-container-${addOnProductsVariants[i]?.sku}`);

      const addOnItemButton = document.createElement("button");
      addOnItemButton.classList.add("people-also-bought-item-button");
      addOnItemButton.innerHTML = "Add to cart";
      addOnItemButton.setAttribute("id", `people-also-bought-item-button-${addOnProductsVariants[i]?.productId}`);

      function handleButtonClick(event) {
        event.stopPropagation();
        recommendationDataArr?.forEach((item) => {
          const defaultVariants = item?.recommendedProducts?.find(product => product?.productId == addOnProductsVariants[i]?.productId);
          peopleAlsoBoughtAllVariants = [];
          peopleAlsoBoughtAllVariants.push(defaultVariants);
        });
        if(peopleAlsoBoughtAllVariants[0]?.variants.length > 1){
          if (event.target.id === `people-also-bought-item-button-${addOnProductsVariants[i]?.productId}`) {
            $('body').css('overflow', 'hidden');
          }
          if(window.innerWidth <= 768){
            addOnItemModalContentMWeb.style.display = "block";
            document.getElementById('mobile-drawer-backdrop-POB').style.display = "block";
          }
          else{
            desktopModalBackdrop.style.display = "block";
            addOnItemModal.style.display = "block";
          }
          dataLayer.push({
            "event": "variant_selection_drawer_viewed",
            "ecommerce": {
              "currency": "INR",
              "value": peopleAlsoBoughtAllVariants[0]?.variants[0]?.price,
              "items": [
              {
                "item_id": peopleAlsoBoughtAllVariants[0]?.variants[0]?.variantId,
                "item_name": peopleAlsoBoughtAllVariants[0]?.variants[0]?.displayName,
                "coupon": "",
                "discount": "",
                "item_brand": peopleAlsoBoughtAllVariants[0]?.brand,
                "item_category": peopleAlsoBoughtAllVariants[0]?.category,
                "item_category2": peopleAlsoBoughtAllVariants[0]?.category2,
                "item_category3": peopleAlsoBoughtAllVariants[0]?.category3,
                "item_category4": "",
                "item_category5": "",
                "item_list_id": "",
                "item_list_name": "",
                "item_variant": peopleAlsoBoughtAllVariants[0]?.variants[0]?.displayName,
                "location_id": "",
                "price": peopleAlsoBoughtAllVariants[0]?.variants[0]?.compareAtPrice,
                "quantity": 1,
              }
              ]
            }
          }); 
          clevertap.event.push("Variant Selection Drawer Viewed", {
            "value": peopleAlsoBoughtAllVariants[0]?.variants[0]?.price,
            "item_id": peopleAlsoBoughtAllVariants[0]?.variants[0]?.variantId,
            "item_name": peopleAlsoBoughtAllVariants[0]?.variants[0]?.displayName,
            "item_brand": peopleAlsoBoughtAllVariants[0]?.brand,
            "item_variant": peopleAlsoBoughtAllVariants[0]?.variants[0]?.displayName,
            "item_category": peopleAlsoBoughtAllVariants[0]?.category,
            "item_category2": peopleAlsoBoughtAllVariants[0]?.category2,
            "item_category3": peopleAlsoBoughtAllVariants[0]?.category3,
            "item_category4": "",
            "item_category5": "",
            "item_list_id": "",
            "item_list_name": "",
            "price": peopleAlsoBoughtAllVariants[0]?.variants[0]?.compareAtPrice,
            "quantity": 1,
            "currency": "INR"
          });
          displayVariantDrawerPOB(peopleAlsoBoughtAllVariants[0]?.variants, eddData,skuArray, peopleAlsoBoughtAllVariants[0]?.handle, peopleAlsoBoughtAllVariants);
          displayVariantDrawerMWebPOB(peopleAlsoBoughtAllVariants[0]?.variants, peopleAlsoBoughtAllVariants[0]?.handle, peopleAlsoBoughtAllVariants); 
        }
        else{
          async function addToCart() {
            const dataLayerObj = {
              "event": "add_to_cart",
              "source": 'PDP recommendation',
              "widget": 'People Also Bought',
              "ecommerce": {
              "currency": "INR",
              "value": peopleAlsoBoughtAllVariants[0]?.variants[0]?.price,
              "items": [
                {
                    "item_id": peopleAlsoBoughtAllVariants[0]?.variants[0]?.variantId,
                    "item_sku": peopleAlsoBoughtAllVariants[0]?.variants[0]?.sku,
                    "item_name": peopleAlsoBoughtAllVariants[0]?.variants[0]?.displayName,
                    "coupon": "",
                    "discount": "",
                    "item_list_id": "",
                    "item_list_name": "",
                    "item_variant": peopleAlsoBoughtAllVariants[0]?.variants[0]?.displayName,
                    "location_id": "",
                    "price": peopleAlsoBoughtAllVariants[0]?.variants[0]?.price,
                    "quantity": 1,
                    "item_category": peopleAlsoBoughtAllVariants[0]?.category,
                    "item_category3": peopleAlsoBoughtAllVariants[0]?.category3,
                    "item_category2": peopleAlsoBoughtAllVariants[0]?.category2,
                    "item_category4": "",
                    "item_category5": "",
                }
              ]
              }
            }
            const clevertapObj = {
              "product_id": peopleAlsoBoughtAllVariants[0]?.variants[0]?.variantId,
              "product_name": peopleAlsoBoughtAllVariants[0]?.variants[0]?.displayName,
              "brand": "",
              "variant_name": peopleAlsoBoughtAllVariants[0]?.variants[0]?.displayName,
              "variant_id": peopleAlsoBoughtAllVariants[0]?.variants[0]?.variantId,
              "price": peopleAlsoBoughtAllVariants[0]?.variants[0]?.price/100,
              "mrp": peopleAlsoBoughtAllVariants[0]?.variants[0]?.compareAtPrice/100,
              "quantity": 1,
              "product_sku": peopleAlsoBoughtAllVariants[0]?.variants[0]?.sku,
              "product_category1": "", 
              "product_category3": "",
              "product_category2": "",
              "product_category4": "",
              "product_category5": "",
              "source": 'PDP recommendation',
              "widget": 'People Also Bought',
            };
            let url = '/cart/add.js';
            const payload = {
              id: peopleAlsoBoughtAllVariants[0]?.variants[0]?.variantId,
              quantity: 1
            };
            addOnItemButton.classList.add('atc-loader-single-variant');
            addOnItemButton.innerText = '';
            try {
              const res = await fetch(url, {
                method: 'POST',
                headers: {
                  Accept: "application/json",
                  "Content-Type": "application/json"
                },
                body: JSON.stringify(payload)
              });
              const data = await res.json();
              if (data?.status !== 422) {
                dataLayer.push(dataLayerObj);
                clevertap.event.push("Product Added", clevertapObj);
                viewToastPOB();
                updateCartIconNumberPOB();
              }
            } catch (err) {
              console.log("error", err);
            }
            finally {
              addOnItemButton.classList.remove('atc-loader-single-variant');
              addOnItemButton.innerText = 'Add to cart';
            }
          }
          addToCart();

        }
        
      }

      addOnItemButton.addEventListener("click", handleButtonClick);

      itemRating.appendChild(itemRatingValue);
      itemRating.appendChild(itemRatingStar);
      itemRating.appendChild(itemRatingDivider);
      itemRating.appendChild(itemRatingTotal);

      addOnItemImgContainer.appendChild(addOnItemImg);
      addOnItemImgContainer.appendChild(itemRating);

      if(!addOnProductsVariants[i]?.metaRating){
        itemRating.style.display = 'none';
      }
      
      addOnItemDescriptionContainer.appendChild(addOnItemDescriptionText);
      addOnItemDescriptionContainer.appendChild(addOnItemDescriptionSubText);
      addOnItemDescriptionContainer.appendChild(addOnItemDescriptionInfo);

      addOnItemDiscount.appendChild(addOnItemDiscountText);

      addOnItemPriceContainer.appendChild(addOnItemPrice);
      addOnItemPriceContainer.appendChild(addOnItemDiscount);

      addOnItemCard.appendChild(addOnItemImgContainer);
      addOnItemCard.appendChild(addOnItemDescriptionContainer);
      addOnItemCard.appendChild(addOnItemPriceContainer);
      addOnItemCard.appendChild(addOnItemOldPrice);
      addOnItemCard.appendChild(addOnItemEddContainer);
      addOnItemCard.appendChild(addOnItemButton);

      addOnItems.appendChild(addOnItemCard);

      displayEddPOB(eddData, addOnProductsVariants[i]?.sku);
      skuMapGlobalPOB = createSkuMapPOB(eddData, addOnProductsVariants[i]?.sku);
    }

    loaderPeopleBought.style.display = "none";
    widgetHeadingText.style.display = "block";

  }, {once: true});

</script>
