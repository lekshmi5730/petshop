  </div><script>
  $(document).ready(function () {
    $(".set-acc > .collapsible-trigger").on("click", function () {
        if ($(this).hasClass("is-open")) {
            $(this).removeClass("is-open");
            $(this).siblings(".collapsible-content").slideUp(200);
        } else {
            $(".set-acc > .collapsible-trigger").removeClass("is-open");
            $(this).addClass("is-open");
            $(".collapsible-content").slideUp(200);
            $(this).siblings(".collapsible-content").slideDown(200);
        }
    });

    $(".set-acc-review > .collapsible-trigger").on("click", function () {
        if ($(this).hasClass("is-open")) {
            $(this).removeClass("is-open");
            $(this).siblings(".product-reviews--tab").slideUp(200);
        } else {
            $(".set-acc-review > .collapsible-trigger").removeClass("is-open");
            $(this).addClass("is-open");
            $(".product-reviews--tab").slideUp(200);
            $(this).siblings(".product-reviews--tab").slideDown(200);
        }
    });
  });

  $(document).ready(function() {
    $('.tabs a').click(function(){
       $('.panel').hide();
       $('.tabs li.active').removeClass('active');
       $(this).parent().addClass('active');
       var panel = $(this).attr('href');
       $(panel).fadeIn(1000);
       return false;  // prevents link action
    });  // end click
    $('.tabs li:first a').click();
    $('.panel_mobile_tab').on('click', function(e){
      e.preventDefault();
      if($(this).parents('.panel').find('.mob_tab_desc_list').hasClass('active')){
        $(this).parents('.panel').find('.mob_tab_desc_list').removeClass('active');
      }else{
      $('.mob_tab_desc_list').removeClass('active');
      $(this).parents('.panel').find('.mob_tab_desc_list').addClass('active');
      }
    })
  }); // end ready
</script>
