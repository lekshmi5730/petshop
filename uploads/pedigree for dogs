    let hideModalTimeout;
    let timeOutAnimation;
    image_to_cart_animation_image.src = product_json.featured_image;
    var addedItem = getSelectedVarient();
    showAddToCartButtonCalculation();
    function sendToGA(eventName, addedItem) {
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            'event': eventName,
            "ecommerce": {
                "currency": "INR",
                "value": addedItem.compare_at_price / 100,
                "items": [{
                    "item_name": "Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food",
                    "item_brand": "Pedigree",
                    "item_variant": addedItem.title,
                    "item_id": "7503199142126",
                    "item_sku": addedItem.sku,
                    "item_category": "Dogs",
                    "item_category2": "Food",
                    "item_category3": "Dry Food",
                    "item_category4": "",
                    "item_category5": "",
                    "price": addedItem.price / 100,
                    "edd": sessionStorage.getItem("edd") || '',
                }]
            },
            "content_group": "PDP"
        });
    }
    function sendToCleverTap(eventName, addedItem){
        clevertap.event.push(eventName,{
            "product_id": "7503199142126",
            "product_name": "Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food",
            "brand": "Pedigree",
            "product_category1": "Dogs",
            "product_category2": "Food",
            "product_category3": "Dry Food",
            "product_category4": "",
            "product_category5": "",
            "variant_name": addedItem.title,
            "variant_id": addedItem.id,
            "price": addedItem.price/100,
            "mrp": addedItem.compare_at_price/100,
            "quantity": 1,
            "rating": 4.89,
            "rating_count": 35,
            "product_sku": addedItem.sku,
            "Page Content": "PDP",
            "Edd": sessionStorage.getItem("edd") || '',
        });
    }
    function sendToMixPanel(eventName, addedItem){
        if(window.mixpanel){
            const dynamicTimeElement = document.getElementById('dynamicTime') ;
            const errorTextElement = document.getElementById('error-text-id') ;
            const dynamicTime = dynamicTimeElement ? dynamicTimeElement.textContent : '';
            const errorText = errorTextElement ? errorTextElement.textContent : '';

            const deliveryDay =  dynamicTime || errorText || '';
            mixpanel.track(eventName,{
                'Product Name': "Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food",
                'Product ID': 7503199142126,
                'Variant ID': addedItem.id,
                'Variant Name': addedItem.name,
                'Brand': "Pedigree",
                'Category L1': "Dogs",
                'Category L2': "Food",
                'Category L3': "Dry Food",
                "Category L4": null,
                "Category L5": null,
                'MRP': addedItem.compare_at_price/100,
                'Selling Price': addedItem.price/100,
                'Rating Count': "35",
                'Rating': "4.89",
                'Product Sku': addedItem.sku,
                'City': localStorage.getItem('city'),
                 location: 'PDP',
                "Page Content": 'Product Page',
                "Delivery Time": deliveryDay,
                "Device Type": localStorage.getItem('_deviceTypeIL'),
                "Edd": sessionStorage.getItem('edd') || ""
            });
        }
    }
    addToCartButton.onclick = addtoCartButtonClick;
    addItemPlusButton.onclick = ()=> { 
        var addedItem = getSelectedVarient();
        // sendToGA('increase_quantity', addedItem);
        // sendToCleverTap('Quantity Increased', addedItem);
        // sendToMixPanel('Quantity Increased', addedItem);
        addItemToCart({quantity:1,minus_button:false,first_time:false});
    };
    removeItemPlusButton.onclick = ()=> {
        var addedItem = getSelectedVarient();
        // const addedItem = getSelectedVarient();
        // sendToGA('decrease_quantity', addedItem);
        // sendToCleverTap('Quantity Decreased', addedItem);
        // sendToMixPanel('Quantity Decreased', addedItem);   
        addItemToCart({quantity:-1,minus_button:true,first_time:false});
        showAddToCartButtonCalculation();
    }
    overlay_atc.onclick = hideModal_atc;
    close_modal_atc.onclick = hideModal_atc;

    modal_atc.addEventListener('mouseenter', () => {
        clearTimeout(hideModalTimeout);
    });

    modal_atc.addEventListener('mouseleave', () => {
        hideModalTimeout = setTimeout(hideModal_atc, 2000);
    });
    function addtoCartButtonClick(e){
        if((window.innerWidth < 768) && shouldDrawerOpen && !isDrawerOpen){
            event.preventDefault();
            sizeDrawerModel.style.display = 'flex';
            plus_minus_button.style.zIndex = '11';
            sizeDrawer.style.display = 'flex';
            sizeChartImageDiv.style.display = 'none';
            openDrawerEvents();
            atcBtnClick();
            setTimeout(() => {
            isDrawerOpen = true;
            }, 500);

            const tooltip = $(".pdp-size-help-container");
            setTimeout(() => {
                tooltip.css("display", "flex").hide().fadeIn();
              }, 2000);
  
            setTimeout(() => {
            tooltip.fadeOut(3000);
            }, 5000);
            return;
        }
        if(sizeDrawerModel) {
            vid && vid.pause();
            sizeDrawerModel.style.display = 'none';
        }
        isDrawerOpen = false;
        addImageAnimation();
        disableButton(addToCartButton);
        addItemToCart({quantity:1,minus_button:false,first_time:true});
        hideElement(addToCartButton);
        showElement(interStateAddToCartButton);
        if(isMobile){
            image_to_cart_animation_image.src = product_json.featured_image;
            showElement(image_to_cart_animation);
            showElement(added_to_cart_animation);
        }
        timeOutAnimation = setTimeout(() => {
            enableButton(addToCartButton);
            hideElement(interStateAddToCartButton);
            showElement(plus_minus_button);
            showElement(goToCart_id);
            if(isMobile){
                hideElement(added_to_cart_animation);
                hideElement(image_to_cart_animation);
            }
            clearTimeout(timeOutAnimation);
        }, 1100);
    }
    
    async function addItemToCart({minus_button,first_time}){
        disableButton(addItemPlusButton);
        disableButton(removeItemPlusButton);
        let url = '/cart/add.js';
        let updateQuantity; 
        const selected_id = $(".product-single__variants").find(':selected').attr("selected_variant_id");
        const itemAlreadyInCart =  checkItemAlreadyInCart();
        if(first_time){
            updateQuantity = 1;
        }
        const payload = {
            form_type: "product",
            utf8: "",
            id: selected_id,
            quantity: updateQuantity
        };
        if(itemAlreadyInCart && !first_time){
            url = '/cart/change.js';
            updateQuantity = minus_button ? itemAlreadyInCart.quantity-1 : itemAlreadyInCart.quantity+1;
            delete payload.form_type;
            delete payload.utf8;
            payload.quantity = updateQuantity;
            payload.id = itemAlreadyInCart.key;
        }
        showElement(loader_id);
        hideElement(quantityElement);
        try{
            const res = await fetch(url, {
                method: 'POST',
                headers: {
                    Accept: "application/json",
                    "Content-Type": "application/json"
                },
                body: JSON.stringify(payload)
            });
            const data = await res.json();
            if(data?.status === 422){
                throw new Error(data?.message);
            }
            hideElement(loader_id);
            showElement(quantityElement);
            var addedItem = getSelectedVarient();
            
            if(updateQuantity){
                quantityElement.textContent = `${updateQuantity}`;
                
            }else{
                hideElement(plus_minus_button);
                hideElement(goToCart_id);
                hideElement(interStateAddToCartButton);
                setTimeout(function() {
                showAddToCartButtonCalculation();
                }, 700);
            }
            await updateCartIconNumber(first_time);
            
                enableButton(addItemPlusButton);
            
            enableButton(removeItemPlusButton);
            if(first_time && !isMobile){
                showCartPreview();
                hideModalTimeout = setTimeout(hideModal_atc, 4000);
            }
            if(!first_time){
                
                    getDetailsOfDeliveryType(updateQuantity);
                  
            }
            
            if(minus_button){
                sendToGA('decrease_quantity', addedItem);
                sendToCleverTap('Quantity Decreased', addedItem);
                sendToMixPanel('Quantity Decreased', addedItem);   
            }else if(!first_time){
                sendToGA('increase_quantity', addedItem);
                sendToCleverTap('Quantity Increased', addedItem);
                sendToMixPanel('Quantity Increased', addedItem);
            }
            updatePhamacyBanner(updateQuantity);
        }catch(err){
                hideElement(loader_id);
                showElement(quantityElement);
                enableButton(addItemPlusButton);
                enableButton(removeItemPlusButton);
                console.log({err});
            }
    }

    $(document).on('change','.product-section .variant-input',function(){
        showAddToCartButtonCalculation();
    });

    async function showAddToCartButtonCalculation(){
        const itemAlreadyInCart = checkItemAlreadyInCart();
        
        if(itemAlreadyInCart){     
            hideElement(addToCartButton);
            hideElement(interStateAddToCartButton);
            showElement(plus_minus_button);
            showElement(goToCart_id);
            const quantity = itemAlreadyInCart.quantity;
            const quantityElement = document.getElementById("item-quantity-id");
            quantityElement.textContent = `${quantity}`;
            updatePhamacyBanner(quantity)
            
                enableButton(addItemPlusButton);
            
        }else{
            showElement(addToCartButton);
            hideElement(interStateAddToCartButton);
            hideElement(plus_minus_button);
            hideElement(goToCart_id);
            updatePhamacyBanner(0)
        }
       

    }
    function hideElement(ele){
        ele.classList.remove("show_atc");
        ele.classList.add("hide_atc");
    }
    function showElement(ele){
        ele.classList.remove("hide_atc");
        ele.classList.add("show_atc");
    }
    function checkItemAlreadyInCart(){
        const selected_id = $(".product-single__variants").find(':selected').attr("selected_variant_id");
        const cartItems = updatedCartItems;
        const itemAlreadyInCart = cartItems?.items?.find(item => item.id == selected_id);
        if(itemAlreadyInCart){
            return itemAlreadyInCart;
        }return false
    }
    async function getCart(){
        const url = '/cart.js';
        const res = await fetch(url);
        const data = await res.json();
        return data;
    }
    async function updateCartIconNumber(first_time){
        updatedCartItems = await getCart();
        const CartBubbles = document.querySelectorAll('cart-link__bubble');
        const cartIcons = document.querySelectorAll(".cart-link__bubble-num");
        if(CartBubbles.length){
            CartBubbles.forEach(element => {
                showElement(element);
            })
        }
        if(cartIcons.length){
            cartIcons.forEach(element => {
                element.textContent = updatedCartItems.item_count;
            })
        }
        if(first_time){
            var addedItem = getSelectedVarient();
            sendEvents({addedItem});
        }
    }
    function showCartPreview(){
        showElement(overlay_atc);
        showElement(modal_atc);
        const goToCart_Modal = document.getElementById("goToCart_Modal_id");
        const moreitems_atc_id = document.getElementById("moreitems_atc_id");
        const product_row_atc= document.getElementById("product_row_atc_id");
        const product_row_atc_innerHtml = updatedCartItems.items.slice(0,2).map(cartItemDiv).join('');
        product_row_atc.innerHTML = product_row_atc_innerHtml;
        goToCart_Modal.textContent = `Go to Cart (${updatedCartItems.item_count})`;
        if( (updatedCartItems.item_count -2) > 0 ){
            showElement(moreitems_atc_id);
            moreitems_atc_id.textContent = `+${updatedCartItems.item_count -2} more in the cart`;
        }else{
            moreitems_atc_id.textContent = "";
            hideElement(moreitems_atc_id);
        }
    }
    function cartItemDiv( item ){    
        return `
        <div class="cart_item_atc" id="${item.id}">
            <img class="item-image" src="${item.image}">
            <span class="item-title">${item.title}</span>
        </div>
        `;
    }
    function hideModal_atc(){
        $('#modal_atc_id').fadeOut(2000,'swing',function(){
            $('#modal_atc_id').removeAttr('style')
            hideElement(modal_atc);
            clearTimeout(hideModalTimeout)
        });
        $('#overlay_atc_id').fadeOut(2000,'swing',function(){
            $('#overlay_atc_id').removeAttr('style')
            hideElement(overlay_atc);
        });
    }
    function disableButton(button){
        button.disabled = true;
    }
    function enableButton(button){
        button.disabled = false;
    }
    window.dataLayer = window.dataLayer || [];
    (function() {
            var originalPush = dataLayer.push;
            dataLayer.push = function() {
                var args = [].slice.call(arguments);
                if (args?.[0]?.[0]==='event' && args?.[0]?.[1] === 'add_to_cart') {
                    return; 
                }
                return originalPush.apply(dataLayer, args);
            };
        })();
    function sendEvents({addedItem}){  
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            "event": "add_to_cart",
            "ecommerce": {
            "currency": "INR",
            "value": addedItem.compare_at_price/100,
            "items": [
            {
                "item_id": "7503199142126",
                "item_sku": addedItem.sku,
                "item_name": "Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food",
                "coupon": "",
                "discount": "",
                "index": 0,
                "item_brand": "Pedigree",
                "item_category": "Dogs",
                "item_category2": "Food",
                "item_category3": "Dry Food",
                "item_category4": "",
                "item_category5": "",
                "item_list_id": "",
                "item_list_name": "",
                "item_variant": addedItem.title,
                "location_id": "",
                "price": addedItem.price/100,
                "quantity": 1,
                "edd": sessionStorage.getItem('edd')  
            }
            ]
            }
        });
        clevertap.event.push("Product Added", {
            "product_id": "7503199142126",
            "product_name": "Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food",
            "brand": "Pedigree",
            "product_category1": "Dogs",
            "product_category2": "Food",
            "product_category3": "Dry Food",
            "product_category4": "",
            "product_category5": "",
            "variant_name": addedItem.title,
            "variant_id": addedItem.id,
            "price": addedItem.price/100,
            "mrp": addedItem.compare_at_price/100,
            "quantity": 1,
            "rating": 4.89,
            "rating_count": 35,
            "product_sku": addedItem.sku,
            'edd': sessionStorage.getItem('edd')  
        });
        if(window.mixpanel){
            const dynamicTimeElement = document.getElementById('dynamicTime');
            const errorTextElement = document.getElementById('error-text-id');
            const dynamicTime = dynamicTimeElement && dynamicTimeElement.textContent ;
            const errorText = errorTextElement && errorTextElement.textContent  ;
            const deliveryDay =  dynamicTime || errorText || 'N/A';
            mixpanel.track("Product Added", {
            'Product Name': "Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food",
            'Product ID': 7503199142126,
            'Variant ID': addedItem.id,
            'Variant Name': addedItem.name,
            'Brand': "Pedigree",
            'Category L1': "Dogs", 
            'Category L2': "Food", 
            'Category L3': "Dry Food", 
            "Category L4": null,
            "Category L5": null,
