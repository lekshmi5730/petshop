<script>
  document.getElementById('pincodeInput').addEventListener('keypress', function (event) {
    var key = event.keyCode; // Only allow numbers to be entered
    if (key < 48 || key > 57 || event.target.value.toString().length >= 6) {
      event.preventDefault();
    }
  });
</script>

<script>
  function validateInput() {
    var inputField = document.getElementById('pincodeInput');
    var inputValue = inputField.value;

    // Remove non-digit characters
    var sanitizedValue = inputValue.replace(/\D/g, '');

    // Truncate to 6 digits
    sanitizedValue = sanitizedValue.slice(0, 6);

    // Update the input value
    inputField.value = sanitizedValue;
  }
</script>

<script>
  function toLowerCase(element) {
    element.value = element.value.toLowerCase();
  }
</script>

<script>
  var customerID = '';
  var customerData = '';
  var addressCount = ``;
  function addEvents(name) {
    try {
      window.dataLayer = window.dataLayer || [];
      window.dataLayer.push({
        event: `${name}`,
      });
    } catch (error) {
      console.log('Error in event', error);
    }
  }

  function mixpanelAddaddressEvent(data){
    try {
      if(window.mixpanel){
        mixpanel.track("Address Added", {
          "Address": data.customer_address.customer_address,
          "Device Type": localStorage.getItem('_deviceTypeIL')
        });
      }
    } catch (error) {
      console.log("Error in mixpanelAddaddressEvent", error)
    } 
  }

  function removeZindexAddToCart() {
    var overlayAddToCart = document.getElementsByClassName('add-to-cart-sticky1');
    if (overlayAddToCart.length !== 0) {
      if (window.matchMedia('(max-width: 768px)').matches) {
        overlayAddToCart[0].style.zIndex = 0;
      }
    }

    let atc = document.querySelector('.sticky-atc');
    if (atc) {
      atc.style.zIndex = 2;
      atc.style.transition = 'none';
    }
  }

  function addZindexAddToCart() {
    var overlayAddToCart = document.getElementsByClassName('add-to-cart-sticky1');

    if (overlayAddToCart.length !== 0) {
      if (window.matchMedia('(max-width: 768px)').matches) {
        // Change the max-width to your mobile breakpoint
        overlayAddToCart[0].style.zIndex = 11; // Apply inline style for mobile
      }
    }

    let atc = document.querySelector('.sticky-atc');
    if (atc) {
      atc.style.zIndex = 9998;
      atc.style.transition = '1s';
    }
  }

  function getCpinCookie(cookieName) {
    const name = cookieName + '=';
    const decodedCookie = decodeURIComponent(document.cookie);
    const cookieArray = decodedCookie.split(';');

    for (let i = 0; i < cookieArray.length; i++) {
      let cookie = cookieArray[i].trim();
      if (cookie.indexOf(name) === 0) {
        return cookie.substring(name.length, cookie.length);
      }
    }

    return null;
  }
  
  
  document.addEventListener('custom:pin-update', function() {
    setPincode()
  })

  document.addEventListener('DOMContentLoaded', function () {
    setPincode()
  })
  
  function setPincode(){
    var alreaysetcookie = getCpinCookie('cpin')
    if (alreaysetcookie) {
      const pincodeUrl = 'https://promise-engine-371111.el.r.appspot.com/GetPincodeDetails?cpin=' + alreaysetcookie;
  
      // Using fetch API
      fetch(pincodeUrl)
        .then(response => {
          if (!response.ok) {
  
            throw new Error(`HTTP error! Status: ${response.status}`);
          }
          return response.json();
        })
        .then(data => {
          var deliveryPinLocationSpan = document.getElementById('delivery_pin_location');
          var deliveryPinLocationMobSpan1 = document.getElementsByClassName('delivery_text');
          var deliveryPinLocationSpan2 = document.querySelectorAll('[data-role="delivery_pin_location"]');
          deliveryPinLocationSpan2.forEach((el, ind) => {
            el.textContent = `${data[0].cPin}, ${data[0].city}`;
          });
          deliveryPinLocationSpan.textContent = `${data[0].cPin}, ${data[0].city}`;
          Array.from(deliveryPinLocationMobSpan1).forEach(ele => {
            ele.textContent = `${data[0].cPin}, ${data[0].city}`;
          })
          // document.cookie = `cpin=${data[0].cPin}`
          document.cookie = `cpin=${data[0].cPin}; path=/`;
          localStorage.setItem('city', data[0].city);
        const pageLocation = window.location
        })
        .catch(error => {
          console.error('Fetch error:', error);
        });
    }
  }
  

  var body = document.body;
  function openModal() {
    var alFLow2 = localStorage.getItem('_showAgain');
    // Old functionality for opening modal
    //var overlay = document.getElementById('overlay');
    //New Urgent fix
    var overlay = document.getElementById('pin-code-modal');
    var gameWidget = document.getElementById('floating-game-custom-image');
    removeZindexAddToCart();

    if (gameWidget) {
      gameWidget.style.zIndex = 0;
    }
    overlay.style.display = 'flex';
    body.style.overflow = 'hidden';
    if (alFLow2 === 'false' && !customerID) {
      addEvents('stkyedd_LS_initiated');
    }
    if (addressCount > 0) {
      addEvents('stkyedd_address_screen');
    }
  }

  // Add the event listener to the document in capturing phase
  document.addEventListener(
    'click',
    function (event) {
      if (event.target === document.getElementById('overlay')) {
        event.stopPropagation();
        var shoppingModal = document.getElementById('overlay');
        shoppingModal.style.display = 'none';
        body.style.overflow = 'auto';
        addZindexAddToCart();
      }
    },
    true
  );

  function pinCodeModalCloseBack() {
    var pinCodeModal = document.getElementById('pin-code-modal');
    var addressModal = document.getElementById('overlay');
    pinCodeModal.style.display = 'none';
    addressModal.style.display = 'flex';

    var body = document.body;
    body.style.overflow = 'auto';
    removeZindexAddToCart();
  }

  function addressModalCloseBack() {
    var addModal = document.getElementById('addadress-code-modal');
    var overlay = document.getElementById('overlay');
    var body = document.body;

    addModal.style.display = 'none';
    overlay.style.display = 'flex';
    body.style.overflow = 'auto';
    clearInputFields();
    removeZindexAddToCart();
  }

  document.addEventListener(
    'click',
    function (event) {
      const current_target = document.getElementById('pin-code-modal');
      if (event.target === current_target) {
        event.stopPropagation();
        var shoppingModal = current_target;
        shoppingModal.style.display = 'none';
        addZindexAddToCart();
      }
      var addModal = document.getElementById('addadress-code-modal');
      if (event.target === addModal) {
        addModal.style.display = 'none';
        body.style.overflow = 'visible';
        clearInputFields();
        addZindexAddToCart();
      }

      var body = document.body;
      body.style.overflow = 'auto';
    },
    true
  );

  // function closeLoginModal(){
  //   var overlay = document.getElementById('overlay');
  //   overlay.style.display = 'none';
  //   body.style.overflow = 'visible';
  //   localStorage.setItem('_showLogin',false)
  //   localStorage.setItem('_showAgain', false)
  // }

  function closeModal() {
    var overlay = document.getElementById('overlay');
    overlay.style.display = 'none';
    body.style.overflow = 'visible';
    addZindexAddToCart();
  }
  function openPopupsecond() {
    var overlay = document.getElementById('pin-code-modal');
    if (customerID) {
      if (addressCount > 0) {
        addEvents('stkyedd_ret_user_pincode');
      } else {
        addEvents('stkyedd_new_user_pincode');
      }
    } else if (!customerID) {
      addEvents('stkyedd_LS_enter_pincode');
    }

    closeModal();
    overlay.style.display = 'flex';
    document.getElementById('pincodeInput').focus();
    body.style.overflow = 'hidden';
    removeZindexAddToCart();
  }
  function closeNewModal() {
    var overlay = document.getElementById('pin-code-modal');
    overlay.style.display = 'none';
    body.style.overflow = 'visible';
  }
  function closeAddressModal() {
    var overlay = document.getElementById('addadress-code-modal');
    overlay.style.display = 'none';
    body.style.overflow = 'visible';
    clearInputFields();
    removeZindexAddToCart();
  }

  var elements = document.getElementsByClassName('click_pincode');
  for (var i = 0; i < elements.length; i++) {
    elements[i].addEventListener('click', openPopupsecond);
  }

  function openPopupaddaddress() {
    const addresModal = document.getElementById('addadress-code-modal');
    if(addresModal){
      const titleElementAdd = addresModal.querySelector('.address_add p');
    }
    titleElementAdd.textContent = 'Add new address';
    var overlay = document.getElementById('addadress-code-modal');
    var body = document.body;
    const imgElement = document.createElement('img');
    imgElement.setAttribute(
      'src',
      'https://cdn.shopify.com/s/files/1/0565/8021/0861/files/left-arrow.svg?v=1699121219'
    ); // Set the image source
    imgElement.setAttribute('width', '100%');
    imgElement.setAttribute('height', '100%');
    imgElement.classList.add('img_hide');
    imgElement.addEventListener('click', addressModalCloseBack);
    titleElementAdd.prepend(imgElement);
    overlay.style.display = 'flex';
    body.style.overflow = 'hidden';
    // removeZindexAddToCart();
    if (addressCount > 0) {
      addEvents('stkyedd_ret_user_newaddress');
    } else addEvents('stkyedd_new_user_newaddress');
    closeModal();
  }

  var elements = document.getElementsByClassName('click_address');
  for (var i = 0; i < elements.length; i++) {
    elements[i].addEventListener('click', openPopupaddaddress);
  }

  function applyButtonClick() {
    const pincodeInput = document.getElementById('pincodeInput');
    var deliveryPinLocationSpan = document.querySelectorAll('[data-role="delivery_pin_location"]');
    const errorSpan = document.getElementById('errorSpan');
    const deliveryPinLocationMobSpan1 = document.getElementsByClassName('delivery_text');
    const pincodeUrl = 'https://promise-engine-371111.el.r.appspot.com/GetPincodeDetails?cpin=' + pincodeInput.value;

    // Using fetch API
    fetch(pincodeUrl)
      .then((response) => {
        if (!response.ok) {
          errorSpan.style.display = 'block';
          throw new Error(`HTTP error! Status: ${response.status}`);
        }
        return response.json();
      })
      .then((data) => {
        deliveryPinLocationSpan.forEach((el, ind) => {
          el.textContent = `${data[0].cPin}, ${data[0].city}`;
        });
        Array.from(deliveryPinLocationMobSpan1).forEach((ele) => {
          ele.textContent = `${data[0].cPin}, ${data[0].city}`;
        });
        // document.cookie = `cpin=${data[0].cPin}`
        document.cookie = `cpin=${data[0].cPin}; path=/`;
        localStorage.setItem('city', data[0].city);
        window.sessionStorage.setItem('enteredPinFlag',true);
        const pageLocation = window.location
        const pathname = window.location.pathname
        if(pathname.includes('/search')){
          window.location.reload();
        }
        if (pathname.includes('/collections')) {
          window.location.reload();
        }
        if (pathname.includes('/products')) {
          // function will be availabe on pdp page edd-section
          
        }
        if (pathname.includes('/cart')) {
          const cartpincodeText = document.getElementById('deliverypin');
          const checkButton = document.getElementById('changeeddbtn');
          cartpincodeText.value = data[0].cPin;
          checkButton.click();
        }
        addEvents('stkyedd_LS_pincode_checked');
        closeNewModal();
        addZindexAddToCart();
      })
      .catch((error) => {
        errorSpan.style.display = 'block';
        console.error('Fetch error:', error);
      });

    // Clear error message if there was any
    errorSpan.style.display = 'none';
  }
</script>

<script>
  function isPhoneNumberValid(evt) {
    evt = evt ? evt : window.event;
    var charCode = evt.which ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57)) {
      return false;
    }
    var userNumberElement = document.getElementById('field3');

    // Get the value of the input element
    var userNumberValue = userNumberElement.value;

    var numericValue = parseFloat(userNumberValue);
    if (isNaN(numericValue)) {
      if (charCode > 53 && charCode < 58) {
        return true;
      }
      return false;
    }
    return true;
  }
</script>

<script>
  document.addEventListener('DOMContentLoaded', function () {
    const innerBodies = document.querySelectorAll('.innerBody');
    const radioButtons = document.querySelectorAll('.radioBtn'); // Get all radio buttons

    // Set the first radio button as checked by default
    if(radioButtons.length > 0){
      radioButtons[0].checked = true;
    }

    innerBodies.forEach(function (innerBody, index) {
      innerBody.addEventListener('click', function () {
        // Remove the "active" class from all innerBody elements
        innerBodies.forEach(function (inner, i) {
          inner.classList.remove('active');
        });

        // Add the "active" class to the clicked innerBody
        this.classList.add('active');
        var targetValue = this.getAttribute('data-target');
        if (targetValue) {
          fetch('https://promise-engine-371111.el.r.appspot.com/GetPincodeDetails?cpin=' + targetValue)
            .then((response) => {
              if (!response.ok) {
                throw new Error(`HTTP error! Status: ${response.status}`);
              }
              return response.json();
            })
            .then(async (data) => {
              var deliveryPinLocationSpan = document.querySelectorAll('[data-role="delivery_pin_location"]');
              const deliveryPinLocationMobSpan1 = document.getElementsByClassName('delivery_text');
              const pathname = window.location.pathname;
              deliveryPinLocationSpan.forEach((el, ind) => {
                el.textContent = `${data[0].cPin}, ${data[0].city}`;
              });
              Array.from(deliveryPinLocationMobSpan1).forEach((ele) => {
                ele.textContent = `${data[0].cPin}, ${data[0].city}`;
              });
              document.cookie = `cpin=${data[0].cPin}; path=/`;
              const addressmodal = document.getElementById('overlay');
              addressmodal.style.display = 'none';
              body.style.overflow = 'auto';
              window.sessionStorage.setItem('enteredPinFlag', true);
              // var  addressId  = this.getAttribute("data-address-id");
              // var updateData = {
              //   c_id: ,
              //   a_id: addressId,
              // }
              // try {
              //   const updateResponse = await fetch(`https://supertails-backend.el.r.appspot.com/onecart/makeAddressDefault`, {
              //     method: 'POST',
              //     headers: {
              //       "Content-Type": "application/json",
              //     },
              //     body: JSON.stringify(updateData)
              //   })
              //   const responseOfUpdate = await updateResponse.json()

              // } catch (error) {
              //   console.log("Error in update", error)
              // }

              if(pathname.includes('/search')){
          window.location.reload();
        }
        if(pathname.includes('/collections')){
          window.location.reload();
        }
        if(pathname.includes('/products')){
          // function will be availabe on pdp page edd-section
          
        }
        if(pathname.includes('/cart')){
        const cartpincodeText = document.getElementById('deliverypin')
        const checkButton = document.getElementById('changeeddbtn')
        cartpincodeText.value = data[0].cPin
        checkButton.click()
        }
              addZindexAddToCart();
            })
            .catch((error) => {
              console.error('Fetch error:', error);
              const addressmodal = document.getElementById('overlay');
              addressmodal.style.display = 'none';
              body.style.overflow = 'auto';
            });
        } else {
          console.error('Unable to fetch target value!!', error);
          const addressmodal = document.getElementById('overlay');
          addressmodal.style.display = 'none';
          body.style.overflow = 'auto';
          addZindexAddToCart();
        }

        // Check the associated radio button
        radioButtons[index].checked = true;
      });
    });
  });
</script>

<script>
  function isNumber(evt) {
    evt = evt || window.event;
    var charCode = evt.which || evt.keyCode;
    var inputField = document.getElementById('pincodeInput');

    if ((charCode < 48 || charCode > 57) && (charCode < 65 || charCode > 90) && (charCode < 97 || charCode > 122)) {
      // Alphabetic character entered, add validation_error class
      inputField.classList.add('validation_error');
      return false;
    } else {
      // Numeric character entered, remove validation_error class
      if (evt.target.value.length === 6) {
        const pincodeUrl = 'https://promise-engine-371111.el.r.appspot.com/GetPincodeDetails?cpin=' + evt.target.value;

        // Using fetch API
        fetch(pincodeUrl)
          .then((response) => {
            if (!response.ok) {
              throw new Error(`HTTP error! Status: ${response.status}`);
            }

            return response.json();
          })
          .then((data) => {
            if (data[0]?.stateFullName) {
              const desktopField = document.getElementById('field10');
              desktopField.value = data[0]?.stateFullName;
              desktopField.classList.remove('validation_error');
              desktopField.classList.add('touched');
              const mobileField = document.getElementById('field1010');
              mobileField.value = data[0]?.stateFullName;
              mobileField.classList.remove('validation_error');
              mobileField.classList.add('touched');
            }
          })
          .catch((error) => {
            console.error('Fetch error:', error);
          });
      }
      inputField.classList.remove('validation_error');
      return true;
    }
  }
</script>

<script>
  
</script>

<script>
  function setupAutoPopulate(sourceFieldId, targetFieldId) {
    const sourceField = document.getElementById(sourceFieldId);
    const targetField = document.getElementById(targetFieldId);

    function updateTargetField() {
      targetField.value = sourceField.value;
    }

    function updateSourceField() {
      sourceField.value = targetField.value;
    }

    sourceField.addEventListener('input', updateTargetField);
    targetField.addEventListener('input', updateSourceField);
  }
</script>


<script>
  /// pincode modal
  document.addEventListener('click', (e) => {
    if (e.target.dataset.role == 'pin-code' || e.target.closest('[data-role="pin-code"]')) {
      let pinCodeBtn = document.getElementById('pin-code');
      pinCodeBtn.click();
    }
  });
</script>

