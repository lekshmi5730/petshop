              continue
            }
            handles.push(prod.handle)
          }

          return handles;
        }
        let dataArray = [];

        const apiUrl = 'https://supertails-backend.el.r.appspot.com/offers/v2/personalize';
        fetch(apiUrl, {
          method: "POST",
          headers: {
            "Content-Type": "application/json",
            "ngrok-skip-browser-warning": "any",
          },
          body: JSON.stringify(postBody),
        })
        .then(async response => {
          if (!response.ok) {
            throw new Error('Failed to fetch data from the API');
          }
          return await response.json();
        })
        .then(data => {
          dataArray.splice(0, dataArray.length);
          Array.prototype.push.apply(dataArray, data.coupons);
          updateDOM(dataArray);
          addTodo(dataArray);
        })
        .catch(error => {
          updateDOM(dataArray);
          addTodo(dataArray);
        });
      }
      let intervalName;
      function updateDOM(dataArray) {
        var offerTilesContainer = document.querySelector('.offer-tiles');
        offerTilesContainer.innerHTML = '';
        dataArray.forEach((item,index) => {
          let text = "";
          let textFinal = "";
          if (item.mov != 0) {
              text = ` | ABOVE <span style="font-weight: bold;">${item.mov}</span>`;
              textFinal = `USE ${item.couponCode} ${text}`;
          }
          if(item.couponCode === 'ICICI'){
            textFinal = `ON ICICI CREDIT CARDS`;
          }
          if(item.couponCode === 'HDFC'){
            textFinal = `ON HDFC CREDIT CARDS`;
          }
          if(item.couponCode === 'AXIS'){
            textFinal = `ON AXIS CREDIT CARDS`;
          }
          

          var offerTile = document.createElement('div');
          var offerTileProgress = document.createElement('div');
          offerTileProgress.classList.add('offer-tile-progress');
          offerTileProgress.innerHTML = 
          `
            <p class="offer-progress" id="offer-progress-${index}">${index === 0 ? `1/${dataArray.length}` : ""}</p>
            <div class="offer-progress-image" id="offer-progress-image-${index}">
              <svg width="22" height="6" viewBox="0 0 22 6" fill="none" xmlns="http://www.w3.org/2000/svg">
                <circle cx="3" cy="3" r="3" fill="black"/>
                <circle cx="11.5" cy="3" r="2.5" fill="#D9D9D9"/>
                <circle cx="19.5" cy="3" r="2.5" fill="#D9D9D9"/>
              </svg>
            </div>
          `
          offerTile.classList.add('offer-tile');
          offerTile.setAttribute('id', item.ID);
          offerTile.innerHTML = `
            <div class="offer-tile-details-container">
              <div class="offer-tile-image-container">
                <img class="offer-tile-image" src="${item.icon || "https://cdn.shopify.com/s/files/1/0565/8021/0861/files/MicrosoftTeams-image_5.png?v=1716879905"}"/>
              </div>
              <div class="offer-tile-details">
                <p class ="offer-title">${item.summaryDescription}</p>
                <p class="offer-description">${item.couponSubtitle}</p>
              </div>
            </div>`; 
          offerTilesContainer.appendChild(offerTile); 
          offerTilesContainer.appendChild(offerTileProgress);
        });
      }
      function isElementInViewport(el) {
          var rect = el.getBoundingClientRect();
          return (
              rect.top >= 0 &&
              rect.left >= 0 &&
              rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
              rect.right <= (window.innerWidth || document.documentElement.clientWidth)
          );
      }

      function addTodo(dataArray){ 
        var offerTiles = document.querySelectorAll('.offer-tile');
        const offerSection = document.getElementById('offer-tiles-id');
        if(window.innerWidth <= 768){
          window.addEventListener('scroll', function() {
            if (isElementInViewport(offerSection)) {
              offerContainer(offerTiles, indexVariableName, true)
            } else {
              offerContainer(offerTiles, indexVariableName, false)
            }
          })
        }

        offerTiles.forEach(function (offerTile) {
          offerTile.addEventListener('click', function (event) {
            if(window.innerWidth <= 768){
              const onOfferClick = (event, title, text , description, heading) =>{ 
                const termsObject = JSON.parse(text);
                const termsArray = Object.values(termsObject);
                const generateLiElements = (termsArray) => {
                  return termsArray.map(term => `<li>${term}</li>`).join('');
                };
                const liElements = generateLiElements(termsArray);
                const innerHTMLforterms = `
                  <ul>
                    ${liElements}
                  </ul>
                `;
                const tooltip_style =  document.getElementById('overlay-on-cp').style.display
                if(tooltip_style == 'flex'){
                  document.getElementById('overlay-on-cp').style.display = 'none';
                }else{
                  dataLayer.push({
                    "event": "clicked_offers",
                    "offer_type": offer_type,
                    "offer_position": `${indexVariableName+1}`,
                    "firebase_screen": "pdp",
                  })
                  if(window.mixpanel){
                    mixpanel.track('clicked_offers', {
                      "offer_type": offer_type,
                      "offer_position": `${indexVariableName+1}`,
                      "firebase_screen": "pdp",
                    });
                  }
                  document.getElementById('overlay-on-cp').style.display = 'flex';
                  document.getElementById("overlay-on-coupons-title-id-1").innerHTML = heading;
                  document.getElementById("overlay-on-coupons-text-id-1").innerHTML = description;
                  document.getElementById("overlay-on-coupons-title-id").innerHTML = title ;
                  document.getElementById("overlay-on-coupons-text-id").innerHTML =  innerHTMLforterms;
                }
                event.preventDefault()
              }
              var termsconditions = [];
              var description = '';
              var heading = '';
              dataArray.forEach(item => {
                var target = event.target.closest('.offer-tile');
                if (!target) return;
                const id_target = Number(target.getAttribute('id'));
                if(id_target === item.ID) {
                  heading = item.descriptionHeading;
                  description = item.descriptionBody;
                  offer_type = item.type;
                  const temp_termsconditions = item.termsAndConditions;
                  termsconditions = temp_termsconditions;
                }
              });

              onOfferClick(event,'Terms & Conditions',termsconditions, description, heading, offer_type);
              const hideTooltip = (event) =>{
                document.getElementById('overlay-on-cp').style.display = 'none';
                offerContainer(offerTiles, indexVariableName, true);
                event.preventDefault()
              }
        
              const hideTooltipOnOverlayClick = (event) =>{
                if (event.target.id === "overlay-on-cp") {
                  offerContainer(offerTiles, indexVariableName, true);
                  document.getElementById('overlay-on-cp').style.display = 'none';
                }
                event.preventDefault()
              }
              document.getElementById("overlay-on-coupons-close-btn-id").onclick = hideTooltip;
              document.getElementById("overlay-on-cp").onclick = hideTooltipOnOverlayClick;  
            }
            else {
              let lastEle = document.getElementById("tool-tip-text");
              if (!lastEle) {
                
                var tooltip = document.createElement('div');
                tooltip.setAttribute('id', 'tool-tip-text');
                tooltip.classList.add('tooltiptext');
                var termsconditions = [];
                dataArray.forEach((item, index) => {
                  var target = event.target.closest('.offer-tile');
                  if (!target) return;
                  const id_target = Number(target.getAttribute('id'));
                  if (id_target === item.ID) {
                    const temp_termsconditions = item.termsAndConditions;
                    offer_type = item.type;
                    dataLayer.push({
                    "event": "clicked_offers",
                    "offer_type": item.type,
                    "offer_position": index,
                    "firebase_screen": "pdp",
                    })
                    if(window.mixpanel){
                      mixpanel.track('clicked_offers', {
                        "offer_type": item.type,
                        "offer_position": index,
                        "firebase_screen": "pdp",
                      });
                    }
                    termsconditions = temp_termsconditions;
                  }
                });
                const generateLiElements = (termsArray) => {
                  termsArray1 = JSON.parse(termsArray);

                  return termsArray1.map(term => `<li>${term}</li>`).join('');
                };
                const liElements = generateLiElements(termsconditions);
            
                tooltip.innerHTML = `
                  <div class="tooltip-title">
                    <p>Terms & Conditions</p>
                  </div>
                  <div class="tooltip-content">
                    <ul>
                      ${liElements}
                    </ul>
                  </div>
                `;
            
                
            
                var rect = event.currentTarget.getBoundingClientRect();
                let contWidth = 332;
                let contheight = 184;
                let windowWidth = window.innerWidth;
                let windowHeight = window.innerHeight;
                let scrolledHeight = window.scrollY;
                tooltip.style.position = 'absolute';
                tooltip.style.top = (scrolledHeight + rect.bottom + 10) + 'px';
                if (rect.right > windowWidth) {
                  tooltip.style.right = '0px'
                  tooltip.classList.add('adjusted');
                } else if (rect.left < 469) {
                  tooltip.style.left = (windowWidth - (rect.left + 275)) + 'px'
                  tooltip.classList.add('adjusted2');
                } else {
                  tooltip.style.left = (rect.left - (contWidth - rect.width) / 2) + 'px';
                }
                tooltip.style.zIndex = '+1000';
                tooltip.style.visibility = "visible";
                let ele = document.getElementById("PageContainer")
                ele.appendChild(tooltip);
              }
            
              if (lastEle) {
                document.getElementById("PageContainer").removeChild(lastEle);
              }
            }
            
            var tooltip = document.getElementById("tool-tip-text");
    
            document.addEventListener('click', function (clickEvent) {
              var isClickInsideContainer = offerTile.contains(clickEvent.target);
              
              var isClickInsideTooltip = tooltip && tooltip.contains(clickEvent.target);
              if (!isClickInsideContainer && !isClickInsideTooltip) {
                // Clicked outside the container and tooltip, remove tooltip
                let lastEle = document.getElementById("tool-tip-text");
                if (lastEle) {
                  document.getElementById("PageContainer").removeChild(lastEle);
                }
                // Remove the event listener to prevent unnecessary removals
                document.removeEventListener('click', arguments.callee);
              }
            });  
          });
        });
        
        document.querySelector(".offer-tiles").addEventListener("scroll", function() {
          let lastEle = document.getElementById("tool-tip-text")
          if(this.scrollLeft>0){
            document.getElementById("PageContainer").removeChild(lastEle);
          }
        });
      }
      let indexVariableName = 0;
      function offerContainer(offersHolder, index, flag) {
          indexVariableName = index ?? 0;
          const tileWidth = 100;
          const gap = 10;
          const intervalTime = 2500;
          const totalTiles = offersHolder.length;
          if(flag == false){
            stopSlideShow();
          }
          else{
            startSlideShow();
          }
          function startSlideShow() {
            if (intervalName) {
              clearInterval(intervalName);
            }
            intervalName = setInterval(moveImages, intervalTime);
          }
          function stopSlideShow() {
            clearInterval(intervalName);
          }

          function moveImages() {
              if (indexVariableName >= totalTiles - 1) {
                  indexVariableName = -1;
              }
              indexVariableName++;
              updateSlide();
          }
          
          function updateSlide() {
              const fullWidth = (tileWidth + (gap / window.innerWidth * 100)) * indexVariableName;
              Array.from(offersHolder).forEach((offer, idx) => {
                  offer.style.transform = `translate3d(-${fullWidth}%, 0,0)`;
              });

              const progressDivs = document.querySelectorAll(".offer-progress");
              progressDivs.forEach(div => div.innerText = '');
              const progressDiv = document.getElementById("offer-progress-" + index);
              const progressDivsImages = document.querySelectorAll(".offer-progress-image");
              progressDivsImages.forEach(div => div.innerHTML = '');
              progressDivsImages.forEach(div => {
                switch (indexVariableName) {
                case 0:
                    div.innerHTML = `<svg width="22" height="6" viewBox="0 0 22 6" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <circle cx="3" cy="3" r="3" fill="black"/>
                      <circle cx="11.5" cy="3" r="2.5" fill="#D9D9D9"/>
                      <circle cx="19.5" cy="3" r="2.5" fill="#D9D9D9"/>
                    </svg>`;
                    break;
                case totalTiles - 1:
                    div.innerHTML = `
                    <svg width="22" height="6" viewBox="0 0 22 6" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <circle cx="2.5" cy="3" r="2.5" fill="#D9D9D9"/>
                      <circle cx="10.5" cy="3" r="2.5" fill="#D9D9D9"/>
                      <circle cx="19" cy="3" r="3" fill="black"/>
                    </svg>
                    `;
                    break;
                default:
                    div.innerHTML = `
                    <svg width="22" height="6" viewBox="0 0 22 6" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <circle cx="2.5" cy="3" r="2.5" fill="#D9D9D9"/>
                      <circle cx="11" cy="3" r="3" fill="black"/>
                      <circle cx="19.5" cy="3" r="2.5" fill="#D9D9D9"/>
                    </svg>`;
                    break;
              }
              })
  
              const progressDivImage = document.getElementById("offer-progress-image-" + indexVariableName);
              progressDiv.innerText = `${indexVariableName + 1}/${totalTiles}`;

              
          }
          Array.from(offersHolder).forEach(offer => {
              offer.addEventListener('click', () => {
                stopSlideShow();
              });
          });

          const offerContainerDiv = document.querySelector('.offer-container');
          let touchStartX = 0;
          let touchEndX = 0;

          if (!offerContainerDiv.dataset.listenersAdded) {
          offerContainerDiv.addEventListener('touchstart', event => {
              touchStartX = event.changedTouches[0].screenX;
          });

          offerContainerDiv.addEventListener('touchend', event => {
              touchEndX = event.changedTouches[0].screenX;
              handleGesture();
          });
          offerContainerDiv.dataset.listenersAdded = true;
        }

          function handleGesture() {
              stopSlideShow();
                if (touchEndX < touchStartX) {
                  indexVariableName = (indexVariableName + 1) % totalTiles;
              } else if (touchEndX > touchStartX) {
                  indexVariableName = (indexVariableName - 1 + totalTiles) % totalTiles;
              }
              updateSlide();
              startSlideShow();   
          }
      }
      getPersonalisedCoupons([skuIdArr]);
      // $('.variant__button-label').on('click', function() {
      //   var price = $(this).parents('.variant-input').find('input').data('productprice');
      //   var sku = $(this).parents('.variant-input').find('input').data('sku');
      //   getPersonalisedCoupons([sku])
      // });

      function getCpinCookie(cookieName) {
    const name = cookieName + "=";
    const decodedCookie = decodeURIComponent(document.cookie);
    const cookieArray = decodedCookie.split(';');

    for (let i = 0; i < cookieArray.length; i++) {
      let cookie = cookieArray[i].trim();
      if (cookie.indexOf(name) === 0) {
        return cookie.substring(name.length, cookie.length);
      }
    }

    return null;
  }
    </script>

