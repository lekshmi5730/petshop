        $('.close--popup').click(function(){
          $('body').removeClass('popup--iframe');
          $('.sizechart-popup-images').removeClass('open-popup');
        });
        $('.sizechart-popup-images').click(function(){
          var popup = $(".open-popup-chart, sizechart-popup-images");
          if(!popup.is(e.target) && popup.has(e.target).length === 0){
