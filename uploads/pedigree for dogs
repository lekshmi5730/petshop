  <script>
    function convertToCamelCaseWithSpace(input) {
      let words = input.split("-");
      let capitalizedWords = words.map(function(word) {
          return word.charAt(0).toUpperCase() + word.slice(1);
      });
      return capitalizedWords.join(" ");
    }
    function handleViewMoreClick(event) {
      event.preventDefault();
      let viewMoreUrl = event.target.getAttribute('href').split("=")[1];
      let term = convertToCamelCaseWithSpace(viewMoreUrl);
      window.dataLayer = window.dataLayer || [];
      dataLayer.push({
          "event": "view_more_click",
          "ecommerce": {
              "currency": "INR",
              "value": 353.0,
              "items": [
                  {
                      "item_id": "7503199142126",
                      "item_name": "Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food",
                      "coupon": "",
                      "discount": "",
                      "index": 0,
                      "item_brand": "Pedigree",
                      "item_category": "Dogs",
                      "item_category2": "Food",
                      "item_category3": "Dry Food",
                      "item_category4": "",
                      "item_category5": "",
                      "item_list_id": "",
                      "item_list_name": "",
                      "item_variant": "",
                      "location_id": "",
                      "price": 353.0,
                      "quantity": 1
                  }
              ]
          },
          "term": term 
      });
      window.location.href = event.target.getAttribute('href');
    }
  </script>

<script>
  $(window).on('load', function(){
  // $('.pdp-atc.add-to-cart').removeClass('disabled');
  //   $(".pdp-atc.add-to-cart").removeAttr("disabled");
  //   $(".pdp-atc.add-to-cart span").text('Add to cart');
  $('.product-single__meta .variant-input-wrap label').on('click',function(){
  setTimeout(function(){
  var titlt_list = '';
  
  $('form .grid__item .variant-input-wrap input[type=radio]:checked+label').each(function(index){
  if(index == 0 || index == 1 || index == 2){
  var tit = $(this).text().trim();
  if(index != 2){
   var count = ' / ';
  }else{
  var count = '';
  }
    if(index == 0){
      titlt_list +=  tit
    }else{
  titlt_list +=  tit + count;
    }
  }
  });
  $('.product-single__variants option').prop('selected','false');
  $('.product-single__variants option[data-vari="'+titlt_list+'"]').prop('selected','true');
  var data_p = $('.product-single__variants option[data-vari="'+titlt_list+'"]').attr('data-price');
  $('.product__price.on-sale span').text(data_p);
$(".pdp-atc.add-to-cart span").text('Add to cart');
$(".pdp-atc.add-to-cart").attr("disabled","disabled");
    $('.pdp-atc.add-to-cart').addClass('disabled');
  
  if($('#sesami-date-0').val() != ''){
    $(".pdp-atc.add-to-cart").removeAttr("disabled");
    $('.pdp-atc.add-to-cart').removeClass('disabled');
  }
    
  }, 50);
  })

$('.sesami__button').on('click', function(){
  setTimeout(function(){
if($('#sesami-date-0').val() != ''){
    $(".pdp-atc.add-to-cart").removeAttr("disabled");
    $('.pdp-atc.add-to-cart').removeClass('disabled');
