    // this is called when collection is loaded
     

    // add to cart event has been added in product-revamp-form.liquid because selected_variant and current_variant are unavailable here

    $('.cart__checkout').click(function() {
      let discountCode = localStorage.getItem("discountCode") || "";
      let eddInSession = JSON.parse(sessionStorage.getItem("cartEdd"))
      let discountApplyUrl = window.location.href + "/checkout?discount=" + discountCode + "&t=" + Date.now();
        fetch(discountApplyUrl)
        .then(console.log("Hitting checkout"))

      const items4 = []

      window.dataLayer = window.dataLayer || [];
      dataLayer.push({ ecommerce: null });  // Clear the previous ecommerce object.
      dataLayer.push({
        event: "begin_checkout",
        ecommerce: {
          currency: "INR",
          value: 0.0,
          coupon: discountCode,
          items: items4,
        }
      });
        clevertap.event.push("Checkout Started", {
        "cart_total": 0.0,
        "item_quantity": 0,
        "coupon_used": discountCode
      });
      const getEDD = (sku) => {
        let edd = '';
        let edd_ele = document.getElementById(`edd_${sku}`);
        let nodelist = edd_ele?.childNodes;
        nodelist?.forEach(node => {
          if (node.nodeType === Node.ELEMENT_NODE && node.tagName === 'INPUT') {
            edd = node.value; 
          }
        });
        if(edd) {
            var value = edd;
            return value;
        } else {
            return '';
        }
      }
      
      if(window.mixpanel){
        const checkoutData = {
          "Cart Value": 0.0,
          "Coupon Used": discountCode,
          "Item Count": 0,
          "City":localStorage.getItem('city'),
          "Device Type": localStorage.getItem('_deviceTypeIL'),
          "Items": []
        }
        mixpanel.track("Checkout Started",checkoutData )
      }
    });


    const input = document.querySelector('input[type="search"]');
      input.onsearch = () => {
        searchInitiated(input.value);
      };


    function searchInitiated(term){
      window.dataLayer = window.dataLayer || [];
      clevertap.event.push("Product Searched", {
        'keyword': term
      });
    }

    $('.variant__button-label').on('click', function(){
      var price = $(this).parents('.variant-input').find('input').data('productprice');
      var sku = $(this).parents('.variant-input').find('input').data('sku');

      window.dataLayer = window.dataLayer || [];
      dataLayer.push({
        'event': 'variant_changed',
        'product_name': 'Pedigree 100% Vegetarian Puppy and Adult Veg Dog Dry Food',
        'variant': $(this).attr('data-variant'),
        'variant_label': $(this).attr('data-variant-label'),
        'sku':  sku,
        'price':  price/100,
        'product_id':  '7503199142126'
      });


    });

    $('.slide-nav__wrapper a').click(function(){
      console.log($(this).attr('data-label'))
      window.dataLayer = window.dataLayer || [];
      dataLayer.push({
        'event': 'hamburger_menu_user',
        'hbm_initiation_page': window.location.href,
        'path_structue': $(this).attr('data-label')
      })
    })

  }, { once: true})
</script>

